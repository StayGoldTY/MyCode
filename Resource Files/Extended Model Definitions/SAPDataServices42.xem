<o:TemplateTargetItem Id="o24">
<a:ObjectID>547E15F7-9AA6-47A4-99BE-719D51DF1D01</a:ObjectID>
<a:Name>getDataTransformationTasks</a:Name>
<a:TemplateTargetItem.Value>.foreach_item(TransformationProcesses)
.if ((%Type% == &quot;SAP Data Services 4.2&quot;) &amp;&amp; (%IsShortcut% == FALSE))
.foreach_item(DataTransformationTasks)
.//do not export the DataTransformationTask which contains DataQueryTransform which was imported from unsupported DS Transform
.if(%notHasUnsupportedDSTransform%)
&lt;!-- Dataflow definition --&gt;
&lt;DIDataflow name=&quot;%Code%&quot; id=&quot;%ObjectID%&quot; typeId=&quot;1&quot; &gt;
&lt;DIUIOptions&gt;\n
.foreach_item(DataTransformationDiagrams)
.set_value(i, 0, new)
.foreach_item(Symbols)
.if %getObjectType% == &quot;NoteSymbol&quot;
&lt;!-- Note to Annotation definition --&gt;
&lt;DIAnnotation name=&quot;ANNOTATION_%i%&quot;&gt;\n
.set_value(i, %+(i,1)%)
&lt;DIAttribute name=&quot;ObjectDesc&quot; value=&quot;%Text%&quot;/&gt;
&lt;/DIAnnotation&gt;\n
.endif
.next
.next
&lt;/DIUIOptions&gt;
&lt;DITransforms&gt;

%createCaseTransforms%

.foreach_item(FlatFileInputs)
%getFlatFileInput%
.next

.foreach_item(FlatFileOutputs)
%getFlatFileOutput%
.next

.foreach_item(XMLInputs)
%getXMLInput%
.next

.foreach_item(XMLOutputs)
%getXMLOutput%
.next

.foreach_item(DatabaseInputs)
.//only map the DatabaseInput (which has only one table and does not modify the SQL) to DS TableSource.
.if (%SourceObjects.Count% == 1) &amp;&amp; (%SQLQuery% == &quot;&quot;) &amp;&amp; (%isUserModified% == False)
.//get the name of DIDatabaseTableSource&#39;s owner, if owner is null, choose the login value as owner name
.set_value(ownerName, %DataConnection.DataStore.UserName%, new)
.if %SourceObjects.First.Owner% != null
.set_value(ownerName, %SourceObjects.First.Owner.Name%, update)
.endif
&lt;!-- DatabaseInput to TableSource definition --&gt;
&lt;DIDatabaseTableSource typeId=&quot;11&quot; GUID=&quot;%ObjectID%&quot; datastoreName=&quot;%DataConnection.DataStore.SourceModels.First.Code%&quot; 
ownerName=&quot;%ownerName%&quot; tableName=&quot;%SourceObjects.First.Code%&quot; &gt;
&lt;DIUIOptions&gt;
&lt;DIAttribute name=&quot;ui_display_name&quot; value=&quot;%Code%&quot;/&gt;
&lt;/DIUIOptions&gt;
&lt;DIOutputView name=&quot;%Code%&quot;  /&gt;
&lt;DIAttributes&gt;
&lt;DIAttribute name=&quot;name&quot; value=&quot;%DataConnection.DataStore.SourceModels.First.Code%&quot;/&gt;
&lt;DIAttribute name=&quot;reader_template_table&quot; value=&quot;no&quot;/&gt;
&lt;/DIAttributes&gt;
&lt;/DIDatabaseTableSource&gt;\n
.else
&lt;!-- DatabaseInput to SQL transform definition --&gt;
&lt;DITransformCall typeId=&quot;23&quot; name=&quot;SQL&quot; GUID=&quot;&quot; &gt;
&lt;DIUIOptions&gt;
&lt;DIAttribute name=&quot;ObjectDesc&quot; value=&quot;%Comment%&quot;/&gt;
&lt;DIAttribute name=&quot;ui_display_name&quot; value=&quot;%Code%&quot;/&gt;
&lt;/DIUIOptions&gt;
&lt;DISchema name=&quot;%Code%&quot; &gt;\n
.//.set_value(GV_s,%DataConnection.DataStore.DataSourceName%)
.//.set_value(GV_dbtype, %DataConnection.DataStore.getDBType%)
.foreach_item(Columns)
&lt;DIElement name=&quot;%Code%&quot; %getInternalDataTypeMap% 
&lt;/DIElement&gt;\n
.next
&lt;/DISchema&gt;
&lt;DIAttributes&gt;
&lt;DIAttribute name=&quot;sql_transform_configuration_enabled&quot; value=&quot;yes&quot;/&gt;
.if %DataConnection% == null
.log %Code% does not have Data Connection, please correct.
.else
&lt;DIAttribute name=&quot;database_datastore&quot; value=&quot;%DataConnection.DataStore.SourceModels.First.Code%&quot;/&gt;
.endif
&lt;DIAttribute name=&quot;sql_transform_configurations&quot; hasNestedXMLTree =&quot;true&quot;&gt;
&lt;SQLTexts&gt;
&lt;SQLText %DataConnection.DataStore.getDBTV%
&lt;sql_text&gt;
%getSQLQuery%
&lt;/sql_text&gt;
&lt;/SQLText&gt;
&lt;/SQLTexts&gt;
&lt;/DIAttribute&gt;
&lt;/DIAttributes&gt;
&lt;/DITransformCall&gt;\n
.endif
.next


.foreach_item(DataTransformationActions)
.if (%getObjectType% == &quot;DataAggregation&quot;) || (%getObjectType% == &quot;DataProjection&quot;) || (%getObjectType% == &quot;DataSort&quot;) || (%getObjectType% == &quot;DataFilter&quot;) || (%getObjectType% == &quot;DataSplit&quot;) || (%getObjectType% == &quot;DataJoin&quot;) || (%getObjectType% == &quot;DataQueryTransform&quot;) || (%getObjectType% == &quot;ScriptExecution&quot;) || (%getObjectType% == &quot;DataQueryExecution&quot;) || (%getObjectType% == &quot;DataCaculator&quot;)  
&lt;!-- %getObjectType% to Query transform definition --&gt;
&lt;DIQuery typeId=&quot;28&quot; GUID=&quot;&quot;  &gt;
&lt;DIUIOptions&gt;\n
.set_value(i, -1, new)
.foreach_item(IncomingDataFlows)
.set_value(i, %+(i,1)%)
&lt;DIAttribute name=&quot;ui_acta_from_schema_%i%&quot; value=&quot;%getSourceObject%&quot;/&gt;\n
.next
&lt;DIAttribute name=&quot;ui_display_name&quot; value=&quot;%Code%&quot;/&gt;
&lt;/DIUIOptions&gt;
&lt;DISchema name=&quot;%Code%&quot; &gt;\n
.foreach_item(Columns)
&lt;DIElement name=&quot;%Code%&quot; %getInternalDataTypeMap% 
&lt;/DIElement&gt;\n
.next
&lt;/DISchema&gt;
&lt;DISelect&gt;
&lt;DIProjection&gt;\n
.foreach_item(Columns)
&lt;DIExpression isString=&quot;true&quot; expr=&quot;%getSourceExpression%&quot; &gt;
&lt;/DIExpression&gt;\n
.next
&lt;/DIProjection&gt;
&lt;DIFrom&gt;

.if (%getObjectType% == &quot;DataJoin&quot;) || (%getObjectType% == &quot;DataQueryTransform&quot;)
.if %Joins.Count% &gt; 0
&lt;DIJoinedTableSpec jointype=&quot;inner join&quot;&gt;\n
.foreach_item(IncomingDataFlows)
&lt;DITableSpec name=&quot;%getSourceObject%&quot; /&gt;\n
.next
&lt;DIOn&gt;\n
.//DS only has one join expression, so use &#39;and&#39; to join PD joins
.set_value(i, -1, new)
.set_value(GV_s, &quot;&quot;)
.set_value(temp1, &quot;&quot;, new)
.set_value(temp2, &quot;&quot;, new)
.foreach_item(Joins)
.set_value(i, %+(i,1)%)
.foreach_item(Outer.IncomingDataFlows)
.if %SourceObject.Code% == %Outer.Column1.Parent.Code%
.set_value(temp1, %getSourceObject%)
.elsif %SourceObject.Code% == %Outer.Column2.Parent.Code%
.set_value(temp2, %getSourceObject%)
.endif
.next
.if %i% == 0
.set_value(GV_s,(%temp1%.%Column1.Code%%JoinExpression%%temp2%.%Column2.Code%))
.else
.set_value(GV_s,%GV_s% and (%temp1%.%Column1.Code%%JoinExpression%%temp2%.%Column2.Code%))
.endif
.next
&lt;DIExpression isString=&quot;true&quot; expr=&quot;%getJoinOpt%&quot; &gt;
&lt;/DIExpression&gt;
&lt;/DIOn&gt;
&lt;DIUIOptions&gt;
&lt;DIAttribute name=&quot;ui_on_text&quot; value=&quot;%getJoinOpt%&quot;/&gt;
&lt;/DIUIOptions&gt;
&lt;/DIJoinedTableSpec&gt;\n
.elsif
.foreach_item(IncomingDataFlows)
&lt;DITableSpec name=&quot;%getSourceObject%&quot; /&gt;\n
.next
.endif
.elsif
.foreach_item(IncomingDataFlows)
&lt;DITableSpec name=&quot;%getSourceObject%&quot; /&gt;\n
.next
.endif
&lt;/DIFrom&gt;

.if (%getObjectType% == &quot;DataSort&quot;) || (%getObjectType% == &quot;DataQueryTransform&quot;)
.if %SortedColumns.Count% &gt; 0
&lt;DIOrderBys&gt;\n
.foreach_item(SortedColumns)
.set_value(GV_s, %Order%)
&lt;DIOrderBy direction=&quot;%getOrder%&quot; &gt;\n
.set_value(GV_s,%Column.getSourceExpression%)
&lt;DIExpression isString=&quot;true&quot; expr=&quot;%getSourceObj%.%getSourceObjColumn%&quot; &gt;
&lt;COLUMN_REFERENCE  qualifier1=&quot;%getSourceObj%&quot; column=&quot;%getSourceObjColumn%&quot; /&gt;
&lt;/DIExpression&gt;
&lt;/DIOrderBy&gt;\n
.next
&lt;/DIOrderBys&gt;\n
.endif
.endif

.if (%getObjectType% == &quot;DataAggregation&quot;) || (%getObjectType% == &quot;DataQueryTransform&quot;)
.if %AggregationColumns.Count% &gt; 0
&lt;DIGroupBy&gt;\n
.foreach_item(AggregationColumns)
.set_value(temp1, &quot;&quot;, new)
.foreach_item(Outer.IncomingDataFlows)
.if %SourceObject.Code% == %Outer.Parent.Code%
.set_value(temp1, %getSourceObject%)
.break
.endif
.next
&lt;DIExpression isString=&quot;true&quot; expr=&quot;%temp1%.%Code%&quot; &gt;
&lt;COLUMN_REFERENCE  qualifier1=&quot;%temp1%&quot; column=&quot;%Code%&quot; /&gt;
&lt;/DIExpression&gt;\n
.next
&lt;/DIGroupBy&gt;\n
.endif
.endif

.if ((%getObjectType% == &quot;DataFilter&quot;) || (%getObjectType% == &quot;DataQueryTransform&quot;)) &amp;&amp; (%Criteria% != &quot;&quot;)
&lt;DIWhere&gt;
&lt;DIExpression isString=&quot;true&quot; expr=&quot;%getCriteria%&quot; &gt;
&lt;/DIExpression&gt;
&lt;/DIWhere&gt;\n
.endif
&lt;/DISelect&gt;
&lt;/DIQuery&gt;\n
.endif
.next

.foreach_item(DataMerges)
&lt;!-- DataMerge to Merge transform definition --&gt;
&lt;DITransformCall typeId=&quot;23&quot; name=&quot;Merge&quot; GUID=&quot;&quot; &gt;\n
.foreach_item(IncomingDataFlows)
&lt;DIInputView name=&quot;%getSourceObject%&quot;  /&gt;\n
.next
&lt;DISchema name=&quot;%Code%&quot; &gt;
&lt;/DISchema&gt;
&lt;/DITransformCall&gt;\n
.next

.foreach_item(DataLookups)
%getLookup%
.next


.foreach_item(DatabaseOutputs)
.//1. DS table as target can only has one input flow, so need add an additional DS Query if PD side has more than one input
.//2. For only one input flow, because DS Table as Target cannot adjust the mapping, but PD can,
.//2.1 so we need check if the target mapping is between different names, if yes, also need additional DS Query.
.//2.2 so we need check if the source mapping is restrict one-one mapping.
.set_value(temp3, &quot;&quot;, new)
.if %IncomingDataFlows.Count% == 1
   .if %Columns.Count% != %TargetObject.Columns.Count%
      .set_value(temp3, &quot;Need&quot;)
   .else
      .foreach_item(Columns)
         .if %TargetObject% != null
            .if %.L:TargetObject.Code% != %.L:Code%
               .set_value(temp3, &quot;Need&quot;)
               .break
            .endif
         .endif
      .next
   .endif
   .if %temp3% == &quot;&quot;
      .if %Columns.Count% != %IncomingDataFlows.First.SourceObject.Columns.Count%
         .set_value(temp3, &quot;Need&quot;)
      .else
         .foreach_item(Columns)
            .if %getSourceExpresionRaw% != %Parent.IncomingDataFlows.First.SourceObject.Code%.%Code%
               .set_value(temp3, &quot;Need&quot;)
               .break
            .endif
         .next
      .endif
   .endif
.endif
.if (%IncomingDataFlows.Count% &gt; 1) || (%temp3% != &quot;&quot;)
.set_value(GV_InputOfTableTarget,QueryFor%Code%)
&lt;!-- Additional DS Query for DatabaseOutput --&gt;
&lt;DIQuery typeId=&quot;28&quot; GUID=&quot;&quot;  &gt;
&lt;DIUIOptions&gt;\n
.set_value(i, -1, new)
.foreach_item(IncomingDataFlows)
.set_value(i, %+(i,1)%)
&lt;DIAttribute name=&quot;ui_acta_from_schema_%i%&quot; value=&quot;%getSourceObject%&quot;/&gt;\n
.next
&lt;DIAttribute name=&quot;ui_display_name&quot; value=&quot;%GV_InputOfTableTarget%&quot;/&gt;
&lt;/DIUIOptions&gt;
&lt;DISchema name=&quot;%GV_InputOfTableTarget%&quot; &gt;\n
.foreach_item(Columns)
.if %TargetObject% != null
&lt;DIElement name=&quot;%TargetObject.Code%&quot; %getInternalDataTypeMap% 
&lt;/DIElement&gt;\n
.endif
.next
&lt;/DISchema&gt;
&lt;DISelect&gt;
&lt;DIProjection&gt;\n
.foreach_item(Columns)
.if %TargetObject% != null
&lt;DIExpression isString=&quot;true&quot; expr=&quot;%getSourceExpression%&quot; &gt;
&lt;/DIExpression&gt;\n
.endif
.next
&lt;/DIProjection&gt;
&lt;DIFrom&gt;\n
.foreach_item(IncomingDataFlows)
&lt;DITableSpec name=&quot;%getSourceObject%&quot; /&gt;\n
