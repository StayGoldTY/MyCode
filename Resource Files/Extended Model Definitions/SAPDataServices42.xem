   Dim sql, sql1, find
   Dim select1, from, where, orderby
   Const cSelect = &quot; select &quot;, cAs = &quot; as &quot;, cFrom = &quot; from &quot;, cWhere = &quot; where &quot;
   Const cGroupby = &quot; group by &quot;, cOrderby = &quot; order by &quot;
   sql = ScriptInputArray(0)
   
   sql = &quot; &quot; + Replace(sql, Chr(13) &amp; Chr(10), &quot; &quot;)
   sql1 = sql
   sql = LCase(sql)
   &#39;Msgbox(sql)
   
   select1 = Mid(sql, InStr(sql, cSelect) + Len(cSelect), InStr(sql, cFrom) - (InStr(sql, cSelect) + Len(cSelect)))
   select1 = Replace(select1, &quot; distinct &quot;, &quot; &quot;)
   &#39;Msgbox(select1)
