Option Explicit &#39; This is to ensure all used variables are defined

Dim select1, from, where, orderby, column1, column2, tablename, tableObj
Const cSelect = &quot; select &quot;, cAs = &quot; as &quot;, cFrom = &quot; from &quot;, cWhere = &quot; where &quot;, cAnd = &quot; and &quot;
Const cGroupby = &quot; group by &quot;, cOrderby = &quot; order by &quot;
Const o1 = &quot;&lt;=&quot;, o2 = &quot;&lt;&gt;&quot;, o3 = &quot;&gt;=&quot;, o4 = &quot;!=&quot;, o8 = &quot;&gt;&quot;, o9 = &quot;=&quot;, o10 = &quot;~&quot;, o5 = &quot; is not &quot;, o6 = &quot;&lt;&quot;, o7 = &quot; is &quot;

Function SplitSql(sql)
   sql = &quot; &quot; + LCase(Replace(sql, Chr(13) &amp; Chr(10), &quot; &quot;))
   sql = Replace(sql, Chr(13), &quot; &quot;)
   sql = Replace(sql, Chr(10), &quot; &quot;)
   &#39;Output(sql)
    
   SplitSql = True
   Dim validLenth
   validLenth = InStr(sql, cFrom) - (InStr(sql, cSelect) + Len(cSelect))
   If validLenth &lt;0 Then
       SplitSql = False
       Exit Function
   End If
   
   select1 = Mid(sql, InStr(sql, cSelect) + Len(cSelect), InStr(sql, cFrom) - (InStr(sql, cSelect) + Len(cSelect)))
   select1 = Replace(select1, &quot; distinct &quot;, &quot; &quot;)
   &#39;Msgbox(select1)
   &#39;Get lookup table name
   If InStr(select1, cAs) &gt; 0 Then
      select1 = Trim(Mid(select1, 1, InStr(select1, cAs)))
      &#39;Msgbox(select1)
   Else
      select1 = Trim(select1)
   End If
   
   validLenth = InStr(sql, cWhere) - (InStr(sql, cFrom) + Len(cFrom))
   If validLenth &lt;0 Then
       SplitSql = False
       Exit Function
   End If
   
   from = Mid(sql, InStr(sql, cFrom) + Len(cFrom), InStr(sql, cWhere) - (InStr(sql, cFrom) + Len(cFrom)))
   &#39;Msgbox(from)
   
   If InStr(sql, cOrderby) &gt; 0 Then      
      validLenth = InStr(sql, cOrderby) - (InStr(sql, cWhere) + Len(cWhere))
      If validLenth &lt;0 Then
          SplitSql = False
          Exit Function
      End If
      where = Mid(sql, InStr(sql, cWhere) + Len(cWhere), InStr(sql, cOrderby) - (InStr(sql, cWhere) + Len(cWhere)))
      &#39;Msgbox(where)
   Else      
      validLenth = Len(sql) - (InStr(sql, cWhere) + Len(cWhere)) + 1
      If validLenth &lt;0 Then
          SplitSql = False
          Exit Function
      End If
      where = Mid(sql, InStr(sql, cWhere) + Len(cWhere), Len(sql) - (InStr(sql, cWhere) + Len(cWhere)) + 1)
   End If
   
   validLenth = Len(sql) - (InStr(sql, cOrderby) + Len(cOrderby)) + 1
   If validLenth &lt;0 Then
       SplitSql = False
       Exit Function
   End If
   orderby = Mid(sql, InStr(sql, cOrderby) + Len(cOrderby), Len(sql) - (InStr(sql, cOrderby) + Len(cOrderby)) + 1)
   &#39;Msgbox(orderby)
   &#39;If InStr(orderby, cGroupby) &gt; 0 Then
      &#39;orderby = Mid(sql, InStr(sql, cOrderby) + Len(cOrderby), InStr(sql, cGroupby) - (InStr(sql, cOrderby) + Len(cOrderby)))
   &#39;End If
   &#39;Msgbox(orderby)
   
End Function

Function GetColumn1(sub_where)
   Dim sub_where1
   
   sub_where = Trim(sub_where)
   sub_where1 = LCase(sub_where)
   
   &#39;Find out column which on the left hand of operator in sub_where clause
   If InStr(sub_where1, o1) &gt; 0 Then
      column1 = Mid(sub_where, 1, InStr(sub_where1, o1) - 1)
   ElseIf InStr(sub_where1, o2) &gt; 0 Then
      column1 = Mid(sub_where, 1, InStr(sub_where1, o2) - 1)
   ElseIf InStr(sub_where1, o3) &gt; 0 Then
      column1 = Mid(sub_where, 1, InStr(sub_where1, o3) - 1)
   ElseIf InStr(sub_where1, o4) &gt; 0 Then
      column1 = Mid(sub_where, 1, InStr(sub_where1, o4) - 1)
   ElseIf InStr(sub_where1, o8) &gt; 0 Then
      column1 = Mid(sub_where, 1, InStr(sub_where1, o8) - 1)
   ElseIf InStr(sub_where1, o9) &gt; 0 Then
      column1 = Mid(sub_where, 1, InStr(sub_where1, o9) - 1)
   ElseIf InStr(sub_where1, o10) &gt; 0 Then
      column1 = Mid(sub_where, 1, InStr(sub_where1, o10) - 1)
   ElseIf InStr(sub_where1, o5) &gt; 0 Then
      column1 = Mid(sub_where, 1, InStr(sub_where1, o5) - 1)
   ElseIf InStr(sub_where1, o6) &gt; 0 Then
      column1 = Mid(sub_where, 1, InStr(sub_where1, o6) - 1)
   ElseIf InStr(sub_where1, o7) &gt; 0 Then
      column1 = Mid(sub_where, 1, InStr(sub_where1, o7) - 1)
   Else
      column1 = &quot;Unexpected_SQL_operator&quot;
   End If
   column1 = Trim(column1)
End Function

Function GetColumn2(sub_where)
   Dim sub_where1
   
   sub_where = Trim(sub_where)
   sub_where1 = LCase(sub_where)
   
   &#39;Find out column which on the right hand of operator in sub_where clause
   If InStr(sub_where1, o1) &gt; 0 Then
      column2 = Mid(sub_where, InStr(sub_where1, o1) + Len(o1), Len(sub_where1) - (InStr(sub_where1, o1) + Len(o1)) + 1)
   ElseIf InStr(sub_where1, o2) &gt; 0 Then
      column2 = Mid(sub_where, InStr(sub_where1, o2) + Len(o2), Len(sub_where1) - (InStr(sub_where1, o2) + Len(o2)) + 1)
   ElseIf InStr(sub_where1, o3) &gt; 0 Then
      column2 = Mid(sub_where, InStr(sub_where1, o3) + Len(o3), Len(sub_where1) - (InStr(sub_where1, o3) + Len(o3)) + 1)
   ElseIf InStr(sub_where1, o4) &gt; 0 Then
      column2 = Mid(sub_where, InStr(sub_where1, o4) + Len(o4), Len(sub_where1) - (InStr(sub_where1, o4) + Len(o4)) + 1)
   ElseIf InStr(sub_where1, o8) &gt; 0 Then
      column2 = Mid(sub_where, InStr(sub_where1, o8) + Len(o8), Len(sub_where1) - (InStr(sub_where1, o8) + Len(o8)) + 1)
   ElseIf InStr(sub_where1, o9) &gt; 0 Then
      column2 = Mid(sub_where, InStr(sub_where1, o9) + Len(o9), Len(sub_where1) - (InStr(sub_where1, o9) + Len(o9)) + 1)
   ElseIf InStr(sub_where1, o10) &gt; 0 Then
      column2 = Mid(sub_where, InStr(sub_where1, o10) + Len(o10), Len(sub_where1) - (InStr(sub_where1, o10) + Len(o10)) + 1)
   ElseIf InStr(sub_where1, o5) &gt; 0 Then
      column2 = Mid(sub_where, InStr(sub_where1, o5) + Len(o5), Len(sub_where1) - (InStr(sub_where1, o5) + Len(o5)) + 1)
   ElseIf InStr(sub_where1, o6) &gt; 0 Then
      column2 = Mid(sub_where, InStr(sub_where1, o6) + Len(o6), Len(sub_where1) - (InStr(sub_where1, o6) + Len(o6)) + 1)
   ElseIf InStr(sub_where1, o7) &gt; 0 Then
      column2 = Mid(sub_where, InStr(sub_where1, o7) + Len(o7), Len(sub_where1) - (InStr(sub_where1, o7) + Len(o7)) + 1)
   Else
      column2 = &quot;Unexpected_SQL_operator&quot;
   End If
   column2 = Trim(column2)
End Function

Function GetColumnName(longname)
   Dim pos
   &#39;like RUILIU.GRADE_FIRST_CLASS.ID return ID
   pos = 1
   While InStr(pos, longname, &quot;.&quot;) &gt; 0      
      pos = InStr(pos, longname, &quot;.&quot;)
      pos = pos + Len(&quot;.&quot;)
   Wend
   GetColumnName = Trim(Right(longname, Len(longname) - pos + 1))
End Function

Function GetTable(sql)
   Dim sql1, find, pos
   Dim select1, from, where, orderby
   
   sql = &quot; &quot; + Replace(sql, Chr(13) &amp; Chr(10), &quot; &quot;)
   sql = Replace(sql, Chr(13), &quot; &quot;)
   sql = Replace(sql, Chr(10), &quot; &quot;)
   sql1 = sql
   sql = LCase(sql)
   &#39;Msgbox(sql)
   
   from = Mid(sql, InStr(sql, cFrom) + Len(cFrom), InStr(sql, cWhere) - (InStr(sql, cFrom) + Len(cFrom)))
   &#39;Msgbox(from)
   
   &#39;get real value
   find = trim(from)
   &#39;from = Mid(sql1, InStr(sql, find), Len(find))
   
   tablename = from
   
   &#39;only return the table name with out other info
   &#39;like RUILIU.GRADE_FIRST_CLASS return GRADE_FIRST_CLASS
   pos = 1
   While InStr(pos, tablename, &quot;.&quot;) &gt; 0      
      pos = InStr(pos, tablename, &quot;.&quot;)
      pos = pos + Len(&quot;.&quot;)
   Wend
   tablename = Trim(Right(tablename, Len(tablename) - pos + 1))
End Function

Function FindTableInPDMRecursive(m1)
   Dim t, p
   FindTable(m1)
   
   If Not(tableObj is Nothing) Then            
      Exit Function
   End If
   
   For Each p in m1.Packages
      FindTableInPDMRecursive(p)      
   Next
End Function

Function FindTable(m1)
   Dim t
   For Each t in m1.Tables
      &#39;output(t.Code)
      If LCase(t.Code) = Trim(tablename) Then
         set tableObj = t
         Exit For
      Else
         set tableObj = Nothing
      End If
   Next
End Function


Function GetList(ListName)    
    Const con_mapFolderPath = &quot;Settings\DataServicesMap&quot;   
    Const con_extenName = &quot;SAP Data Services 4.2&quot; 
    Dim matchObj 
    Dim extObj, wsObj  
    
    For Each extObj In ActiveModel.ExtendedModelDefinitions
        If extObj.Name = con_extenName Then
            If Not extObj.IsShortcut Then
                Set matchObj = extObj.FindItemByPath(con_mapFolderPath, ListName, false)
            Else
                Set matchObj = extObj.TargetObject.FindItemByPath(con_mapFolderPath, ListName, false)
            End If
            Exit For
        End If
    Next
    
    If Not (matchObj Is Nothing) Then
       GetList = matchObj.List    
    End If     
End Function

Function GetObjectType(obj)
    If obj.ObjectType = &quot;Shortcut&quot; Then
        GetObjectType = obj.TargetObject.ObjectType
    Else
        GetObjectType = obj.ObjectType
    End If
End Function
