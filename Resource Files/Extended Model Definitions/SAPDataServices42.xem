<a:ObjectID>246CC895-EFAB-4677-92E6-DB89F0CE047B</a:ObjectID>
<a:Name>Invalid data structure columns</a:Name>
<a:CreationDate>1411955445</a:CreationDate>
<a:Creator>I309440</a:Creator>
<a:ModificationDate>1420717832</a:ModificationDate>
<a:Modifier>I063945</a:Modifier>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:HelpMessage>Data query transforms that specify an aggregation must specify data structure columns from among their aggregation columns.</a:HelpMessage>
<a:CheckScript>Function %Check%(obj)
   %Check% = True
   If obj.Parent.Parent.Type &lt;&gt; &quot;SAP Data Services 4.2&quot; Then
      Exit Function
   End If
   
   If obj.AggregationColumns.Count &gt; 0 Then
      Dim i,j
      Dim dscDic, aggDic
      Dim tempDSCol, tempAggCol
      Dim aggKeys, dscKeys, aggTempkey, dscTempKey
      Dim dscSourExpression,aggSourExpression    
      Set dscDic = CreateObject(&quot;Scripting.Dictionary&quot;)   
      Set aggDic = CreateObject(&quot;Scripting.Dictionary&quot;)
      For Each tempDSCol In obj.Columns       
         If tempDSCol.SourceExpression = &quot;&quot; Then
            dscSourExpression = tempDSCol.GetDefaultSourceExpression
         Else
            dscSourExpression = tempDSCol.SourceExpression
         End If     
         If dscSourExpression &lt;&gt; &quot;&quot; Then        
            If Not dscDic.exists(dscSourExpression) Then
               dscDic.Add dscSourExpression, &quot;&quot;
            End IF        
         End If
      Next
      For Each tempAggCol In obj.AggregationColumns
         If Not(tempAggCol.Parent Is Nothing) Then
            aggSourExpression = tempAggCol.Parent.Code &amp; &quot;.&quot; &amp; tempAggCol.Code       
            If aggSourExpression &lt;&gt; &quot;&quot; Then
              If Not aggDic.exists(aggSourExpression) Then
                   aggDic.Add aggSourExpression, &quot;&quot;
              End IF         
            End If
         End If                
      Next  
      aggKeys = aggDic.keys
      dscKeys = dscDic.keys
      For i = 0 To dscDic.Count -1
          dscTempKey = dscKeys(i)
          If Not aggDic.exists(dscTempKey) Then
              %Check% = False
              For j = 0 To aggDic.Count - 1
                  aggTempkey = aggKeys(j)
                  If InStr(dscTempKey, aggTempkey) Then
                     %Check% = True
                     Exit For
                  End If
              Next           
          End If
          If %Check% = False Then
              Exit For
          End If
      Next  
      If Not(dscDic Is Nothing) Then
          Set dscDic = Nothing
      End If 
      If Not(aggDic Is Nothing) Then
          Set aggDic = Nothing
      End If
   End If  
End Function



</a:CheckScript>
