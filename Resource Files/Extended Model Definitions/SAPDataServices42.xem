<a:Name>procWhere</a:Name>
<a:TemplateTargetItem.Value>.vbscript(%SQLQuery%)
   Dim sql, sql1, find, pos
   Dim select1, from, where, where1, orderby
   Const cSelect = &quot; select &quot;, cAs = &quot; as &quot;, cFrom = &quot; from &quot;, cWhere = &quot; where &quot;, cAnd = &quot; and &quot;
   Const cGroupby = &quot; group by &quot;, cOrderby = &quot; order by &quot;
   sql = ScriptInputArray(0)
   
   sql = &quot; &quot; + Replace(sql, Chr(13) &amp; Chr(10), &quot; &quot;)
   sql1 = sql
   sql = LCase(sql)
   &#39;Msgbox(sql)
   
   If InStr(sql, cOrderby) &gt; 0 Then
      where = Mid(sql, InStr(sql, cWhere) + Len(cWhere), InStr(sql, cOrderby) - (InStr(sql, cWhere) + Len(cWhere)))
      &#39;Msgbox(where)
   Else
      where = Mid(sql, InStr(sql, cWhere) + Len(cWhere), Len(sql) - (InStr(sql, cWhere) + Len(cWhere)) + 1)
   End If
   
   &#39;get real value
   find = trim(where)
   where = Mid(sql1, InStr(sql, find), Len(find))
   
   &#39;change all types of &#39;and&#39; to &#39;;&#39; so that GTL can foreach_part
   where1 = where
   where = LCase(where)
   &#39;Msgbox(where)
   pos = 1
   While InStr(pos, where, cAnd) &gt; 0
      pos = InStr(pos, where, cAnd)
      where1 = Left(where1, pos) + cAnd + Right(where1, Len(where) - (pos + Len(cAnd)) + 1)
      pos = pos + Len(cAnd)
      &#39;Msgbox(where1)
   Wend
   ScriptResult = Replace(where1, cAnd, &quot; ; &quot;)
.endvbscript</a:TemplateTargetItem.Value>
