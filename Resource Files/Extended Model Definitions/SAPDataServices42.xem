<a:TemplateTargetItem.Value>.vbscript(%transformType%, %matchValue%)    
   Dim infoArray
   str = ScriptInputArray(0)   
   resultstr = ScriptInputArray(1)   
   Dim i,j
   i = 0
   j = 0
   infoArray = Split(str, &quot;@&quot;, -1, 1)
   For i = 0 To UBound(infoArray)       
       resultstr = Replace(resultstr,&quot;%n&quot;&amp;j&amp;&quot;%&quot;, infoArray(j))
       j = j + 1
   Next
   resultstr = Replace(resultstr,&quot;&amp;&quot;, &quot;=&quot;)
   
   ScriptResult = resultstr
.endvbscript</a:TemplateTargetItem.Value>
