<a:TemplateTargetItem.Value>.if (%getObjectType% != &quot;DatabaseOutput&quot;) &amp;&amp; (%getObjectType% != &quot;XMLOutput&quot;) &amp;&amp; (%getObjectType% != &quot;FlatFileOutput&quot;)
.set_value(tempCases, &quot;&quot;, new)
.set_value(isCaseWithDefault, &quot;false&quot;, new)
.set_value(tempCaseName, &quot;&quot;, new)
.foreach_item(OutgoingDataFlows)
.if %ConditionExpression% != &quot;&quot;
.if %tempCaseName% == &quot;&quot;
.set_value(tempCaseName, %procCaseName%)
.endif
.if %tempCases% != &quot;&quot;
.set_value(tempCases, %tempCases%@%procSwitchName%|%procSwitchExpression%)
.else
.set_value(tempCases, %procSwitchName%|%procSwitchExpression%)
.endif
.else
.set_value(isCaseWithDefault, &quot;true&quot;)
.endif
.next
.if %tempCases% != &quot;&quot;
&lt;!-- Additional DS Case for PD DataFlows --&gt;
&lt;DITransformCall typeId=&quot;38&quot; name=&quot;Case_Operation&quot; GUID=&quot;&quot;  &gt;
&lt;DIUIOptions&gt;\n
.foreach_part(%tempCases%, &#39;@&#39;)
.set_value(temp1, &quot;&quot;)
.set_value(temp2, &quot;&quot;)
.foreach_part(%CurrentPart%, &#39;|&#39;)
.if %temp1% == &quot;&quot;
.set_value(temp1, %CurrentPart%)
.else
.set_value(temp2, %CurrentPart%)
.endif
.next
.if %temp1% != &quot;&quot;
&lt;DIAttribute name=&quot;%temp1%&quot; value=&quot;%temp2%&quot;/&gt;\n
.endif
.next
.if %isCaseWithDefault%
&lt;DIAttribute name=&quot;%tempCaseName%_default&quot; value=&quot;DEFAULT&quot;/&gt;\n
.endif
&lt;DIAttribute name=&quot;ui_acta_from_schema_0&quot; value=&quot;%Code%&quot;/&gt;
&lt;DIAttribute name=&quot;ui_display_name&quot; value=&quot;%tempCaseName%&quot;/&gt;
&lt;/DIUIOptions&gt;
&lt;DIInputView name=&quot;%Code%&quot; /&gt;
&lt;DISchema name=&quot;%tempCaseName%&quot; &gt;
&lt;/DISchema&gt;
&lt;DICase&gt;\n
.foreach_part(%tempCases%, &#39;@&#39;)
.set_value(temp1, &quot;&quot;)
.set_value(temp2, &quot;&quot;)
.foreach_part(%CurrentPart%, &#39;|&#39;)
.if %temp1% == &quot;&quot;
.set_value(temp1, %CurrentPart%)
.else
.set_value(temp2, %CurrentPart%)
.endif
.next
.if %temp1% != &quot;&quot;
&lt;DISwitch label=&quot;%temp1%&quot; outputView=&quot;%temp1%&quot; isDefault=&quot;false&quot;&gt;
&lt;DIExpression isString=&quot;true&quot; expr=&quot;(%temp2%)&quot; &gt;
&lt;/DIExpression&gt;
&lt;/DISwitch&gt;\n
.endif
.next
.if %isCaseWithDefault%
&lt;DISwitch label=&quot;default&quot; outputView=&quot;%tempCaseName%_default&quot; isDefault=&quot;true&quot;&gt;
&lt;/DISwitch&gt;\n
.endif
&lt;/DICase&gt;
&lt;DIAttributes&gt;
&lt;DIAttribute name=&quot;Mutually_Exclusive&quot; value=&quot;yes&quot;/&gt;
&lt;DIAttribute name=&quot;Preserve_Expression_Order&quot; value=&quot;no&quot;/&gt;
&lt;/DIAttributes&gt;
&lt;/DITransformCall&gt;\n
.endif
.endif</a:TemplateTargetItem.Value>
