<a:CheckScript>Function %Check%(obj)   
   %Check% = True
   If obj.Parent.Parent.Type &lt;&gt; &quot;SAP Data Services 4.2&quot; Then
      Exit Function
   End If 
   If Not obj.FirstSourceObject is Nothing Then
       If obj.FirstSourceObject.TargetObject is Nothing Then
           Msgbox(&quot;Please open referenced XSMs of &quot; &amp; obj.Code &amp; &quot;, and then recheck the model.&quot;)
           %Check% = False
           Exit Function
       End If
   Else
       Exit Function
   End If     
   Dim col, sourceObj
   Dim resultObj
   For Each col In obj.Columns
       For Each sourceObj In col.SourceObjects
         If Not sourceObj.TargetObject is Nothing Then
            If sourceObj.TargetObject.Reference is Nothing Then
                Set resultObj = sourceObj.TargetObject
            Else
                Set resultObj = sourceObj.TargetObject.Reference
            End If
            If GetObjectType(resultObj) = &quot;Element&quot; Then
                If resultObj.GroupType &lt;&gt; &quot;&quot; Then
                    %Check% = False
                    Exit Function
                End If
            End If
         End If
       Next
   Next
End Function</a:CheckScript>
