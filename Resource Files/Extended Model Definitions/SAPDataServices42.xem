<a:CheckScript>Function %Check%(obj) 
   %Check% = True
   If obj.Parent.Parent.Type &lt;&gt; &quot;SAP Data Services 4.2&quot; Then
      Exit Function
   End If
   
   Dim targetObj,col 
   Set targetObj = obj.TargetObject
   If Not(targetObj is Nothing) Then
      If targetObj.TargetObject is Nothing Then
         Msgbox(&quot;Please open referenced PDMs of &quot; &amp; obj.Code &amp; &quot;, and then recheck the model.&quot;)
         %Check% = False
         Exit Function
      End If      
      For Each col In targetObj.TargetObject.Columns
          If col.Primary Then 
              Dim dscol  
              Dim isMapping
              isMapping = false
              For Each dscol In obj.Columns
                 If Not(dscol.TargetObject Is Nothing) Then       
                     If dscol.TargetObject.TargetObject.ObjectID = col.ObjectID Then
                           isMapping = true
                           Exit For
                     End If 
                 End If 
              Next
              If isMapping = False Then
                  %Check% = False
                  Exit Function
              End If
          End If
      Next
   End If
