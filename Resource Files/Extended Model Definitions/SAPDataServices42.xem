<a:ObjectID>C64F025D-CE12-4A59-A00C-2FB4AA9EE62E</a:ObjectID>
<a:Name>parseSourceDataType</a:Name>
<a:TemplateTargetItem.Value>.vbscript(%original%)    
   Dim infoArray()
   Dim defArrLen
   defArrLen = 6
   ReDim Preserve infoArray(defArrLen)
   str = ScriptInputArray(0)   
   Dim isNum
   Dim tempFir
   Dim tempSec
   Dim ascFir
   Dim ascSec
   Dim sourceType
   Dim tempString
   Dim resultString
   Dim returnValue
   Dim i
   Dim j
   j = 0
   tempString = &quot;&quot;
   resultString = &quot;&quot;
   returnValue = &quot;&quot;
   
   Dim suppDic
   Set suppDic = CreateObject(&quot;Scripting.Dictionary&quot;)  
   suppDic.Add &quot;)&quot;,&quot;&quot;
   suppDic.Add &quot;,&quot;,&quot;&quot;
   suppDic.Add &quot; &quot;,&quot;&quot;  
   
   Dim specialDtDic
   Set specialDtDic = CreateObject(&quot;Scripting.Dictionary&quot;)  
   specialDtDic.Add &quot;VARCHAR2&quot;,&quot;&quot;
   specialDtDic.Add &quot;DATETIME2&quot;,&quot;&quot;
   
   sourceType = 1
   If specialDtDic.exists(str) Then
       sourceType = 2
   End If
   
   For i = 0 To Len(str) - 1       
       tempFir = Mid(str,i+1,1)
       tempSec = Mid(str, i+2,1)
       ascFir = Asc(tempFir)
       ascSec = 0
       If tempSec &lt;&gt; &quot;&quot;  Then
           ascSec = Asc(tempSec)
       End If 
       isNum = False 
       If (ascFir &gt; 47) And (ascFir &lt; 58) Then
           tempString = tempString &amp; tempFir 
           isNum = True          
       End If     
       If sourceType = 2 Then
          If (suppDic.exists(tempFir)) Then
              If tempString &lt;&gt; &quot;&quot; Then
                  If (j + 1) &gt;= defArrLen Then
                      ReDim Preserve infoArray(j+1+1)
                  End If
                  infoArray(j+1) = tempString               
                  j = j + 1
                  resultString = resultString &amp; &quot;%n&quot; &amp; j
                  tempString = &quot;&quot;   
              End If  
          Elseif (isNum &lt;&gt; True) Or (tempSec = &quot;&quot;) Then
             resultString = resultString &amp; tempString            
             tempString = &quot;&quot;                      
          End if
       Else
          If (suppDic.exists(tempFir)) Or (tempSec = &quot;&quot;) Then
              If tempString &lt;&gt; &quot;&quot; Then
                  If (j + 1) &gt;= defArrLen Then
                      ReDim Preserve infoArray(j+1+1)
                  End If
                  infoArray(j+1) = tempString               
                  j = j + 1
                  resultString = resultString &amp; &quot;%n&quot; &amp; j
                  tempString = &quot;&quot;   
              End If  
          Elseif isNum &lt;&gt; True Then
             resultString = resultString &amp; tempString            
             tempString = &quot;&quot;                      
          End if
      End If      
      If isNum &lt;&gt; True Then
          resultString = resultString &amp; tempFir
      End If
   Next
   infoArray(0) = resultString
   For i = 0 To Ubound(infoArray)
       If infoArray(i) &lt;&gt; &quot;&quot; Then           
           If returnValue &lt;&gt; &quot;&quot; Then
               returnValue = returnValue &amp; &quot;@&quot; &amp; infoArray(i)
           Else
               returnValue = infoArray(i)
           End If           
       End if       
   Next
   If Not(suppDic Is Nothing) Then
       Set suppDic = Nothing
   End If 
   If Not(specialDtDic Is Nothing) Then
       Set specialDtDic = Nothing
   End If 

   ScriptResult = returnValue
.endvbscript</a:TemplateTargetItem.Value>
