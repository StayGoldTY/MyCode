<a:ObjectID>575588CC-188A-4F38-9B41-6970F95F3974</a:ObjectID>
<a:Name>Inconsistent inputs</a:Name>
<a:CreationDate>1413249904</a:CreationDate>
<a:Creator>I309440</a:Creator>
<a:ModificationDate>1420713874</a:ModificationDate>
<a:Modifier>I063945</a:Modifier>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:HelpMessage>All inputs to a data merge must have the same data structure columns, with identical column names, column types, column sizes, and column orders.
</a:HelpMessage>
<a:CheckScript>Function %Check%(obj)
   %Check% = True
   If obj.Parent.Parent.Type &lt;&gt; &quot;SAP Data Services 4.2&quot; Then
      Exit Function
   End If   
   
   Dim flow
   Dim i,t      
   Dim arrayTemp()
   Dim mergeColNum   
   Dim mergeColName
   Dim inComingNum
   
   If Not(obj.Columns Is Nothing) Then
       mergeColNum = obj.Columns.Count       
   End If 
   inComingNum = obj.IncomingDataFlows.Count
   Redim arrayTemp(inComingNum + 1) 
 
   For i = 0 To mergeColNum -1 
       t = 0
       mergeColName = obj.Columns.Item(i).Code &amp; &quot;-&quot; &amp; obj.Columns.Item(i).DataType
       arrayTemp(t) = mergeColName
       t = t + 1
       For Each flow In obj.IncomingDataFlows
          Dim flowColNum 
          flowColNum = flow.SourceObject.Columns.Count
          If (flowColNum - 1 &gt;= i) Then
              arrayTemp(t) = flow.SourceObject.Columns.Item(i).Code &amp; &quot;-&quot; &amp; flow.SourceObject.Columns.Item(i).DataType 
              t = t + 1 
          End If          
       Next 
       %Check% = isMatch(arrayTemp) 
       If (%Check% = False) Then
           Exit For
       End If      
   Next 
   
End Function

Function isMatch(tempArr)
   Dim m,n 
   n = 1
   isMatch = True         
   For m = 0 to UBound(tempArr) - 2          
      For n = m + 1 to UBound(tempArr)-1         
          If UCase(tempArr(m)) &lt;&gt; UCase(tempArr(n)) Then
              isMatch = False
              Exit For
          End If 
      Next
      If isMatch = False Then
          Exit For
      End If
   Next    
End Function</a:CheckScript>
<a:AutoFixScrpt>Function %Fix%(obj, outmsg)
   &#39; Implement your automatic correction on &lt;obj&gt; here
   &#39; filling &lt;outmsg&gt; as you wish
   &#39; and return True if successful.

   outmsg = &quot;Automatic correction not implemented&quot;

   %Fix% = False
End Function</a:AutoFixScrpt>
