<a:TemplateTargetItem.Value>.vbscript(%GV_dbtype%, %GV_ActiveModel%)    
    Const con_mapFolderPath = &quot;Settings\DataServicesMap&quot;   
    Const con_extenName = &quot;SAP Data Services 4.2&quot; 
    Dim matchObj 
    Dim extObj, wsObj  
    
    For Each mObj In Models
       If mObj.Code = ScriptInputArray(1) Then
          For Each extObj In mObj.ExtendedModelDefinitions
              If extObj.Name = con_extenName Then
                  If Not extObj.IsShortcut Then
                      Set matchObj = extObj.FindItemByPath(con_mapFolderPath, ScriptInputArray(0), false)
                  Else
                      Set matchObj = extObj.TargetObject.FindItemByPath(con_mapFolderPath, ScriptInputArray(0), false)
                  End If
                  Exit For
              End If
          Next
          Exit For
       End If
    Next  
    
    If Not (matchObj Is Nothing) Then
       ScriptResult = matchObj.List    
    End If     
.endvbscript</a:TemplateTargetItem.Value>
