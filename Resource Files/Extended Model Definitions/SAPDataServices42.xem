<a:Name>getTables</a:Name>
<a:TemplateTargetItem.Value>.foreach_item(Tables)
.//have to export all tables since datalookup table may not be a table from databaseinput/output
.set_value(GV_TableObjectID, %ObjectID%)
%findTableInList%
.if %GV_IsTemplateTable% != &quot;&quot;
.//get the name of table&#39;s owner, if owner is null, choose the login value as owner name
.set_value(ownerName, %GV_s1%, new)
.if %Owner% != null
.set_value(ownerName, %Owner.Code%, update)
.endif

.if %Columns.Count% &gt; 0
&lt;!-- Table definition --&gt;
&lt;DITable name=&quot;%Code%&quot; owner=&quot;%ownerName%&quot; datastore=&quot;%GV_s2%&quot; database=&quot;%GV_s2%&quot; description=&quot;%Comment%&quot; &gt;
&lt;DIProperties&gt;
&lt;DIAttribute name=&quot;Database_Object_Type&quot; value=&quot;TABLE&quot; /&gt;
&lt;DIAttribute name=&quot;Table_Type&quot; value=&quot;TABLE&quot; /&gt;
.//.if %Owner% != null
.//not work
.//.if %Owner.SchemaOwner% != null
.//.set_value(ownerName, %Owner.SchemaOwner.Code%, update)
.//.endif
.//.endif
&lt;DIAttribute name=&quot;db_alias_name&quot; value=&quot;%ownerName%&quot; /&gt;
&lt;DIAttribute name=&quot;Loader_Is_Template_Table&quot; value=&quot;%GV_IsTemplateTable%&quot; /&gt;
&lt;/DIProperties&gt;\n
.foreach_item(Columns)
.set_value(GV_s,%Mandatory%)
&lt;DIColumn name=&quot;%Name%&quot; nullable=&quot;%getNegation%&quot; 
.set_value(GV_datatype, %DataType%)
%getDBDataTypeMap% 
&lt;/DIColumn&gt;\n
.next
&lt;DIPrimaryKey&gt;\n
.foreach_item(Keys)
.if %Primary% == TRUE
.foreach_item(Columns)
&lt;DIPrimaryKeyColumn name=&quot;%Name%&quot; /&gt;\n
.next
.endif
.next
&lt;/DIPrimaryKey&gt;\n
.foreach_item(Indexes)
&lt;DITableIndex name=&quot;%Code%&quot; unique=&quot;%Unique%&quot;&gt;\n
.foreach_item(IndexColumns)
&lt;DIIndexColumn name=&quot;%Name%&quot; /&gt;\n
.next
&lt;/DITableIndex&gt;\n
.next
.foreach_item(Outer.References)
.if %ChildTable.Code% == %Outer.Code%
.if %ParentTable% != null
&lt;DIForeignKey name=&quot;%Code%&quot; referredTable=&quot;%ParentTable.Code%&quot; referredTableOwner=&quot;
.if %ParentTable.Owner% != null
%ParentTable.Owner.Code%
.endif
&quot; referredTableKey=&quot;%ParentKey.Code%&quot; &gt;\n
.foreach_item(Joins)
&lt;DIPKFKColumn referredColumn=&quot;%ParentTableColumn.Name%&quot; column=&quot;%ChildTableColumn.Name%&quot; /&gt;\n
.next
&lt;/DIForeignKey&gt;\n
.endif
.endif
.next
&lt;/DITable&gt;\n\n
.endif
.endif
.next</a:TemplateTargetItem.Value>
