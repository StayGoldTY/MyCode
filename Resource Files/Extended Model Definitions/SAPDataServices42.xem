<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:HelpMessage>XML outputs must not contain nested elements.</a:HelpMessage>
<a:CheckScript>Function %Check%(obj)   
   %Check% = True
   If obj.Parent.Parent.Type &lt;&gt; &quot;SAP Data Services 4.2&quot; Then
      Exit Function
   End If   
   If Not obj.TargetObject is Nothing Then
       If obj.TargetObject.TargetObject is Nothing Then
          Msgbox(&quot;Please open referenced XSMs of &quot; &amp; obj.Code &amp; &quot;, and then recheck the model.&quot;)
          %Check% = False
          Exit Function       
       End If
   Else
       Exit Function     
   End If      
   Dim col, targetObj
   Dim resultObj
   For Each col In obj.Columns
       If Not col.TargetObject is Nothing Then
          Set targetObj = col.TargetObject.TargetObject
          If Not targetObj is Nothing Then
              If targetObj.Reference is Nothing Then
                  Set resultObj = targetObj
              Else
                  Set resultObj = targetObj.Reference
              End If              
              If GetObjectType(resultObj) = &quot;Element&quot; Then
                  If resultObj.GroupType &lt;&gt; &quot;&quot; Then
                      %Check% = False
                      Exit Function
                  End If
              End If
          End If       
       End If
   Next
End Function</a:CheckScript>
