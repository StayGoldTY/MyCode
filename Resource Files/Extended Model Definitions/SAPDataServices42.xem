<a:ObjectID>F34D3517-929B-4637-9773-61CD7B957B5C</a:ObjectID>
<a:Name>Unsupported xs:NOTATION element</a:Name>
<a:CreationDate>1420698819</a:CreationDate>
<a:Creator>I309440</a:Creator>
<a:ModificationDate>1422437864</a:ModificationDate>
<a:Modifier>I309440</a:Modifier>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:HelpMessage>XML outputs must not contain xs:NOTATION elements or attributes.</a:HelpMessage>
<a:CheckScript>Function %Check%(obj) 
   %Check% = True
   If obj.Parent.Parent.Type &lt;&gt; &quot;SAP Data Services 4.2&quot; Then
      Exit Function
   End If
   Dim referItem
   Dim targetObjRef 
   Set targetObjRef = obj.TargetObject
   If Not(targetObjRef is Nothing) Then
      If targetObjRef.TargetObject is Nothing Then
         Msgbox(&quot;Please open referenced XSMs of &quot; &amp; obj.Code &amp; &quot;, and then recheck the model.&quot;)
         %Check% = False
         Exit Function
      End If
      
      Set referItem = targetObjRef.Targetobject.Reference     
      If referItem is Nothing Then
          %Check% = isValid(targetObjRef.Targetobject)         
      Else
          %Check% = isValid(referItem)          
      End If 
   End If
End Function

Function isValid(targetObj)
   Dim item, attr
   Dim targetEleName
   Dim targetGroupType
   isValid = true   
   targetEleName = targetObj.TypeName
   If targetEleName &lt;&gt; &quot;&quot; Then
       If targetEleName = &quot;xs:NOTATION&quot; Then
           isValid = False
           Exit Function
       End If
   End If          
   For Each attr In targetObj.Attributes
       If attr.Reference is Nothing Then                       
           If attr.TypeName = &quot;xs:NOTATION&quot; Then
               isValid = False
               Exit Function
           End If
       Else              
           If attr.Reference.TypeName = &quot;xs:NOTATION&quot; Then
               isValid = False
               Exit Function               
           End If
        End If
    Next          
    targetGroupType = targetObj.GroupType    
    If targetGroupType &lt;&gt; &quot;&quot; Then
       If targetGroupType &lt;&gt; &quot;group&quot; Then
           For Each item In targetObj.GroupItems          
               If (item.ClassName = &quot;Element&quot;) Or (item.ClassName = &quot;Attribute&quot;) Then       
                  If item.TypeName = &quot;xs:NOTATION&quot; Then
                      isValid = False
                      Exit Function
                  End If
               End If
           Next
       End If    
    End If
End Function</a:CheckScript>
<a:AutoFixScrpt>Function %Fix%(obj, outmsg)
   &#39; Implement your automatic correction on &lt;obj&gt; here
   &#39; filling &lt;outmsg&gt; as you wish
   &#39; and return True if successful.

   outmsg = &quot;Automatic correction not implemented&quot;

   %Fix% = False
End Function</a:AutoFixScrpt>
</o:CustomCheckTargetItem>
