<o:CustomCheckTargetItem Id="o260">
<a:ObjectID>E8A16DCB-6E7F-4F2B-85D3-46C728FF2C7D</a:ObjectID>
<a:Name>Inconsistent with inputs</a:Name>
<a:CreationDate>1418804885</a:CreationDate>
<a:Creator>I309440</a:Creator>
<a:ModificationDate>1420713851</a:ModificationDate>
<a:Modifier>I063945</a:Modifier>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:HelpMessage>Data merges must have the same data structure columns, with identical column names, column types, column sizes, and column orders as their inputs.</a:HelpMessage>
<a:CheckScript>Function %Check%(obj)
   %Check% = True
   If obj.Parent.Parent.Type &lt;&gt; &quot;SAP Data Services 4.2&quot; Then
      Exit Function
   End If
   
   Dim col
   Dim flow
   Dim i,j,t
   Dim inputNum    
   Dim arrayTemp()
   Dim result
   Dim num
   Dim colNum
   If Not(obj.IncomingDataFlows Is Nothing) Then
      inputNum = obj.IncomingDataFlows.Count      
      Redim arrayTemp(inputNum)
      i = 0
      For Each flow In obj.IncomingDataFlows
          arrayTemp(i) = flow.SourceObject.Columns.Count
          i = i + 1
      Next
      colNum = arrayTemp(0)
   End If
   result = True
   result = isMatch(arrayTemp)
   i = 1
   j = 1
   t = 0
   num = 1
   If (result = True) And (colNum &gt; 0) Then
      For num = 0 To colNum - 1          
          For Each flow In obj.IncomingDataFlows
             j = 0
             For Each col In flow.SourceObject.Columns
                 If j = num Then
                     arrayTemp(t) = col.Code &amp; &quot;-&quot; &amp; col.DataType 
                     t = t + 1
                     Exit For                  
                 Else
                     j = j + 1
                 End If                 
             Next            
             If t &gt;= inputNum Then
                 Exit For
             End If                     
         Next         
         If t &gt;= inputNum Then
             t = 0
             %Check% = isMatch(arrayTemp)
             If %Check% = False Then
                Exit For
             End If         
         End If 
      Next
   Else
       %Check% = False
   End If   
End Function

Function isMatch(tempArr)
   Dim m,n 
   n = 1
   isMatch = True         
   For m = 0 to UBound(tempArr) - 2         
      For n = m + 1 to UBound(tempArr)-1
          If UCase(tempArr(m)) &lt;&gt; UCase(tempArr(n)) Then
              isMatch = False
              Exit For
          End If 
      Next
      If isMatch = False Then
          Exit For
      End If
   Next    
End Function</a:CheckScript>
<a:AutoFixScrpt>Function %Fix%(obj, outmsg)
   &#39; Implement your automatic correction on &lt;obj&gt; here
   &#39; filling &lt;outmsg&gt; as you wish
   &#39; and return True if successful.

   outmsg = &quot;Automatic correction not implemented&quot;

   %Fix% = False
End Function</a:AutoFixScrpt>
