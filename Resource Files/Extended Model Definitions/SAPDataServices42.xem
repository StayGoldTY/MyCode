<a:ObjectID>BE12B2A1-8428-487F-A4FA-11CF2B59E3CB</a:ObjectID>
<a:Name>Missing data mapping</a:Name>
<a:CreationDate>1420448574</a:CreationDate>
<a:Creator>I309440</a:Creator>
<a:ModificationDate>1422437848</a:ModificationDate>
<a:Modifier>I309440</a:Modifier>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:HelpMessage>XML output elements and attributes must have data mappings.</a:HelpMessage>
<a:CheckScript>Function %Check%(obj) 
   %Check% = True
   If obj.Parent.Parent.Type &lt;&gt; &quot;SAP Data Services 4.2&quot; Then
      Exit Function
   End If
   
   Dim targetObj,col,attr,item

   Set targetObj = obj.TargetObject
   If Not(targetObj is Nothing) Then
      If targetObj.TargetObject is Nothing Then
         Msgbox(&quot;Please open referenced XSMs of &quot; &amp; obj.Code &amp; &quot;, and then recheck the model.&quot;)
         %Check% = False
         Exit Function
      End If 
      
      Dim refObj
      Dim resultObj
      Set refObj = targetObj.TargetObject.Reference
      If refObj is Nothing Then
         Set resultObj = targetObj.TargetObject
      Else
         Set resultObj = refObj
      End If   
      For Each attr In resultObj.Attributes
          If attr.Reference is Nothing Then               
              %Check% = isMapping(obj, attr)
          Else
              %Check% = isMapping(obj, attr.Reference)
          End If
          If %Check% = false Then
              Exit Function
          End If
       Next
       If resultObj.GroupType &lt;&gt; &quot;&quot; Then
          If resultObj.GroupType &lt;&gt; &quot;group&quot; Then
              For Each item In resultObj.GroupItems  
                  %Check% = isMapping(obj, item)        
                  If %Check% = false Then
                      Exit Function
                  End If
              Next
          End If      
       End If
   End If
End Function

Function isMapping(obj, mappObj)
    isMapping = false
    Dim dscol
    For Each dscol In obj.Columns
       If Not(dscol.TargetObject Is Nothing) Then
           If dscol.TargetObject.TargetObject.ObjectID = mappObj.ObjectID Then
                 isMapping = true
                 Exit Function
           End If 
