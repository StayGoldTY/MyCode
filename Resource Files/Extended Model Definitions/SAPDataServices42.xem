&lt;sql_server_dataserver&gt;%ServerName%&lt;/sql_server_dataserver&gt;
&lt;sql_server_database&gt;%DatabaseName%&lt;/sql_server_database&gt;
&lt;db_port_no&gt;%Port%&lt;/db_port_no&gt;
&lt;user&gt;%UserName%&lt;/user&gt;
&lt;password&gt;&lt;/password&gt;
.endif
&lt;/DSConfiguration&gt;
&lt;/DSConfigurations&gt;
&lt;/DIAttribute&gt;
&lt;/DIAttributes&gt;
&lt;/DIDatabaseDatastore&gt;\n\n
.endif
.endif
.next</a:TemplateTargetItem.Value>
