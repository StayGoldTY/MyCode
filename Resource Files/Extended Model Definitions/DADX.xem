<o:EventHandlerTargetItem Id="o26">
<a:ObjectID>C6CEC9D4-1D2B-4990-A052-7135213F6BF4</a:ObjectID>
<a:Name>Initialize</a:Name>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:HandlerScript>Function %Initialize%(obj)
   &#39;check if namespaces with xs and wsdl already exist
   Dim curNS, bCreateXS, bCreateWSDL
   bCreateXS = True
   bCreateWSDL = True
   For Each curNS In obj.Namespaces
      If curNS.Prefix = &quot;xs&quot; Then
         bCreateXS = False
      End If
      If curNS.Prefix = &quot;wsdl&quot; Then
         bCreateWSDL = False
      End If
   Next
   &#39;add default namespaces
   Dim namespace
   If bCreateXS Then 
      set namespace = obj.Namespaces.CreateNew
      namespace.Prefix = &quot;xs&quot;
      namespace.Uri = &quot;http://www.w3.org/2001/XMLSchema&quot;
   End If
   Dim namespacewsdl
   If bCreateWSDL Then 
      set namespacewsdl = obj.Namespaces.CreateNew
      namespacewsdl.Prefix = &quot;wsdl&quot;
      namespacewsdl.Uri = &quot;http://schemas.xmlsoap.org/wsdl/&quot;
   End If
   %Initialize% = True
End Function</a:HandlerScript>
</o:EventHandlerTargetItem>
</c:Categories>
