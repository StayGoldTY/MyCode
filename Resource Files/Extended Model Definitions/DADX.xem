<a:TemplateTargetItem.Value>.vbscript
On Error Goto 0
Dim ResultColumn
set ResultColumn = ActiveSelection.Item(0)

Dim ElementShortcut
set ElementShortcut = ResultColumn.TypeObject
If Not (ElementShortcut is nothing) Then
   Dim Element
   If (ElementShortcut.IsShortcut) Then
      set Element = ElementShortcut.TargetObject
   End If
   If Not (Element is nothing) Then      
      Dim xem
      Dim value
      For Each xem In Element.Model.ExtendedModelDefinitions
         If (xem.Code = &quot;DB2XMLExtender&quot;) Then
            ScriptResult = Element.EvaluateText(&quot;%StorageName%&quot;, xem)               
         End If
      Next      
   End If
End If
.endvbscript</a:TemplateTargetItem.Value>
