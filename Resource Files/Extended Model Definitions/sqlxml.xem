<a:ObjectID>C7078CC0-789D-4539-ABD2-2EF05C7E716E</a:ObjectID>
<a:Name>generateSQLXMLQueries</a:Name>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:Comment>Generate a SQL/XML Query for  each root element</a:Comment>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:MethodScript>Sub %Method%(obj)

   Dim selection &#39; as ObjectSelection
   
   &#39; Create a new selection
   set selection = obj.CreateSelection
   
   &#39; Add all objects of the active selection
   selection.AddActiveSelectionObjects
   
   &#39; Generate SQL/XML queries
   InteractiveMode = im_Dialog
   obj.GenerateFiles &quot;&quot;, selection, &quot;%CurrentTargetCode%&quot;

   selection.Delete
   
End Sub</a:MethodScript>
