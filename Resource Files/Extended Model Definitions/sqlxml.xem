<o:TemplateTargetItem Id="o12">
<a:ObjectID>42FD24B9-EDAE-439F-A82D-CFC6E8EF6AD1</a:ObjectID>
<a:Name>xmlcontent</a:Name>
<a:TemplateTargetItem.Value>.set_object(groupParticle, %Group%, new)
.// element base on complex type, get group particle of complex type
.if ((%BaseType%) and (%BaseType.ObjectType% == ComplexType))
.set_object(groupParticle, %BaseType.Group%)
.endif
.if ((%groupParticle%) and (%groupParticle.ObjectType% != Group))
,
.// group particle contains several items
.if (%groupParticle.Items.Count% &gt; 1)
XMLCONCAT(
.endif
.foreach_item ( groupParticle.Items ,\n)
.set_value(mappedtable, %table%, new)
.// a table or view is mapped to the element
.if (%table%)
(select XMLAGG ( %xmlelement% )
 from %table%
  .if (%DefaultMapping%)
    .foreach_item(DefaultMapping.SourceLinks,&quot;\n where &quot;)
    .// filter on child table
    .if (%ObjectType% == Reference)
       .if ((%ChildTable.Code% == %mappedtable%)) 
          .foreach_item(Joins)
%ParentTableColumn.FullCode% = %ChildTableColumn.FullCode%
          .next(&quot;\n   and &quot;)
       .endif
    .elsif (%ObjectType% == ViewReference)
       .if ((%TableView2.Code% == %mappedtable%)) 
          .foreach_item(Joins)
%Column1.FullCode% = %Column2.FullCode%
          .next(&quot;\n   and&quot;)
       .endif
    .endif
    .next(&quot; and \n&quot;)
    .if %DefaultMapping.Criteria%
      .if (%DefaultMapping.SourceLinks.IsEmpty%)
\n where %DefaultMapping.Criteria%
      .else
 and \n      %DefaultMapping.Criteria%
      .endif
    .endif
   .endif(\n)
.else
    %xmlelement%
.endif
.if (%table%) 
)
.endif
.next(,\n)
.if (%groupParticle.Items.Count% &gt; 1)
)
.endif
.else
.// there is no group for this element
 .if (%column%)
,%column%
 .endif
.endif</a:TemplateTargetItem.Value>
