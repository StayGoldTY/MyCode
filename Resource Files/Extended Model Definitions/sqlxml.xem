<o:TemplateTargetItem Id="o10">
<a:ObjectID>3B7533BE-80C6-4054-B663-38670739F4C0</a:ObjectID>
<a:Name>xmlattributes</a:Name>
<a:TemplateTargetItem.Value>.if (%Parent%) and (%Parent.ObjectType% == Model) and (%Parent.TargetNamespace% != &quot;&quot;)
, XMLATTRIBUTES (%.q:Parent.TargetNamespace% as &quot;xmlns&quot;)
.endif
.if (%DefaultMapping%)
   .foreach_item(DefaultMapping.AttributeMappings,&quot;, XMLATTRIBUTES (&quot;, &quot;)&quot;)
      .if (%MappedTo% == %Attribute.Code%)
         .if (%SourceFeature%)
%MappedTo%
         .else
%SourceFeature.GeneratedName%
         .endif
      .else
         .if (%SourceFeature%)
%SourceFeature.GeneratedName% AS %.Q:Attribute.Code%
         .else
%MappedTo% AS %.Q:Attribute.Code%
         .endif
      .endif
   .next(,)
.endif</a:TemplateTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:Comment>List of attributes</a:Comment>
