<a:Name>query</a:Name>
<a:TemplateTargetItem.Value>select 
.if (%Model.GenOptions.AddXMLHeader%)
&#39;&lt;?xml version=&quot;1.0&quot; encoding=&quot;UTF-8&quot; ?&gt;&#39; ||\n
.endif
.if (%isDB2%)
VARCHAR(XML2CLOB(\n
.elsif (%isOracle%)
sys.xmlType.getCLobVal(
.endif
%xmlelement%\n
.if (%table%)
from %table%
.elsif (%isDB2%)
))
from sysibm.sysdummy1
.elsif (%isOracle%)
) from dual
.elsif (%isASA%)
.// nothing
.endif


</a:TemplateTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
