<o:TemplateTargetItem Id="o11">
<a:ObjectID>F7D7B80A-E3D0-4E09-852C-0E3991CAFC2C</a:ObjectID>
<a:Name>xmlelement</a:Name>
<a:TemplateTargetItem.Value>XMLELEMENT( NAME %.Q:Code%[%xmlattributes%][%xmlcontent%])
</a:TemplateTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
