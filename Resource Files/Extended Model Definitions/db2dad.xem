<a:Name>namespaceNodeDeclaration</a:Name>
<a:TemplateTargetItem.Value>.if (%NamespaceNode%)
   .set_value(_name, &quot;&quot;, new)
   .set_value(_value, &quot;&quot;, new)
   .foreach_line(%NamespaceNode%)
      .foreach_part(%CurrentLine%, &quot;&#39;=&#39;&quot;)
         .ifnot (%_name%)
            .set_value(_name, &quot;%CurrentPart%&quot;, update)
         .elsifnot (%_value%)
            .set_value(_value, &quot;%CurrentPart%&quot;, update)
         .endif
      .next()
&lt;namespace_node name=&quot;%.T:_name%&quot; value=&quot;%.T:_value%&quot;/&gt;
      .set_value(_name, &quot;&quot;, update)
      .set_value(_value, &quot;&quot;, update)
   .next(\n)
.endif</a:TemplateTargetItem.Value>
