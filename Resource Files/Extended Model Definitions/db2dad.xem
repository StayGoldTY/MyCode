create procedure %Code%_InsertXML(xmlpath varchar(256))
language SQL
begin
   declare out_code integer;
   declare out_msg varchar(256);
   call DB2XML.dxxInsertXML(&#39;%StorageName%&#39;, DB2XML.XMLCLOBFromFile(xmlpath), 
      out_code, out_msg);
   IF (out_code &lt;&gt; 0) THEN
      SIGNAL SQLSTATE &#39;75007&#39; set message_text=out_msg;
   END IF;
end
%endins%

drop procedure %Code%_ShredXML
%endins%
create procedure %Code%_ShredXML(xmlpath varchar(256))
language SQL
begin
   declare out_code integer;
   declare out_msg varchar(256);
   call DB2XML.dxxShredXML(DB2XML.XMLCLOBFromFile(&#39;%getGenerationPath%%Name%.dad&#39;), DB2XML.XMLCLOBFromFile(xmlpath), out_code, out_msg);
   IF (out_code &lt;&gt; 0) THEN
      SIGNAL SQLSTATE &#39;75007&#39; set message_text=out_msg;
   END IF;
end
%endins%

drop procedure %Code%_GenXML
%endins%
create procedure %Code%_GenXML(xmlpath varchar(256))
language SQL
begin
   create table Temp_%Code%_XML(ITEM DB2XML.XMLvarchar, DXX_VALID INTEGER);
   begin
      declare res varchar(4000);
      declare out_code integer;
      declare out_code2 integer;
      declare out_msg varchar(256);
      declare exit handler for sqlexception
      begin
         drop table Temp_%Code%_XML;
      end;

      call DB2XML.dxxGenXML(
      DB2XML.XMLCLOBFromFile(&#39;%getGenerationPath%%Name%.dad&#39;), &#39;Temp_%Code%_XML&#39;, &#39;ITEM&#39;, &#39;DXX_VALID&#39;, 0, &#39;&#39;, 500, out_code, out_code2, out_msg);
      IF (out_code2 &lt;&gt; 0) THEN
         drop table Temp_%Code%_XML;
         SIGNAL SQLSTATE &#39;75007&#39; set message_text=out_msg;
      END IF;
      begin
         declare v_no_data SMALLINT DEFAULT 0;
         declare v_prolog     varchar(4000);
         declare v_xmlcontent varchar(4000);
         begin
            declare c_xml cursor for 
            select 
              SUBSTR(DB2XML.VARCHARTOVARCHAR(ITEM), 1, SYSFUN.LOCATE(&#39;&lt;&#39;, DB2XML.VARCHARTOVARCHAR(ITEM), SYSFUN.LOCATE(&#39;!DOCTYPE&#39;, DB2XML.VARCHARTOVARCHAR(ITEM) )) -1) as prolog,
              SUBSTR(DB2XML.VARCHARTOVARCHAR(ITEM), SYSFUN.LOCATE(&#39;&lt;&#39;, DB2XML.VARCHARTOVARCHAR(ITEM), SYSFUN.LOCATE(&#39;!DOCTYPE&#39;, DB2XML.VARCHARTOVARCHAR(ITEM) ))) as xmlcontent
            FROM Temp_%Code%_XML;
            declare continue handler for not found set v_no_data = 1;

            open c_xml;
            set res = &#39;&#39;;
               FETCH from c_xml INTO v_prolog, v_xmlcontent;
               WHILE( v_no_data = 0 ) DO
   	      set res = concat(res,v_xmlcontent);
                                      set res = concat(res, CHR(10));
   	      FETCH from c_xml INTO v_prolog, v_xmlcontent;
            END WHILE;

            set res = concat(v_prolog,res);
            close c_xml;
         end;
      end;
      set res = DB2XML.XMLFileFromVarchar(res, xmlpath, &#39;UTF-8&#39;);
      drop table Temp_%Code%_XML;
   end;
end
%endins%

drop procedure %Code%_RetrieveXML
%endins%
create procedure %Code%_RetrieveXML(xmlpath varchar(256))
language SQL
begin
   create table Temp_%Code%_XML(ITEM DB2XML.XMLvarchar, DXX_VALID INTEGER);
   begin
      declare res varchar(4000);
      declare out_code integer;
