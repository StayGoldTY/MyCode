<a:ObjectID>092F80C8-9E4D-4CAA-8234-5B97327D07C6</a:ObjectID>
<a:Name>Custom Checks</a:Name>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
<c:Categories>
<o:CustomCheckTargetItem Id="o77">
<a:ObjectID>ED67ACB8-3633-441D-B4A5-CFB376FC44D0</a:ObjectID>
<a:Name>Source tables flagged &quot;generated&quot; </a:Name>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:Comment>For an Xcolumn mapping, source table of root element must be flagged not generated.</a:Comment>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:HelpMessage>This check ensures that source tables are flagged not generated</a:HelpMessage>
<a:OutputMessage>The source table should be not generated :</a:OutputMessage>
<a:CheckScript>&#39;static variable for array of sources
Dim index
index = 0

&#39;this function recursivly get the table source of the root element and its children
Function getSource(element, coll)
   Dim sf, elt, shortcutclosed
   &#39;get source part
   if ((Not IsNull(element)) and (Not IsEmpty(element.DefaultMapping))) then
      For each sf in element.DefaultMapping.ElementSources
         shortcutclosed = false
         If (Not isNull(sf)) Then
            &#39;case of a shortcut, retrieve the object
            If (sf.IsShortcut) Then 
               If Not (sf.TargetObject Is Nothing) Then
                  Set sf = sf.TargetObject
               else
                  shortcutclosed = true
               End If
            End If
            
            If (Not shortcutclosed) Then
               &#39;msgbox(sf)
               If (sf.IsKindOf(PDPDM.cls_Column)) Then
                  Set sf = sf.Parent
               End if
               
               &#39;test generated flag on source tables
               If (sf.generated = True) Then
                  coll(index) = false
               else
                  coll(index) = true
               End if
               
               &#39;update index and array size dynamically
               index = index + 1
               ReDim Preserve coll(index+1)
            End if
         End if
      Next
   end if
   
   &#39;rec part
   If (Not IsNull(element)) Then
      If (Not (element.Group Is Nothing)) Then
         For each elt in element.Group.Items
            If (Not IsNull(elt)) Then
               Call getSource(elt, coll)         
            End If
         Next
      End If
   End If
End Function

Function %Check%(obj)
   Dim ret, i
   Dim coll()
   Redim coll(1)
   ret = True
   
   &#39;do the check only if Exa Attr StorageType is Xcolumn
   if (obj.GetExtendedAttribute(&quot;StorageType&quot;) = &quot;Xcolumn&quot;) Then
      &#39;Get all sources from element and its children
      Call getSource(obj, coll)
      &#39;check if all sources are flagged NOT GENERATE
      For i = 1 to index-1
         If (coll(i) = False) Then
            ret = False
         End If
      Next
   End if
   %Check% = ret
End Function</a:CheckScript>
<a:AutoFixScrpt>Function FixGeneratedFlag(element)
   Dim sf, elt, shortcutclosed
   &#39;get source part
   if ((Not IsNull(element)) and (Not IsEmpty(element.DefaultMapping))) then
      For each sf in element.DefaultMapping.ElementSources
         shortcutclosed = false
         If (Not isNull(sf)) Then
            &#39;case of a shortcut, retrieve the object
            If (sf.IsShortcut) Then 
               If Not (sf.TargetObject Is Nothing) Then
                  Set sf = sf.TargetObject
               else
                  shortcutclosed = true
               End If
            End If
            
            If (Not shortcutclosed) Then
               If (sf.IsKindOf(PDPDM.cls_Column)) Then
                  Set sf = sf.Parent
               End if
               
               &#39;fix generated flag on source tables to false
               If (sf.generated = True) Then
                  sf.generated = false
               End if
            End if
         End if
      Next
   end if
   
   &#39;rec part
   If (Not IsNull(element)) Then
      If (Not (element.Group Is Nothing)) Then
         For each elt in element.Group.Items
            If (Not IsNull(elt)) Then
               Call FixGeneratedFlag(elt)         
            End If
         Next
      End If
   End If
End Function


Function %Fix%(obj, outmsg)
   Dim ret
   ret = False
   if (obj.GetExtendedAttribute(&quot;StorageType&quot;) = &quot;Xcolumn&quot;) Then
      FixGeneratedFlag(obj)
      outmsg = &quot;Generated flag of source tables set to false&quot;
      ret = True
   End if
   %Fix% = ret
End Function</a:AutoFixScrpt>
<a:EnableAutoFix>1</a:EnableAutoFix>
</o:CustomCheckTargetItem>
</c:Categories>
</o:TypedCategoryTargetItem>
