<a:ObjectID>5524E034-184E-4926-B747-E94B1882EE44</a:ObjectID>
<a:Name>genDAD</a:Name>
<a:TemplateTargetItem.Value>&lt;?xml version=&quot;1.0&quot;?&gt;\n
.if (%GenOptions.optPathDADDTD%)
&lt;!DOCTYPE DAD SYSTEM &quot;%GenOptions.optPathDADDTD%&quot;&gt;\n
.endif
&lt;DAD&gt;
[%dtdId% 
][%validation% 
]
.if (%StorageType% == &quot;Xcolumn&quot;)
%xcolumn%
.elsif (%StorageType% == &quot;Xcollection&quot;)
%xcollection%
.endif

&lt;/DAD&gt;

</a:TemplateTargetItem.Value>
