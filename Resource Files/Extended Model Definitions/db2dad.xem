<o:TemplateTargetItem Id="o20">
<a:ObjectID>EE473E63-9BA7-4652-9069-DF91BC97031A</a:ObjectID>
<a:Name>elementNodeDeclaration</a:Name>
<a:TemplateTargetItem.Value>.ifnot (%Reference%)
&lt;element_node[ name=&quot;%Code%&quot;][ %elementMultiOccurence%][ ID=&quot;%ID%&quot;][ BASE_URI=&quot;%elementBaseURI%&quot;]&gt;\n
   .if (%DefaultMapping%) and (%DefaultMapping.ElementSources.Count% &gt; 0)
   %rdbNode%\n
   .endif
   .if (%Attributes.Count% &gt; 0)
      .foreach_item(Attributes)
   %attributeNodeDeclaration%
      .next(\n)
   .elsifnot (%Group%)
   %textNodeDeclaration%
   .endif()
   .if (%Attributes.Count% == 0) and (%Group%)
[%childrenElementNodeDeclaration%]
   .else
[
%childrenElementNodeDeclaration%]   
   .endif()
.ifnot ((%Group%) or (%Attributes.Count% != 0))
.endif(\n)
[
   %namespaceNodeDeclaration%][
   %processInstNodeDeclaration%][
   %commentNodeDeclaration%]
&lt;/element_node&gt;
.else
.// case of reference 
   .if (%DefaultMapping%)
.// Why a mapping on a reference element ?
&lt;element_node[ name=&quot;%Reference.Code%&quot;][ %elementMultiOccurence%][ ID=&quot;%ID%&quot;][ BASE_URI=&quot;%elementBaseURI%&quot;]&gt;\n
      .ifnot (%Group%)
   %textNodeDeclaration%
      .endif()
[   %namespaceNodeDeclaration%][
   %processInstNodeDeclaration%][
   %commentNodeDeclaration%]
&lt;/element_node&gt;
   .else
%Reference.elementNodeDeclaration%
   .endif
.endif</a:TemplateTargetItem.Value>
