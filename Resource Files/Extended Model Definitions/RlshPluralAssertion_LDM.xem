<a:Name>pluralAssertion</a:Name>
<a:TemplateTargetItem.Value>Each %Entity1.Name%
.if %Entity1ToEntity2RoleMandatory%
 must
.else
 may
.endif
.if %Entity1ToEntity2Role%
 %.L:Entity1ToEntity2Role%
.else
 have
.endif
.if %Entity1ToEntity2RoleMaximumCardinality%==1
   .if %Entity1ToEntity2RoleMandatory%
 one and only one
   .else
 at most one
   .endif
.else
 one or more
.endif
.if %Entity1%==%Entity2%
 other
.endif
.if %Entity1ToEntity2RoleMaximumCardinality%==1
 %Entity2.Name%
.else
   .if %Entity2.PluralName%
 %Entity2.PluralName%
   .else
 %Entity2.Name%s
   .endif
.endif
..
Each %Entity2.Name%
.if %Entity2ToEntity1RoleMandatory%
 must
.else
 may
.endif
.if %Entity2ToEntity1Role%
 %.L:Entity2ToEntity1Role%
.else
 have
.endif
.if %Entity2ToEntity1RoleMaximumCardinality%==1
.if %Entity2ToEntity1RoleMandatory%
 one and only one
.else
 at most one
.endif
.else
 one or more
.endif
.if %Entity1%==%Entity2%
 other
.endif
.if %Entity2ToEntity1RoleMaximumCardinality%==1
 %Entity1.Name%
.else
   .if %Entity1.PluralName%
 %Entity1.PluralName%
   .else
 %Entity1.Name%s
    .endif
.endif
..</a:TemplateTargetItem.Value>
