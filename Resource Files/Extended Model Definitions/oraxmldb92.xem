<o:TemplateTargetItem Id="o7">
<a:ObjectID>BE4424FD-3DDA-4A29-BF84-E7BFF10F37BD</a:ObjectID>
<a:Name>getXmlSchemaMapping</a:Name>
<a:TemplateTargetItem.Value>.vbscript()
Dim sPrefix
bFound = ActiveModel.HasNamespace(&quot;http://xmlns.oracle.com/xdb&quot;)
if (not bFound) then
   ScriptResult  = &quot;sql:&quot;
else
sPrefix = ActiveModel.GetNamespacePrefix(&quot;http://xmlns.oracle.com/xdb&quot;)
if (sPrefix &lt;&gt; &quot;&quot;) then
   ScriptResult = sPrefix + &quot;:&quot;
end if
end if
.endvbscript</a:TemplateTargetItem.Value>
