<a:TemplateTargetItem.Value>using System;
using System.Collections.Generic;
using System.Text;
using Microsoft.Practices.Mobile.PasswordAuthentication;
using Microsoft.Practices.Mobile.CompositeUI;

namespace %Model.Code%ShellApplication
{
	public class MobilePasswordAuthenticationService : IMobilePasswordAuthenticationService
	{
		private AuthenticationToken token;

		public MobilePasswordAuthenticationService ()
		{
		}

		internal void SetTokenData(string tokenData)
		{
			token = new AuthenticationToken(tokenData);
		}

		public PasswordIdentity CheckCredentials(string userName, string password)
		{
			using (RsaAesCryptographyProvider cryptoProvider = new RsaAesCryptographyProvider(&quot;AdventureWorksToGo&quot;))
			{
				return token.Authenticate(userName, password, cryptoProvider);
			}
		}
	}
}
