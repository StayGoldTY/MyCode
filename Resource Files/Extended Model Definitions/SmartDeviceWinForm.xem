<a:TemplateTargetItem.Value>.vbscript(%imageSourcePath%,%Model.shellModuleName%,%generationPath%)
Dim srcPath,desPath,desPath1,desPath2,desPath3,desPath4
srcPath = ScriptInputArray(0)
.//desPath = ScriptInputArray(1)+&quot;\&quot;
.//desPath2 = ScriptInputArray(1)+&quot;\&quot;
desPath1 = ScriptInputArray(2)+&quot;\&quot;
.//desPath3 = desPath1+desPath2
desPath4 = &quot;Resources&quot;
.//desPath = ScriptInputArray(2)+&quot;\&quot;+ScriptInputArray(1)+&quot;\&quot;+&quot;Resources\&quot;
desPath = desPath1+&quot;\&quot;+&quot;Resources\&quot;
Dim fso,folder,subFolder
Set fso = CreateObject(&quot;Scripting.FileSystemObject&quot;)
.//If desPath = &quot;\&quot; Then
If (fso.FolderExists(desPath)) Then
Else
   Set folder = fso.GetFolder(desPath1)
   folder.SubFolders.Add(desPath4)
End If

If (fso.FolderExists(srcPath)) Then 
   If(fso.FolderExists(desPath)) Then 
   
     For each file In fso.GetFolder(desPath).Files
         file.Delete True
     Next  
     
     For each file In fso.GetFolder(srcPath).Files  
         file.Copy(desPath+file.Name)
      Next 
   Else
      MsgBox(&quot;The selected image location folder does not exist!&quot;) 
  End If
End If 
.endvbscript 
