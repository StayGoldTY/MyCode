            presenter.View = (I%Code%ListView)this.%classInstance%ListView;

            //presenter.UpdateCustomerList();

            WindowSmartPartInfo info = new WindowSmartPartInfo();
            shell.DialogWorkspace.SmartPartClosing += new EventHandler&lt;WorkspaceCancelEventArgs&gt;(OnSmartPartClosing);
            shell.DialogWorkspace.Show(this.%classInstance%ListView, info);

        }

        public void Close(object presenter)
        {
            if (presenter is %Code%ListPresenter)
            {
                ((Form)shell.DialogWorkspace.ActiveSmartPart).Close();
            }
        }

