<a:TemplateTargetItem.Value>using System;
using System.Collections.Generic;
using System.Text;
using Microsoft.Practices.Mobile.PasswordAuthentication;
using Microsoft.Practices.Mobile.Configuration;
using Microsoft.Practices.Mobile.CompositeUI;
using Microsoft.Practices.Mobile.CompositeUI.Services;
using Microsoft.Practices.Mobile.CompositeUI.Utility;
using System.Security.Cryptography;
using System.IO;

namespace %Model.Code%ShellApplication
{
	public class LoginPresenter
	{
		private WorkItem rootWorkItem;
		private IMobilePasswordAuthenticationService mobileAuthenticationService;
		private ILoginView view;

		public LoginPresenter(\[ServiceDependency\] WorkItem rootWorkItem)
		{
			this.rootWorkItem = rootWorkItem;
		}

		\[ServiceDependency\]
		public IMobilePasswordAuthenticationService MobilePasswordAuthenticationService
		{
			set { mobileAuthenticationService = value; }
		}

		public ILoginView View
		{
			set 
			{
				Guard.ArgumentNotNull(value, &quot;value&quot;);
				view = value;
				view.LoginEvent += view_LoginEvent;
			}
		}

		void view_LoginEvent(object sender, EventArgs e)
		{
			PasswordIdentity identity = ValidateLogin(view.Username, view.Password);
			if (identity != null &amp;&amp; identity.IsAuthenticated)
			{
				rootWorkItem.Items.Add(identity, &quot;userIdentity&quot;);
				InitializeConfigurationProvider(identity.CryptoKey);
				view.Close();
			}
			else
				view.ShowMessage(&quot;User is not Authenticated&quot;);
		}

		public PasswordIdentity ValidateLogin(string userName, string passWord)
		{
			if (mobileAuthenticationService == null)
				return new PasswordIdentity(userName, passWord, new RsaAesCryptographyProvider(string.Empty));

			return mobileAuthenticationService.CheckCredentials(userName, passWord);
		}

		/// &lt;summary&gt;
		///		In a real application, you would need to add additional code here. The key used to
		///		decrypt configuration sections is protected by a password-derived key &lt;paramref name=&quot;passwordKey&quot;/&gt;
		///		here. In a real application, you would want to save this encrypted key in a file or
		///		the registry. You would also have additional code that could re-encrypt the key with
		///		a new password and save it again so the user can change their password. Here we&#39;re
		///		simply hard-coding the encrypted key.
		/// &lt;/summary&gt;
		/// &lt;param name=&quot;passwordKey&quot;&gt;&lt;/param&gt;
		private void InitializeConfigurationProvider(byte\[\] passwordKey)
		{
			String contents = FileUtility.ReadFile(&quot;Config.EncryptedKey&quot;);
			byte\[\] encryptedKey = Convert.FromBase64String(contents);
			Rijndael algorithm = RijndaelManaged.Create();
			CryptographyBlock block = new CryptographyBlock(algorithm, passwordKey);
			byte\[\] key = block.Decrypt(encryptedKey);
			ConfigurationManager.ProtectedConfigurationProvider = new RijndaelConfigurationProvider(key);
		}
	}
}</a:TemplateTargetItem.Value>
