<a:Name>batchSavingMetadataDefinition</a:Name>
<a:TemplateTargetItem.Value>/// &lt;summary&gt;
/// Batch saving if there are many records saved once.
/// &lt;/summary&gt;
/// &lt;param name=&quot;%classInstance%&quot;&gt;%classCode% domain object&lt;/param&gt;
/// &lt;returns&gt;true if operation is successful&lt;/returns&gt;
public bool BatchSave(List&lt;BatchSavingStruct&lt;%classCode%&gt;&gt; %collectionClassInstance%)
{
   foreach (BatchSavingStruct&lt;%classCode%&gt; batch in %collectionClassInstance%)
   {
      bool result = true;
      switch (batch.Type)
      {                
         case ObjectStatus.Added:
            result &amp;= Insert(batch.Model);
            break;
         case ObjectStatus.Modified:
            result &amp;= Update(batch.Model);
            break;
         case ObjectStatus.Deleted:
            result &amp;= Delete(batch.Model);
            break;  
      }
   }
   return true;
}</a:TemplateTargetItem.Value>
