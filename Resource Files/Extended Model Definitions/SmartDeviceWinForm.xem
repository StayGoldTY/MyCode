<a:TemplateTargetItem.Value>using System;
using System.Collections.Generic;
using System.Text;
using Microsoft.Practices.Mobile.ObjectBuilder;
using Microsoft.Practices.Mobile.CompositeUI;
using System.Windows.Forms;
using Microsoft.Practices.Mobile.CompositeUI.Common;
using %Model.Code%ShellApplication;

namespace %Model.Code%ShellApplication
{
    public class LauncherPresenter
    {
        private ILauncherView view;
        private LaunchItemsCatalog launcherItems;
        private WorkItem workItem;

        public LauncherPresenter()
        {

        }

        public ILauncherView View
        {
            get { return view; }
            set { view = value; AttachView(view); }
        }

        private void AttachView(ILauncherView launcherView)
        {
            this.view = launcherView;
            launcherView.LaunchItemClicked += new EventHandler&lt;LaunchItemSelectedEventArgs&gt;(launcherView_LaunchItemClicked);
        }

        \[ServiceDependency\]
        public WorkItem WorkItem
        {
            get { return workItem; }
            set { workItem = value; }
        }

        \[ServiceDependency\]
        public LaunchItemsCatalog LauncherItems
        {
            get { return this.launcherItems; }
            set { this.launcherItems = value; }
        }

        void launcherView_LaunchItemClicked(object sender, LaunchItemSelectedEventArgs e)
        {

            using (WaitCursor wc = new WaitCursor(true))
            {
                .//WorkItem child = workItem.WorkItems.AddNew(e.LaunchItem.WorkItemType);
                foreach(LaunchItem item in launcherItems)
                    if (item.Text == e.WorkItemName)
                    {
                        WorkItem child = workItem.WorkItems.AddNew(item.WorkItemType);
                        child.Run();
                        break;
                    }               
            }
        }

        public void RefreshLauncherItems()
        {
            view.ClearLaunchItems();

            foreach (LaunchItem item in launcherItems)
            {
                view.AddLaunchItem(item);
            }
        }

    }
}
