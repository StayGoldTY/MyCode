        /// &lt;param name=&quot;maxLen&quot;&gt;Define the validation column length.
        /// If the length is over the max length of field, it&#39;ll return false. 
        /// It&#39;ll add invalid field into errorValideColumnList collection. You can retrieve them by ErrorMessage property in your concrete BLL class.
        /// &lt;/param&gt;
        /// &lt;returns&gt;true if field is valid&lt;/returns&gt;
        protected bool IsValidField(T obj, string fieldName, short maxLen)
        {
            short i = (short)(GetColumnField(obj, fieldName).GetValue(obj).ToString().Trim().Length);
            if ((i &lt; 1) || (i &gt; maxLen))
            {
                //Add error field into list.
                errorValideColumnList.Add(fieldName);
                return false;
