            base.RootWorkItem.Services.AddNew&lt;MobileCabAuthenticationService, IAuthenticationService&gt;();

            MobilePasswordAuthenticationService mobilePasswordAuthenticationService =
               base.RootWorkItem.Services.AddNew&lt;MobilePasswordAuthenticationService, IMobilePasswordAuthenticationService&gt;();

            if (mobilePasswordAuthenticationService != null)
            {
                //The following token data was taken from an AuthenticationToken generated by creating a 
                //PasswordIdentity based on the &quot;user&quot;/&quot;pwd&quot; and passing the PinIdentity to the AuthenticationToken
                //constructor.
                //It is impossible to decrypt the token data to get the username &amp; password
                String userToken = FileUtility.ReadFile(&quot;User.Token&quot;);
                mobilePasswordAuthenticationService.SetTokenData(userToken);
            }
        }
    }
}
