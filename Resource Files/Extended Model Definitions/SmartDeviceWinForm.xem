              %Code%ListView cusListView = sender as %Code%ListView;
              cusListView.TopMost = false;
              %Code%CreateDlg dlg = new %Code%CreateDlg(this.%classInstance%ListController.WorkItem.RootWorkItem.State);
              dlg.ShowDialog();
              if (dlg.IsCancel == true)
                return;
              this.%classInstance%ListController.Invoke_Create();
