            .//myGridStyle.GridColumnStyles.Add(colStyle2);
            .// Add new table styles
            .//
            .//dg.TableStyles.Add(myGridStyle);
        }

        void OnExit(object sender, EventArgs e)
        {
            .//view.Exit -= OnExit;
            view.BindDataSource -= OnBindDataSource;
            view.Create%Code% -= OnCreate%Code%;
            view.Delete%Code% -= OnDelete%Code%;
            view.Init -= OnInit;
            %classInstance%ListController.Close(this);
        }

        public void UpdateCustomerList()
        {

        }

        public void SetController(%Code%Controller %classInstance%ListController)
        {
            this.%classInstance%ListController = %classInstance%ListController;
        }
    }
