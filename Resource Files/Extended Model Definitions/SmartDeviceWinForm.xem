<a:TemplateTargetItem.Value>/// &lt;summary&gt;
/// Find method in %.FU:Code% BLL.
/// &lt;/summary&gt;
/// &lt;param name=&quot;%classInstance%&quot;&gt;%classCode% domain object&lt;/param&gt;
/// &lt;param name=&quot;isThrowException&quot;&gt;If not retrieving anything from database and isThrowException is true, throw execption only when isThrowException is . Otherwise, return null object. &lt;/param&gt;
/// &lt;param name=&quot;isRetrieveAll&quot;&gt;If true, retrieve all, otherwise, retrieve data according to %classInstance% criteria.&lt;/param&gt;
/// &lt;returns&gt;%classCode% collection&lt;/returns&gt;
public %dalContainerType% Find(%classCode% %classInstance%, bool isRetrieveAll)
{
   %idalClassCode% %dalClassInstance% = ConcreteDALFactory.%createDALMethodName%();
   %dalContainerType% %collectionClassInstance% = new %dalContainerType%();
   if (isRetrieveAll)
      %collectionClassInstance% = %dalClassInstance%.%getListName%();
   else
   {
      if (%classInstance% == null)
         throw new ApplicationException(&quot;%classCode% is null object.&quot;);
      else
         %collectionClassInstance% = %dalClassInstance%.QueryByExample(%classInstance%);
  }\n
  .if(%Model.useADODotNetCF%)
[  %eargeLoadingMetadataDefinition%\n]\
  .endif
  return %collectionClassInstance%;
}\n\n
.if(%Model.useADODotNetCF%)
.foreach_item(AssociationsA,,,%RoleBNavigability%)
[%findByNavigationA%\n]\
.next
.foreach_item(AssociationsB,,,%RoleANavigability%)
[%findByNavigationB%\n]\
.next
.endif</a:TemplateTargetItem.Value>
