/// &lt;summary&gt;
/// Find by %ClassA.classCode%.
/// &lt;/summary&gt;
/// &lt;param name=&quot;%ClassA.classInstance%&quot;&gt;&lt;/param&gt;
/// &lt;returns&gt;%.FU:ClassA.classInstance% collection&lt;/returns&gt;
protected %ClassB.dalContainerType% Find%.FU:roleBCode%(%ClassA.classCode% %ClassA.classInstance%)
{
    %ClassB.idalClassCode% %ClassB.dalClassInstance% = ConcreteDALFactory.%ClassB.createDALMethodName%();
    %ClassB.dalContainerType% %ClassB.collectionClassInstance% = new %ClassB.dalContainerType%();\n
    .if(%isNavigableB%)
      .if(%RoleBMultiple%)
    %ClassB.collectionClassInstance% = %ClassB.dalClassInstance%.%findByNavigationMethodA%(%ClassA.classInstance%);\n
      .else
    %ClassB.collectionClassInstance%.Add(%ClassB.dalClassInstance%.%findByNavigationMethodA%(%ClassA.classInstance%));\n
      .endif
    .else
    if (%ClassA.classInstance%.%roleBPropertyCode% != null)
       %ClassB.collectionClassInstance%.Add(%ClassB.dalClassInstance%.Load(%ClassA.classInstance%.%roleBPropertyCode%.%.FU:ClassB.pkClassInstance%));
    else
       %ClassB.collectionClassInstance% = null;\n
    .endif
    return %ClassB.collectionClassInstance%;
}
.//.endif</a:TemplateTargetItem.Value>
