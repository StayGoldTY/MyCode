<a:TemplateTargetItem.Value>using System;
using System.Collections.Generic;
using System.Text;
using Microsoft.Practices.Mobile.CompositeUI.Services;
using Microsoft.Practices.Mobile.CompositeUI;
using Microsoft.Practices.Mobile.PasswordAuthentication;

namespace %Model.Code%ShellApplication
{
	public class MobileCabAuthenticationService : IAuthenticationService
	{
		private WorkItem rootWorkItem;

		public MobileCabAuthenticationService(\[ServiceDependency\] WorkItem rootWorkItem)
		{
			this.rootWorkItem = rootWorkItem;
		}

		public void Authenticate()
		{
			LoginView loginView = rootWorkItem.Items.AddNew&lt;LoginView&gt;();
			LoginPresenter loginPresenter = rootWorkItem.Items.AddNew&lt;LoginPresenter&gt;();
			loginPresenter.View = loginView;
			
			loginView.ShowDialog();

			PasswordIdentity identity = rootWorkItem.Items.Get&lt;PasswordIdentity&gt;(&quot;userIdentity&quot;);
			if (identity == null)
				throw new QuitApplicationException();
						
		}
	}
}
