        /// &lt;/summary&gt;
        /// &lt;param name=&quot;obj&quot;&gt;Current domain object&lt;/param&gt;
        /// &lt;returns&gt;true if field is used for searching criertia&lt;/returns&gt;
        protected List&lt;string&gt; GetCriteriaField(T obj)
        {
            List&lt;string&gt; notUsed = new List&lt;string&gt;();
            PropertyInfo\[\] infos = obj.GetType().GetProperties();
            foreach (PropertyInfo pi in infos)
            {
                if (pi.CanRead)
                {
                    if (pi.GetValue(obj, null) == null)
                        notUsed.Add(pi.Name);
                    else if ((short)(pi.GetValue(obj, null).ToString().Trim().Length) == 0)
                        notUsed.Add(pi.Name);
                }
            }
            return notUsed;
        }
