           }
           catch (Exception ex)
           {
               MessageBox.Show(ex.Message);
           }
        }

        void OnDelete%Code%(object sender, EventArgs e)
        {
           try
           {
               using (WaitCursor wc = new WaitCursor(true))
               {
                  DataGrid dg = sender as DataGrid;
                  int index = dg.CurrentRowIndex;
                  if (index &lt; 0)
                     throw new ArgumentOutOfRangeException();
                  %dalContainerType% list = this.%classInstance%ListController.List%Code%s();
                  this.%classInstance%ListController.WorkItem.RootWorkItem.State\[%Code%StringConstant.DELETE_VALUE\]
                  = list\[index\];
                  this.%classInstance%ListController.Invoke_Delete();
                 ((%Code%ListView)dg.Parent).RefreshDataSource();
               }
           }
           catch (Exception ex)
           {
               MessageBox.Show(ex.Message);
           } 
        }

