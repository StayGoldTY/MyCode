            view.Create%Code% += OnCreate%Code%;
            view.Delete%Code% += OnDelete%Code%;
            view.ShowDetailView += OnShowDetailView;
            view.Init += OnInit;
            view.Find%Code% += OnFind%Code%;
        }

        void OnFind%Code%(object sender, EventArgs e)
        {
          try
          {
            %Code%ListView cusListView = sender as %Code%ListView;
            cusListView.TopMost = false;
            %Code%FindDlg dlg = new %Code%FindDlg(this.%classInstance%ListController.WorkItem.RootWorkItem.State);
            dlg.ShowDialog();
            if (dlg.IsCancel == true)
                return;
            cusListView.RefreshDataSource(this.%classInstance%ListController.Invoke_Find());
          }
          catch (Exception ex)
          {
             MessageBox.Show(ex.Message);
          }
        }

        void OnInit(object sender, EventArgs e)
        {
            ((%Code%ListView)this.View).RefreshDataSource();
            this.%classInstance%ListController.Synchronize += new EventHandler(%classInstance%ListController_Synchronize);
        }

        void OnShowDetailView(object sender, EventArgs e)
        {
          try
          {
            DataGrid dg = sender as DataGrid;
            ((%Code%ListView)dg.Parent).TopMost = false;

            using (WaitCursor wc = new WaitCursor(true))
            {
                int index = dg.CurrentRowIndex;
                %dalContainerType% lists = this.%classInstance%ListController.List%Code%s();
                this.%classInstance%ListController.WorkItem.RootWorkItem.State\[%Code%StringConstant.CURRENT_VALUE\] =
                    lists\[index\];
                this.%classInstance%ListController.AttachDetailView();
            }
          }
          catch (Exception ex)
          {
              MessageBox.Show(ex.Message);
          }
        }

        void OnCreate%Code%(object sender, EventArgs e)
