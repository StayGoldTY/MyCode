<a:Name>extendedAttributes</a:Name>
<a:TemplateTargetItem.Value>.set_value(mapPrefix, %getXmlSchemaMapping%, new)
.set_value(_sqlRelation, %sqlRelation%)
.set_value(_sqlField, %sqlField%)
.if (%_sqlRelation%)
[%mapPrefix%relation=&quot;%_sqlRelation%&quot;]
.elsif (%_sqlField%)
[%mapPrefix%field=&quot;%_sqlField%&quot;]
.elsif (%isConstant% != &quot;&quot;)
 %mapPrefix%is-constant=&quot;%isConstant%&quot;
.endif
.if (%use-cdata%!=&quot;&quot;)
 %mapPrefix%use-cdata=&quot;1&quot;
.endif
.if (%overflow-field%)
 %mapPrefix%overflow-field=&quot;%overflow-field%&quot;
.endif
.if (%key-fields%)
 %mapPrefix%key-fields=&quot;%key-fields%&quot;
.endif
.if (%map-field%!=&quot;&quot;)
 %mapPrefix%map-field=&quot;%map-field%&quot;
.endif
.if (%limit-field%)
 %mapPrefix%limit-field=&quot;%limit-field%&quot;
.endif
.if (%limit-value%)
 %mapPrefix%limit-value=&quot;%limit-value%&quot;
.endif
</a:TemplateTargetItem.Value>
