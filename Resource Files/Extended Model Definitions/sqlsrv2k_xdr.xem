<a:Name>getXmlSchemaMapping</a:Name>
<a:TemplateTargetItem.Value>.vbscript()
Dim sPrefix
bFound = ActiveModel.HasNamespace(&quot;urn:schemas-microsoft-com:xml-sql&quot;)
if (not bFound) then
   ScriptResult  = &quot;sql:&quot;
else
sPrefix = ActiveModel.GetNamespacePrefix(&quot;urn:schemas-microsoft-com:xml-sql&quot;)
if (sPrefix &lt;&gt; &quot;&quot;) then
   ScriptResult = sPrefix + &quot;:&quot;
end if
end if
.endvbscript
