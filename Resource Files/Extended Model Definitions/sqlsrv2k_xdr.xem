<a:Name>elementContent</a:Name>
<a:TemplateTargetItem.Value>.if (%DefaultMapping%)
.set_value(mapPrefix, %getXmlSchemaMapping%, new)
   .foreach_item(DefaultMapping.SourceLinks)
      .if (%ObjectType% == Reference)   
.set_value(parent_table, %ParentTable.GeneratedName%)   
&lt;%mapPrefix%relationship key-relation=&quot;%validGenName(parent_table)%&quot;
.foreach_item(Joins, &quot; key=\&quot;&quot;, &quot;\&quot;&quot;)
%ParentTableColumn.Code%
.next(&quot; &quot;)
.foreach_item(Joins, &quot; foreign-key=\&quot;&quot;, &quot;\&quot;&quot;)
%ChildTableColumn.Code%
.next(&quot; &quot;)
.set_value(child_table, %ChildTable.GeneratedName%)
 foreign-relation=&quot;%validGenName(child_table)%&quot;/&gt;   
      .elsif (%ObjectType% == ViewReference)
.set_value(parent_table, %TableView1.GeneratedName%)   
&lt;%mapPrefix%relationship key-relation=&quot;%validGenName(parent_table)%&quot;
.foreach_item(Joins, &quot; key=\&quot;&quot;, &quot;\&quot;&quot;)
%Column1.Code%
.next(&quot; &quot;)
.foreach_item(Joins, &quot; foreign-key=\&quot;&quot;, &quot;\&quot;&quot;)
%Column2.Code%
.next(&quot; &quot;)
.set_value(child_table, %TableView2.GeneratedName%)
 foreign-relation=&quot;%validGenName(child_table)%&quot;/&gt;   
      .endif
   .next(\n)
.endif</a:TemplateTargetItem.Value>
