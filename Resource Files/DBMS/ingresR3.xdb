<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, SYNONYM, BASEOWNER, BASEOBJECT}

select
   s.synonym_owner,
   s.synonym_name,
   s.table_owner,
   s.table_name
from
   iisynonyms s
where 1 = 1
[  and s.synonym_owner = %.q:SCHEMA%]
order by 1, 2</a:TextTargetItem.Value>
