<a:Name>SqlAttrQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, VIEW, COMMENT, VIEWCOLN, SCRIPT} 

select
   u.user_name,
   t.table_name,
   (select r.remarks from sys.sysremark r where r.object_id = t.object_id),
   (select list(c.column_name, &#39;, &#39; order by c.column_id) from sys.syscolumn c where c.table_id = t.table_id),
   v.view_def
from
   sys.systab t
   join sys.sysuserperms u on (u.user_id = t.creator)
   join sys.sysview v on (v.view_object_id = t.object_id)
where
   t.table_type in (21)
[  and u.user_name=%.q:OWNER%]
[  and t.table_name=%.q:VIEW%]
