<a:TextTargetItem.Value>.set_value(OldConstName, %OLD.CheckConstraintName%)
.if(%OldConstName% == &quot;&quot;)
.set_value(OldConstName, %OLD.ConstName%)
.endif
.set_value(NewConstName, %NEW.CheckConstraintName%)
.if(%NewConstName% == &quot;&quot;)
