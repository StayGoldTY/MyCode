   modify %COLUMN% [constraint %CONSTNAME% ]check (%.A:CONSTRAINT%)
.else
alter table [%QUALIFIER%]%TABLE%
   alter %COLUMN% drop constraint %OldConstName%;

%AddColnChck%

.endif()</a:TextTargetItem.Value>
