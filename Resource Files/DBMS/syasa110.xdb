   u.user_name,
   t.table_name,
   &#39;materialized view&#39;
from
   sys.systab t
   join sys.sysuserperms u on (u.user_id = t.creator)
where
   t.table_type in (2)
[  and u.user_name=%.q:SCHEMA%]
order by 1, 2</a:TextTargetItem.Value>
