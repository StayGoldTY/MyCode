<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, TABLE, ISPKEY, CONSTNAME, COMMENT, COLUMNS, CLUSTER}

select
   u.user_name,
   t.table_name,
   case(k.constraint_type) when &#39;P&#39; then 1 else 0 end,
   k.constraint_name,
   (select r.remarks from sys.sysremark r where r.object_id = i.object_id),
   (select list(c.column_name, &#39;, &#39;)
    from sys.sysidxcol x join sys.syscolumn c on (c.table_id = x.table_id and c.column_id = x.column_id)
    where x.table_id = i.table_id and x.index_id = i.index_id) as coln,
   case(i.index_id) when t.clustered_index_id then &#39;TRUE&#39; else &#39;&#39; end
from
   sys.sysconstraint k
   join sys.systab t on (t.object_id = k.table_object_id)
   join sys.sysuserperms u on (u.user_id = t.creator)
   join sys.sysidx i on (i.object_id = k.ref_object_id)
where
   k.constraint_type in (&#39;P&#39;, &#39;U&#39;)
[  and t.table_name=%.q:TABLE%]
[  and u.user_name=%.q:OWNER%]
order by 1, 2, 3 desc, 4
</a:TextTargetItem.Value>
