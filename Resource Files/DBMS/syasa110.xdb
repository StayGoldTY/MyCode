<o:TextTargetItem Id="o202">
<a:ObjectID>A360443A-E51B-40E7-AF89-6947743D6B83</a:ObjectID>
<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{TABLOWNER, TABLE, TRIGGER, TRGEVENT, TRGTIME}

select 
   u.user_name, 
   t.table_name, 
   r.trigger_name, 
   case(r.event)
      when &#39;A&#39; then &#39;insert, delete&#39;
      when &#39;B&#39; then &#39;insert, update&#39;
      when &#39;C&#39; then &#39;update&#39;
      when &#39;D&#39; then &#39;delete&#39;
      when &#39;E&#39; then &#39;delete, update&#39;
      when &#39;I&#39; then &#39;insert&#39;
      when &#39;M&#39; then &#39;insert, delete, update&#39;
      when &#39;U&#39; then &#39;update&#39;
      else &#39;&#39;
   end as colnA,
   case(r.trigger_time)
      when &#39;A&#39; then &#39;after&#39;
      when &#39;B&#39; then &#39;before&#39;
      when &#39;R&#39; then &#39;resolve&#39;
      else &#39;&#39;
   end as colnB
from 
   sys.systrigger r
   join sys.systab t on (t.table_id = r.table_id)
   join sys.sysuserperms u on (u.user_id = t.creator)
where 
   r.trigger_name is not null
[  and u.user_name=%.q:SCHEMA%]
[  and t.table_name=%.q:TABLE%] 
order by 
   1,2,r.trigger_id
</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>1286805236</a:ModificationDate>
<a:Modifier>mledier</a:Modifier>
<a:Comment>SQL query to list objects</a:Comment>
</o:TextTargetItem>
<o:TextTargetItem Id="o203">
<a:ObjectID>44449CE8-ED7D-4A53-84DD-5AB6313B25A7</a:ObjectID>
<a:Name>SqlAttrQuery</a:Name>
<a:TextTargetItem.Value>{TABLOWNER, TABLE, TRIGGER, COMMENT, SCRIPT}

select
   u.user_name,
   t.table_name,
   d.trigger_name,
   (select r.remarks from sys.sysremark r where r.object_id = d.object_id),
   isnull(d.source, d.trigger_defn)
from
   sys.sysuserperms u
      join sys.systable t on (t.creator=u.user_id)
      join sys.systrigger d on (d.table_id = t.table_id)
where
   d.trigger_name is not null
[  and u.user_name=%.q:TABLOWNER%]
[  and t.table_name=%.q:TABLE%]
[  and d.trigger_name=%.q:TRIGGER%]
</a:TextTargetItem.Value>
