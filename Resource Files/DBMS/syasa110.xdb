<a:CheckGlobalScript>&#39;******************************************************************************
&#39;* Purpose:  This VB-Script holds global definitions shared by all the custom-
&#39;*            checks scripts of the model extension.
&#39;******************************************************************************

Option Explicit &#39; This is to ensure all used variables are defined

Sub ParseFullTextEvent(pEvent, sEventText)
   If (sEventText = &quot;&quot;) then
      Exit Sub
   End If
   Dim sEventTextClean : sEventTextClean = Trim(Replace(Replace(Replace(sEventText, vbCr, &quot; &quot;), vbLf, &quot; &quot;), vbTab, &quot; &quot;))
   Dim s : s = &quot;&quot;
   Dim pos : pos = InStrRev(LCase(sEventTextClean), &quot; handler &quot;)
   If pos &gt; 0 Then
      s = Mid(sEventText, pos + Len(&quot; handler&quot;)) &#39;sEventText still contains crlf &amp; indent as user entered them
      pEvent.SetExtendedAttribute &quot;Handler&quot;, Trim(s)
      sEventTextClean = Trim(Left(sEventTextClean, pos - 1))
   end if
   If (LCase(WordAt(sEventTextClean, -2)) = &quot;at&quot;) Then 
      s = WordAt(sEventTextClean, -1)
      pEvent.SetExtendedAttribute &quot;Database&quot;, s
      sEventTextClean = Trim(Left(sEventTextClean, Len(sEventTextClean) - Len(s)))
      sEventTextClean = Trim(Left(sEventTextClean, Len(sEventTextClean) - Len(&quot;at&quot;)))
   End If
   If (LCase(WordAt(sEventTextClean, -1)) = &quot;enable&quot;) Then 
      pEvent.SetExtendedAttribute &quot;Enable&quot;, True
      sEventTextClean = Trim(Left(sEventTextClean, Len(sEventTextClean) - Len(&quot;enable&quot;)))
   End If
   If (LCase(WordAt(sEventTextClean, -1)) = &quot;disable&quot;) Then 
      pEvent.SetExtendedAttribute &quot;Enable&quot;, False
      sEventTextClean = Trim(Left(sEventTextClean, Len(sEventTextClean) - Len(&quot;disable&quot;)))
   End If
   If (LCase(WordAt(sEventTextClean, 1)) = &quot;type&quot;) Then
      pEvent.SetExtendedAttribute &quot;ScheduledEvent&quot;, False
      s = WordAt(sEventTextClean, 2)
      pEvent.SetExtendedAttribute &quot;EventType&quot;, s
      sEventTextClean = Trim(Mid(sEventTextClean, Len(&quot;type&quot;) + 1))
      sEventTextClean = Trim(Mid(sEventTextClean, Len(s) + 1))
      If (LCase(WordAt(sEventTextClean, 1)) = &quot;where&quot;) Then
         pEvent.SetExtendedAttribute &quot;TriggerCondition&quot;, Trim(Mid(sEventTextClean, Len(&quot;where&quot;) + 1))
      End If
   Else
      pEvent.SetExtendedAttribute &quot;ScheduledEvent&quot;, True
      pEvent.SetExtendedAttribute &quot;SchedulesText&quot;, Left(sEventText, Len(sEventTextClean))
   End If

End Sub

Function WordAt(sText, n)
   Dim s
   s = trim(replace(replace(replace(sText, vbCrLf, &quot; &quot;), vbTab, &quot; &quot;), &quot;  &quot;, &quot; &quot;))
   Dim pos
   If (n = 0) Then
      WordAt = &quot;&quot;
   ElseIf (n &gt; 1) Then
      pos = InStr(s, &quot; &quot;)
      If (pos = 0) Then
         WordAt = &quot;&quot;
      Else
         WordAt = WordAt(Mid(s, pos + 1), n - 1)
      End If
   ElseIf (n = 1) Then
      pos = InStr(s, &quot; &quot;)
      If (pos = 0) Then
         WordAt = s
      Else
         WordAt = Left(s, pos - 1)
      End If
   ElseIf (n = -1) Then
      pos = InStrRev(s, &quot; &quot;)
      If (pos = 0) Then
         WordAt = s
      Else
         WordAt = Mid(s, pos + 1)
      End If
   ElseIf (n &lt; -1) Then
      pos = InStrRev(s, &quot; &quot;)
      If (pos = 0) Then
         WordAt = &quot;&quot;
      Else
         WordAt = WordAt(Left(s, pos - 1), n + 1)
      End If
   End If      
End Function

Function GetParamValue(sDef, sParam)
   dim pos : pos = InStr(lCase(sDef), lCase(sParam))
   if (pos = 0) then 
      GetParamValue = &quot;&quot;
   else
      dim s : s = Trim(Mid(sDef, pos + Len(sParam)))
      pos = 0
      if (Left(s, 1) = &quot;&#39;&quot;) then
         s = mid(s, 2)
         pos = InStr(s, &quot;&#39;&quot;)
      elseif(Left(s, 1) = &quot;(&quot;) then
         s = mid(s, 2)
         pos = InStr(s, &quot;)&quot;)
      else
         pos = InStr(s, &quot; &quot;)
         if (pos = 0) then pos = Len(s) + 1
      end if
      if (pos = 0) then
         GetParamValue = &quot;&quot;
      else
         GetParamValue = left(s, pos - 1)
      end if
   end if
end function
   
Function GetScheduleEvent(pSchedule)
   dim pExtColl
   for each pExtColl in pSchedule.ExtendedInverseCollections
      if (pExtColl.CollectionName = &quot;Schedules&quot; and pExtColl.InverseCollectionName = &quot;Event&quot;) then
         Set GetScheduleEvent = pExtColl.Parent
         Exit Function
      end if
   next
   Set GetScheduleEvent = Nothing
End Function
