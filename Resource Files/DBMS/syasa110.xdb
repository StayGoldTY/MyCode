<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{COWNER, CHILD, FKCONSTRAINT, COMMENT, POWNER, PARENT, CHCKONCMMT, CLUSTER, UPDCONST, DELCONST}

select
   cu.user_name,
   ct.table_name,
   fk.role,
   (select r.remarks from sys.sysremark r where r.object_id = fk.object_id),
   pu.user_name,
   pt.table_name,
   case(fk.check_on_commit) when &#39;Y&#39; then &#39;yes&#39; else &#39;&#39; end,
   (select &#39;TRUE&#39; from sys.sysidx ix where ix.object_id = fk.object_id and ix.index_id = ct.clustered_index_id),
   (select case (fd.referential_action) when &#39;C&#39; then &#39;cascade&#39; when &#39;D&#39; then &#39;set default&#39; when &#39;N&#39; then &#39;set null&#39; else &#39;restrict&#39; end
    from sys.systrigger fd where fd.foreign_table_id=fk.foreign_table_id and fd.foreign_key_id=fk.foreign_key_id and fd.event = &#39;C&#39;   
   ) as colnA,
   (select case (fu.referential_action) when &#39;C&#39; then &#39;cascade&#39; when &#39;D&#39; then &#39;set default&#39; when &#39;N&#39; then &#39;set null&#39; else &#39;restrict&#39; end
    from sys.systrigger fu where fu.foreign_table_id=fk.foreign_table_id and fu.foreign_key_id=fk.foreign_key_id and fu.event = &#39;D&#39;   
   ) as colnB
from
   sys.sysforeignkey fk
   join sys.systab ct on (ct.table_id = fk.foreign_table_id)
   join sys.sysuserperms cu on (cu.user_id = ct.creator)
   join sys.systab pt on (pt.table_id=fk.primary_table_id)
   join sys.sysuserperms pu on (pu.user_id=pt.creator)
where 1=1
[  and cu.user_name=%.q:SCHEMA%]
[  and ct.table_name=%.q:TABLE%]</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>1286805236</a:ModificationDate>
<a:Modifier>mledier</a:Modifier>
<a:Comment>SQL query to list objects</a:Comment>
</o:TextTargetItem>
