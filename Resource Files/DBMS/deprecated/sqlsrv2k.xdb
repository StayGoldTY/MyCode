<o:TextTargetItem Id="o239">
<a:ObjectID>867B9F8B-34AB-4727-A5A5-89D118CAB979</a:ObjectID>
<a:Name>SqlAttrQuery</a:Name>
<a:TextTargetItem.Value>{OWNER ID, VIEW ID, VIEWSTYLE ID, SCRIPT ...}

select
   u.name,
   v.name,
   case
      when (v.status &amp; 0x08000 = 0x08000) then &#39;View_Metadata&#39;
      when (v.status &amp; 0x04000000 = 0x04000000) then &#39;SchemaBinding&#39;
      when (lower(substring(ltrim(t.text), 1, 1)) &lt;&gt; &#39;c&#39;) then &#39;Encryption&#39;
      else &#39;View&#39;
   end as coln,
   t.text
from
   sysusers u,
   sysobjects v,
   syscomments t
where
   t.id=v.id
   and u.uid=v.uid
   and v.type=&#39;V&#39;
   [and u.name=%.q:OWNER%]
   [and v.name=%.q:VIEW%]
order by
   t.id,
   t.number,
   t.colid
</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:Comment>SQL query to reverse object attributes</a:Comment>
</o:TextTargetItem>
<o:TextTargetItem Id="o240">
<a:ObjectID>3DE6387F-5BEF-484C-AD3B-0BA18B39EABE</a:ObjectID>
<a:Name>SqlPermQuery</a:Name>
<a:TextTargetItem.Value>{GRANTEE ID, PERMISSION ...}

select u1.name grantee,
case
    when (s.action = 193) then &#39;SELECT&#39;
    when (s.action = 195) then &#39;INSERT&#39;
    when (s.action = 196) then &#39;DELETE&#39;
    when (s.action = 197) then &#39;UPDATE&#39;
end +
case
    when (s.protecttype = 204) then &#39;+&#39;
    when (s.protecttype = 205) then &#39;&#39;
    when (s.protecttype = 206) then &#39;-&#39;
end
+ &#39;,&#39; as coln
from sysprotects s, sysusers u, sysusers u1, sysobjects o
where
  o.name = %.q:VIEW% and
  [u.name = %.q:OWNER% and]
  o.uid = u.uid and
  s.id = o.id and
  u1.uid = s.uid
order by grantee</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:Comment>SQL query to reverse object permissions</a:Comment>
</o:TextTargetItem>
