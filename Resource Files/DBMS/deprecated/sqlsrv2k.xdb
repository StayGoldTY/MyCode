<o:TextTargetItem Id="o251">
<a:ObjectID>C174336A-1C5C-4B3A-800F-66CFFEC9268D</a:ObjectID>
<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{TABLOWNER, TABLE, OWNER, TRIGGER, TRGEVENT}

select
   ub.name,
   tb.name,
   ur.name,
   tr.name,
   case (tr.status &amp; 0x400) when 0x400 then &#39;insert,&#39; else &#39;&#39; end +
   case (tr.status &amp; 0x200) when 0x200 then &#39;update,&#39; else &#39;&#39; end +
   case (tr.status &amp; 0x100) when 0x100 then &#39;delete,&#39; else &#39;&#39; end as coln
from
   sysusers ub,   sysobjects tb,   sysobjects tr, sysusers ur
where
   ur.uid = tr.uid 
   and ub.uid = tb.uid
   and tr.type = &#39;TR&#39;
   and tr.parent_obj = tb.id
[  and ub.name =%.q:SCHEMA%]
[  and tb.name = %.q:TABLE%]
order by 1, 2, 3, 4</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:Comment>SQL query to list objects</a:Comment>
</o:TextTargetItem>
<o:TextTargetItem Id="o252">
<a:ObjectID>42736C4B-00A1-4F37-A687-0FCFCD1794D5</a:ObjectID>
<a:Name>SqlAttrQuery</a:Name>
<a:TextTargetItem.Value>{TABLOWNER ID, TABLE ID, OWNER ID, TRIGGER ID, SCRIPT ...}

select
   ub.name,
   tb.name,
   ur.name,
   tr.name,
   cm.text
from
   sysusers ub,   sysobjects tb,   sysobjects tr, sysusers ur, syscomments cm
where
   ur.uid = tr.uid 
   and ub.uid = tb.uid
   and tr.type = &#39;TR&#39;
   and tr.parent_obj = tb.id
   and cm.id = tr.id
[  and ub.name =%.q:TABLOWNER%]
[  and tb.name = %.q:TABLE%]
[  and ur.name =%.q:OWNER%]
[  and tr.name =%.q:TRIGGER%]
order by
   1,2,3,4,
   cm.number,
   cm.colid
</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:Comment>SQL query to reverse object attributes</a:Comment>
</o:TextTargetItem>
