<a:ObjectID>C7A69470-9430-4425-B288-89A06E0CD9D0</a:ObjectID>
<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{COWNER, CHILD, FKCONSTRAINT, ExtFkNotForReplication, UPDCONST, DELCONST, POWNER, PARENT}

select
   u.name,
   c.name,
   o.name,
   case (o.status &amp; 0XC00 ) when 0XC00  then &#39;true&#39; else &#39;false&#39; end,
   case (o.status &amp; 0x2000) when 0x2000 then &#39;cascade&#39; else &#39;restrict&#39; end,
   case (o.status &amp; 0x1000) when 0x1000 then &#39;cascade&#39; else &#39;restrict&#39; end,
   v.name,
   p.name
from
   dbo.sysreferences r
   join dbo.sysobjects o on (o.id = r.constid)
   join dbo.sysobjects c on (c.id = r.fkeyid)
   join dbo.sysusers u on (u.uid = c.uid)
   join dbo.sysobjects p on (p.id = r.rkeyid)
   join dbo.sysusers v on (v.uid = p.uid)
where
   o.type = &#39;F&#39;
[  and u.name = %.q:SCHEMA%]
[  and c.name = %.q:TABLE%]
</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:Comment>SQL query to list objects</a:Comment>
</o:TextTargetItem>
<o:TextTargetItem Id="o148">
<a:ObjectID>C4367658-1D90-4E79-A7B4-A62BBE9EEF6B</a:ObjectID>
<a:Name>SqlListChildrenQuery</a:Name>
<a:TextTargetItem.Value>{COWNER, CHILD, FKCONSTRAINT, FKEYCOLUMN, CKEYCOLUMN}

select
   u.name,
   t.name,
   o.name,
   c.name,
   p.name
from
   dbo.sysforeignkeys j
      join dbo.sysobjects o on(o.id = j.constid)
      join dbo.syscolumns p on (p.id = j.rkeyid and p.colid = j.rkey)
      join dbo.syscolumns c on (c.id = j.fkeyid and c.colid = j.fkey)
      join dbo.sysobjects t on (t.id = j.fkeyid)
      join dbo.sysusers u on (u.uid = t.uid)
where
   o.type = &#39;F&#39;
[   and o.name = %.q:FKCONSTRAINT%]
[  and u.name = %.q:COWNER%]
[  and t.name = %.q:TABLE%]
order by 1, 2, 3, j.keyno</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:Comment>SQL query to list reference joins</a:Comment>
