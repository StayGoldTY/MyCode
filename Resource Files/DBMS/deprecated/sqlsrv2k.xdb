<a:Name>SqlColnListQuery</a:Name>
<a:TextTargetItem.Value>{OWNER ID, VIEW ID, VIEWCOLN ...}

select
   u.name,
   o.name,
   c.name + &#39;,&#39;,
   c.colid
from
   dbo.sysusers u
      join dbo.sysobjects o on (o.uid = u.uid and o.type=&#39;V&#39;)
      join dbo.syscolumns c on (c.id = o.id)
where
  1=1
  [and u.name = %.q:OWNER%]
  [and o.name=%.q:VIEW%]
order by 1, 2, 4
