<o:TextTargetItem Id="o64">
<a:ObjectID>D4A70E8C-2121-43E5-8BB0-CAE78D57F962</a:ObjectID>
<a:Name>SqlOptsQuery</a:Name>
<a:TextTargetItem.Value>{OWNER ID, TABLE ID, OPTIONS ...}

select 
   user_name(t.uid),
   t.name,
   case(i.indid) when 0 then &#39;on &#39; + f.groupname when 1 then &#39;on &#39; + f.groupname else &#39;&#39; end
   + case(i.indid) when 255 then &#39; textimage_on &#39; + f.groupname else &#39;&#39; end as coln
from 
   sysobjects t
   join sysindexes i on (i.id = t.id and i.indid in (0, 1, 255))
   join sysfilegroups f on (f.groupid = i.groupid)
where 
   t.type = &#39;U&#39;
[  and t.name = %.q:TABLE%]
[  and user_name(t.uid) = %.q:OWNER%]
order by 1, 2, i.indid</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:Comment>SQL query to reverse object physical options</a:Comment>
</o:TextTargetItem>
<o:TextTargetItem Id="o65">
<a:ObjectID>6BAAA5D9-A223-44CC-AA5A-3C68F605AE5D</a:ObjectID>
<a:Name>SqlChckQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, TABLE, CONSTNAME, ExtCktNotForReplication, CONSTRAINT}

select
   u.name,
   t.name,
   o.name,
   case (k.status &amp; 0x00200000) when 0x00200000 then &#39;true&#39; else &#39;false&#39; end,
   m.text
from
   dbo.sysobjects t,
   dbo.sysusers u,
   dbo.sysconstraints k,
   dbo.syscomments m,
   dbo.sysobjects o
where
   o.xtype = &#39;C&#39;
   and u.uid = t.uid
   and k.id = t.id
   and o.id = k.constid
   and k.constid = m.id
   and k.colid=0
[  and t.name = %.q:TABLE%]
[  and u.name = %.q:OWNER%]
order by u.name, t.name, o.name</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:Comment>SQL query to reverse object check constraints</a:Comment>
</o:TextTargetItem>
<o:TextTargetItem Id="o66">
<a:ObjectID>CE4124A4-9177-43F4-A11C-5ECCCC169F39</a:ObjectID>
<a:Name>SqlPermQuery</a:Name>
<a:TextTargetItem.Value>{GRANTEE ID, PERMISSION ...}

select u1.name grantee,
case
    when (s.action = 193) then &#39;SELECT&#39;
    when (s.action = 195) then &#39;INSERT&#39;
    when (s.action = 196) then &#39;DELETE&#39;
    when (s.action = 197) then &#39;UPDATE&#39;
    when (s.action = 26)   then &#39;REFERENCES&#39;
end +
case
    when (s.protecttype = 204) then &#39;+&#39;
    when (s.protecttype = 205) then &#39;&#39;
    when (s.protecttype = 206) then &#39;-&#39;
end
+ &#39;,&#39; as coln
from sysprotects s, sysusers u, sysusers u1, sysobjects o
where
  (s.columns IS NULL or s.columns = 1) and
  o.name = %.q:TABLE% and
  [u.name = %.q:OWNER% and]
  o.uid = u.uid and
  s.id = o.id and
  u1.uid = s.uid
order by grantee</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:Comment>SQL query to reverse object permissions</a:Comment>
</o:TextTargetItem>
