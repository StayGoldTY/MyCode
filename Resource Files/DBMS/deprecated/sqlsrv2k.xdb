<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, TABLE, COMMENT}

select
   u.name,
   o.name,
   convert(varchar(8000), p.value)
from
   dbo.sysusers u
      join dbo.sysobjects o on (o.uid = u.uid and o.type in (&#39;U&#39;, &#39;S&#39;))
      left outer join dbo.sysproperties p on (p.id = o.id and p.smallid = 0)
where o.xtype != &#39;S&#39;
[  and u.name = %.q:OWNER%]
[  and o.name=%.q:TABLE%]
order by 1, 2</a:TextTargetItem.Value>
