<a:Name>SqlAttrQuery</a:Name>
<a:TextTargetItem.Value>{OWNER ID, PROC ID, SCRIPT ...}

select
   u.name,
   p.name,
   t.text
from
   sysusers u,
   sysobjects p,
   syscomments t
where
   u.uid = p.uid
   [and u.name=%.q:OWNER%]
   [and p.name=%.q:PROC%]
   and p.type in (&#39;P&#39;, &#39;IF&#39;, &#39;FN&#39;, &#39;TF&#39;)
   and t.id=p.id
order by
   t.id,
   t.number,
   t.colid
