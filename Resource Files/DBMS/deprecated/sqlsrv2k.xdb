<a:Name>SqlPermQuery</a:Name>
<a:TextTargetItem.Value>{USER ID, PRIVILEGE ...}

select u1.name grantee,
case
    when (s.action = 178) then &#39;CREATE FUNCTION&#39;
    when (s.action = 198) then &#39;CREATE TABLE&#39;
    when (s.action = 203) then &#39;CREATE DATABASE&#39;
    when (s.action = 207) then &#39;CREATE VIEW&#39;
    when (s.action = 222) then &#39;CREATE PROCEDURE&#39;
    when (s.action = 228) then &#39;BACKUP DATABASE&#39;
    when (s.action = 233) then &#39;CREATE DEFAULT&#39;
    when (s.action = 235) then &#39;BACKUP LOG&#39;
    when (s.action = 236) then &#39;CREATE RULE&#39;
end +
case
    when (s.protecttype = 204) then &#39;+&#39;
    when (s.protecttype = 205) then &#39;&#39;
    when (s.protecttype = 206) then &#39;-&#39;
end
+ &#39;,&#39; as coln
from sysprotects s, sysusers u1
where u1.uid = s.uid and u1.issqlrole &lt;&gt; 1
order by 1
</a:TextTargetItem.Value>
