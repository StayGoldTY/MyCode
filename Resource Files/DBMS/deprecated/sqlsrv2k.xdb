<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, PROC, FUNC}

select
   u.name,
   p.name, null
from
   sysusers u,
   sysobjects p
where
   u.uid = p.uid
   [and u.name=%.q:SCHEMA%]
   and p.type = &#39;P&#39;
union select
   u.name,
   null, p.name
from
   sysusers u,
   sysobjects p
where
   u.uid = p.uid
   [and u.name=%.q:SCHEMA%]
   and p.type in (&#39;IF&#39;, &#39;FN&#39;, &#39;TF&#39;)
order by
   1,2,3
</a:TextTargetItem.Value>
