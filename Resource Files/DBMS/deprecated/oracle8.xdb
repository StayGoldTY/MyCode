order by 2
:
select 
  s.grantee, 
  s.privilege || decode (s.grantable,&#39;YES&#39;,&#39;+&#39;,&#39;NO&#39;,&#39;&#39;),
  s.column_name || &#39;,&#39;
from USER_COL_PRIVS s
where s.table_name = %.q:TABLE%
[and s.owner = %.q:OWNER%]
order by 2
