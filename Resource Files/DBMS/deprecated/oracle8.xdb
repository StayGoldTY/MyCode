<a:TextTargetItem.Value>{OWNER, DBPACKAGE}

[%ISODBCUSER% ?
select distinct
   %.q:SCHEMA%,
   decode (type, &#39;PACKAGE&#39;, name, &#39;&#39;)
from
   sys.user_source
where
   type in (&#39;PACKAGE&#39;)
order by
   2
:
