<a:TextTargetItem.Value>[%ISLONGDTTP%?{ AverageLength }
select
[%.U:COLTYPE%=BFILE?null:[%.U:COLTYPE%=LONG?null:[%.U:COLTYPE%=&quot;LONG RAW&quot;?null:[%ISSTRDTTP%?avg(length(%COLUMN%)):[%ISBLOBDTTP%?avg(dbms_lob.getLength(%COLUMN%)):nvl(vsize(%COLUMN%), 0) + 1]]]]] as average_length
from [%QUALIFIER%]%TABLE%
:{ NullValuesRate, DistinctValues, AverageLength }
select
[%.U:COLTYPE%=BFILE?null:[%.U:COLTYPE%=LONG?null:[%.U:COLTYPE%=&quot;LONG RAW&quot;?null:[%ISMAND%?null:(count(*) - count(%COLUMN%)) * 100 / count(*)]]]] as null_values,
[%.U:COLTYPE%=BFILE?null:[%.U:COLTYPE%=LONG?null:[%.U:COLTYPE%=&quot;LONG RAW&quot;?null:count(distinct %COLUMN%)]]]] as distinct_values,
[%.U:COLTYPE%=BFILE?null:[%.U:COLTYPE%=LONG?null:[%.U:COLTYPE%=&quot;LONG RAW&quot;?null:[%ISVARDTTP%?[%ISSTRDTTP%?avg(length(%COLUMN%)):[%ISBLOBDTTP%?avg(dbms_lob.getLength(%COLUMN%)):nvl(vsize(%COLUMN%), 0) + 1]]:null]]]] as average_length
from [%QUALIFIER%]%TABLE%]</a:TextTargetItem.Value>
