<a:Name>SqlPartColumnList</a:Name>
<a:TextTargetItem.Value>{VAR ID, VAL ...}

select
   c.owner||c.name,
   decode (c.column_position, 1, &#39;&#39;, &#39;, &#39;)||c.column_name
from
