<a:TextTargetItem.Value>{GRANTEE ID, COLNPERMISSION ID, COLUMN ...}

[%ISDBAUSER% ?
select 
  s.grantee, 
  s.privilege || decode (s.grantable,&#39;YES&#39;,&#39;+&#39;,&#39;NO&#39;,&#39;&#39;),
  s.column_name || &#39;,&#39;
