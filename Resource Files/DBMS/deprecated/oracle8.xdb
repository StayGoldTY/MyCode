<a:Text>.if (%canDefineSelectProcedure%)
create or replace function [%QUALIFIER%]%PROC% 
    .foreach_item(Table.Columns,&quot; (&quot;,&quot;) \n&quot;, %Primary%)
p_%Code% IN %parameterDataType%
    .next(&quot;,&quot;)
RETURN PDTypes.ref_cursor as
my_cursor PDTypes.ref_cursor;
begin
open my_cursor for
select * from 
    .if(%Owner%)
%Owner.Code%.%Table.GeneratedName%
    .else
%Table.GeneratedName%
    .endif(&quot;\n&quot;)
    .foreach_item(Table.Columns,&quot;where &quot;,&quot;;\n&quot;, %Primary%)
%GeneratedName% = p_%Code%
    .next(&quot; and &quot;)
return my_cursor;
end;
.endif</a:Text>
