<a:TextTargetItem.Value>{GRANTEE ID, PERMISSION ...}

[%ISDBAUSER%?select 
   s.grantee,
   s.privilege
   ||decode (s.grantable,&#39;YES&#39;,&#39;+&#39;,&#39;NO&#39;,&#39;&#39;)|| &#39;,&#39;
from 
   sys.dba_tab_privs s
where
   s.table_name = %.q:SQNC%
[  and s.owner = %.q:OWNER%]
:select 
   s.grantee,
   s.privilege
   ||decode (s.grantable,&#39;YES&#39;,&#39;+&#39;,&#39;NO&#39;,&#39;&#39;)|| &#39;,&#39;
from 
   sys.user_tab_privs s
