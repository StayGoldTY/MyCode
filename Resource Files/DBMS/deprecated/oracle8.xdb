   sys.all_views v,
   sys.all_tab_columns c,
   sys.all_col_comments cc
where
   c.table_name=v.view_name
   and c.owner=v.owner
   and cc.owner=v.owner
   and cc.table_name=c.table_name
   and cc.column_name=c.column_name
   [and v.owner=%.q:OWNER%]
   [and v.view_name=%.q:VIEW%]
union select
   v.owner,
   v.name,
   c.column_id,
   cc.comments
from
  sys.all_snapshots  v,
   sys.all_tab_columns c,
   sys.all_col_comments cc
where
   c.table_name=v.name
   and c.owner=v.owner
   and cc.owner=v.owner
   and cc.table_name=c.table_name
   and cc.column_name=c.column_name
   [and v.owner=%.q:OWNER%]
   [and v.name=%.q:VIEW%]
order by
   1, 2, 3</a:TextTargetItem.Value>
