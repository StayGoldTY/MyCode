<a:Name>UsingBlockTerm</a:Name>
<a:TextTargetItem.Value>create procedure,%TERMINATOR%,  1
{
   begin, end, 3
   {
      select, ;, 4
      {
         case, end, 5
      }
      if, end if, 4
      case, end case, 4
      loop, end loop, 4
      for, end for, 4
      {
         cursor, for, 5
      }
      declare, ;, 4
      {
         cursor, for, 5
      }
   }
}
create function,%TERMINATOR%,  1
{
   begin, end, 3
   {
      select, ;, 4
      {
         case, end, 5
      }
      if, end if, 4
      case, end case, 4
      loop, end loop, 4
      for, end for, 4
      {
         cursor, for, 5
      }
      declare, ;, 4
      {
         cursor, for, 5
      }
   }
}</a:TextTargetItem.Value>
