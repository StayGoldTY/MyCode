<a:GetMethodScript>Function %Get%(obj)
   Dim sDttp, sPrec
   sDttp = obj.GetExtendedAttribute(&quot;CastDataType&quot;)
   sPrec = obj.GetExtendedAttribute(&quot;CastDataTypePrec&quot;)
   if (sPrec &lt;&gt; &quot;&quot;) then
      %Get% =  sDttp &amp; &quot;(&quot; &amp; sPrec &amp; &quot;)&quot;
   else
      %Get% =  sDttp
   end if
End Function</a:GetMethodScript>
<a:SetMethodScript>Sub %Set%(obj, value)
   dim pos, sPrec
   pos = InStr(value, &quot;(&quot;)
   if (pos = 0) then
      obj.SetExtendedAttribute &quot;%CurrentTargetCode%.CastDataType&quot;, value
   else
      obj.SetExtendedAttribute &quot;%CurrentTargetCode%.CastDataType&quot;, Left(value, pos - 1)
      sPrec = Mid(value, pos + 1)
      pos = InStrRev(sPrec, &quot;)&quot;)
      if (pos &gt; 0) then
         obj.SetExtendedAttribute &quot;%CurrentTargetCode%.CastDataTypePrec&quot;, Left(sPrec, pos - 1)
      end if
   end if
End Sub</a:SetMethodScript>
