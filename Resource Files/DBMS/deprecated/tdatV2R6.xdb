<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:Comment>Query to reverse privileges</a:Comment>
</o:TextTargetItem>
<o:TextTargetItem Id="o169">
<a:ObjectID>16A39315-B40C-4EC2-9F5C-C8CF807CADF4</a:ObjectID>
<a:Name>SqlAttrQuery</a:Name>
<a:TextTargetItem.Value>{USER, Comment, DBOwner, TimeZone, Startup, Collation, DefaultDatabase, DefaultJournalTable, DateForm, DefaultCharacterSet, DefaultRole, Profile, PermanentSpace, SpoolSpace, TemporarySpace, Account, Fallback, Journal, AfterJournal}

select 
   d.DatabaseName,
   d.CommentString,
   d.ownername,
   case 
      when (u.timezonehour is null) then &#39;&#39;
      when (u.timezonehour || u.timezoneminute = &#39;00&#39;) then &#39;local&#39;
      else (case(substring(trim(u.timezonehour) from 1 for 1)) when &#39;-&#39; then &#39;-&#39; else &#39;&#39; end
           || &#39;&#39;&#39;&#39; || case(length(trim(abs(u.timezonehour)))) when 1 then &#39;0&#39; else &#39;&#39; end || trim(abs(u.timezonehour))
           || &#39;:&#39; || case(length(trim(u.timezoneminute))) when 1 then &#39;0&#39; else &#39;&#39; end || trim(u.timezoneminute)
           || &#39;&#39;&#39;&#39;)
   end,
   u.StartupString,
   case u.DefaultCollation
      when &#39;A&#39; then &#39;ASCII&#39;
      when &#39;E&#39; then &#39;EBCDIC&#39;
      when &#39;M&#39; then &#39;MULTINATIONAL&#39;
      when &#39;H&#39; then &#39;HOST&#39;
      else &#39;&#39;
   end as colnA,
   u.DefaultDataBase,
   coalesce(trim(j.Journal_DB) || &#39;.&#39; || trim(j.JournalName), trim(j.JournalName), &#39;&#39;),
   case u.DefaultDateForm
      when &#39;I&#39; then &#39;integerdate&#39;
      when &#39;A&#39; then &#39;ansidate&#39;
      else &#39;&#39; end,
   case u.DefaultCharType
      when 1 then &#39;Latin&#39;
      when 2 then &#39;Unicode&#39;
      when 3 then &#39;KanjiSJIS&#39;
      when 4 then &#39;Graphic&#39;
      when 5 then &#39;Kanji1&#39;
      else &#39;&#39;
   end as colnB,
   u.RoleName,
   u.ProfileName,
   d.PermSpace,
   d.SpoolSpace,
   d.TempSpace,
   d.AccountName,
   case d.ProtectionType
       when &#39;F&#39; then &#39;FALLBACK&#39;
       when &#39;N&#39; then &#39;NO FALLBACK&#39;
       else &#39;FALLBACK PROTECTION&#39;
   end as colnC,
   case SUBSTR(d.JournalFlag,1,1)
       when &#39;N&#39; then &#39;NO JOURNAL&#39;
       when &#39;S&#39; then (case SUBSTR(d.JournalFlag,2,1) when &#39;S&#39; then &#39;JOURNAL&#39;  else &#39;BEFORE JOURNAL&#39; end)
       when &#39;D&#39; then &#39;DUAL JOURNAL&#39;
       else &#39;&#39;
   end as colnD,
   case SUBSTR(d.JournalFlag,2,1)
       when &#39;N&#39; then &#39;NO AFTER JOURNAL&#39;
       when &#39;S&#39; then &#39;AFTER JOURNAL&#39;
       when &#39;D&#39; then &#39;DUAL AFTER JOURNAL&#39;
       when &#39;L&#39; then &#39;LOCAL AFTER JOURNAL&#39;
       else &#39;&#39;
   end as colnE
from 
   DBC.databases d
   left outer join DBC.Users u on (u.UserName = d.DatabaseName)
   left outer join DBC.User_Default_Journals j on (j.UserName = d.DatabaseName)
where d.DBKind = &#39;U&#39;
[  and u.OwnerName = %.q:CATALOG%]
[  and d.DatabaseName = %.q:USER%]
</a:TextTargetItem.Value>
