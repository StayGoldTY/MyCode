<a:Name>SqlAttrQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, ADT, ADTPROC, SpecificMethodName, Create TPL}

[%ISDBAUSER%?select
   t.DatabaseName,
   t.TableName,
   trim(substring(m.RequestText from position(&#39;method&#39; in lower(m.RequestText)) + 7 for position(&#39;(&#39; in m.RequestText) - position(&#39;method&#39; in lower(m.RequestText)) - 7)) as MethodName,
   m.TableName,
   m.RequestText
from
   dbc.Tables t
   join dbc.Dependency d on (upper(d.Database1Name) = upper(t.DatabaseName) and upper(d.Object1Name) = upper(t.TableName) and d.RelationshipCode = &#39;DM&#39;)
   join dbc.Tables m on (upper(m.DatabaseName) = upper(d.Database2Name) and upper(m.TableName) = upper(d.Object2Name) and m.RequestText is not null)
where
   t.TableKind = &#39;U&#39;
[  and t.DatabaseName = %.q:OWNER%]
[  and t.TableName = %.q:ADT%]
order by 1, 2]
</a:TextTargetItem.Value>
