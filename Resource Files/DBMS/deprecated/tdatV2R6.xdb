<a:Name>SqlAttrQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, VIEW, Create TPL}

select
   t.DatabaseName,
   t.TableName,
   case(substr(lower(trim(t.RequestText)), 1, length(&#39;replace&#39;))) when &#39;replace&#39; then
        cast(&#39;create &#39; || substring(t.RequestText from position(&#39;replace&#39; in lower(t.RequestText)) + 8) as varchar(32000))
   else cast(t.RequestText as varchar(32000)) 
   end
from
   DBC.Tables t
where 
   TableKind =&#39;V&#39;
[  and t.DatabaseName = %.q:SCHEMA%]
[  and t.TableName = %.q:VIEW%]
</a:TextTargetItem.Value>
