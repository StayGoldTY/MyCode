<a:TextTargetItem.Value>{TABLOWNER, TABLE, OWNER, TRIGGER, SCRIPT}

select
   t.SubjectTableDataBaseName,
   t.TableName,
   t.DataBaseName,
   t.TriggerName,
   t.RequestText
from
   dbc.Triggers t
where 1=1
[  and t.SubjectTableDataBaseName = %.q:SCHEMA%]
[  and t.TableName = %.q:TABLE%]
