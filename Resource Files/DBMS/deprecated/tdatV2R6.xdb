<a:ObjectID>59CC972A-8CB6-4E3D-9D0D-CE6F83BBCB24</a:ObjectID>
<a:Name>SqlPermQuery</a:Name>
<a:TextTargetItem.Value>{USER ID, PRIVILEGE ...}

select 
   r.UserName, 
   (case r.AccessRight
    when &#39;AP&#39; then &#39;ALTER PROCEDURE&#39;
    when &#39;AS&#39; then &#39;ABORT SESSION&#39;
    when &#39;CD&#39; then &#39;CREATE DATABASE&#39;
    when &#39;CG&#39; then &#39;CREATE TRIGGER&#39;
    when &#39;CM&#39; then &#39;CREATE MACRO&#39;
    when &#39;CO&#39; then &#39;CREATE PROFILE&#39;
    when &#39;CP&#39; then &#39;CHECKPOINT&#39;
    when &#39;CR&#39; then &#39;CREATE ROLE&#39;
    when &#39;CT&#39; then &#39;CREATE TABLE&#39;
    when &#39;CU&#39; then &#39;CREATE USER&#39;
    when &#39;CV&#39; then &#39;CREATE VIEW&#39;
    when &#39;D&#39; then &#39;DELETE&#39;
    when &#39;DD&#39; then &#39;DROP DATABASE&#39;
    when &#39;DG&#39; then &#39;DROP TRIGGER&#39;
    when &#39;DM&#39; then &#39;DROP MACRO&#39;
    when &#39;DO&#39; then &#39;DROP PROFILE&#39;
    when &#39;DP&#39; then &#39;DUMP&#39;
    when &#39;DR&#39; then &#39;DROP ROLE&#39;
    when &#39;DT&#39; then &#39;DROP TABLE&#39;
    when &#39;DU&#39; then &#39;DROP USER&#39;
    when &#39;DV&#39; then &#39;DROP VIEW&#39;
    when &#39;E&#39; then &#39;EXECUTE&#39;
    when &#39;I&#39; then &#39;INSERT&#39;
    when &#39;IX&#39; then &#39;INDEX&#39;
    when &#39;MR&#39; then &#39;MONITOR RESOURCE&#39;
    when &#39;MS&#39; then &#39;MONITOR SESSION&#39;
    when &#39;PC&#39; then &#39;CREATE PROCEDURE&#39;
    when &#39;PD&#39; then &#39;DROP PROCEDURE&#39;
    when &#39;PE&#39; then &#39;EXECUTE PROCEDURE&#39;
    when &#39;RO&#39; then &#39;REPLICATION OVERRIDE&#39;
    when &#39;R&#39; then &#39;SELECT&#39;
    when &#39;RF&#39; then &#39;REFERENCE&#39;
    when &#39;RS&#39; then &#39;RESTORE&#39;
    when &#39;SS&#39; then &#39;SET SESSION RATE&#39;
    when &#39;SR&#39; then &#39;SET RESOURCE RATE&#39;
    when &#39;U&#39; then &#39;UPDATE&#39;
   else &#39;&#39;
   end) || 
  (case r.GrantAuthority 
    when &#39;Y&#39; then &#39;+&#39;
    else &#39;&#39; 
   end) || &#39;,&#39; as coln
from
   DBC.AllRights r
where
   r.TableName=&#39;All&#39;
</a:TextTargetItem.Value>
