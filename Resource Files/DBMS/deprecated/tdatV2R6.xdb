<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{OWNER ID, TABLE ID, INDEX ID, NamedIndex ID, UNIQUE ID, AllIndex ID, PrimaryIndex ID, CIDXLIST ...}

select
   i.DatabaseName,
   i.TableName,
   coalesce(i.IndexName, &#39;Index_&#39; || trim(i.IndexNumber)),
   case when (i.IndexName is null) then &#39;false&#39; else &#39;true&#39; end,
   case(i.UniqueFlag) when &#39;Y&#39; then &#39;unique&#39; else &#39;&#39; end,
   case(i.IndexType) when &#39;H&#39; then &#39;true&#39; when &#39;O&#39; then &#39;true&#39; else &#39;false&#39; end,
   case(i.IndexType) when &#39;P&#39; then &#39;true&#39; when &#39;Q&#39; then &#39;true&#39; else &#39;false&#39; end,
   i.ColumnName || &#39;,&#39;
from
   DBC.Indices i
where
   i.IndexType in (&#39;S&#39;, &#39;H&#39;, &#39;P&#39;, &#39;V&#39;, &#39;O&#39;, &#39;Q&#39;, &#39;J&#39;, &#39;N&#39;)
[  and i.DatabaseName = %.q:OWNER%]
[  and i.TableName = %.q:TABLE%]
order by 1, 2, 3, i.ColumnPosition
</a:TextTargetItem.Value>
