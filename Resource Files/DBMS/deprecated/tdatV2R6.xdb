<a:Name>SqlAttrQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, PROC, SCRIPT}

select
   f.DatabaseName,
   f.FunctionName,
   t.RequestText
from
   dbc.Functions f
   join dbc.Tables t on (t.TableName = f.FunctionName and t.DatabaseName = f.DatabaseName and t.TableKind in (&#39;F&#39;, &#39;A&#39;, &#39;R&#39;, &#39;E&#39;))
where
   f.RoutineKind = &#39;R&#39;
[and f.FunctionName = %.q:PROC%]</a:TextTargetItem.Value>
