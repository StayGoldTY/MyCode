<a:ObjectID>33143203-B99E-4F78-9E03-43D185F11AC0</a:ObjectID>
<a:Name>SqlIdtQuery</a:Name>
<a:TextTargetItem.Value>{VAR, VAL}

select
   trim(d.DatabaseName) || &#39;~&#39; || trim(t.TVMName),
   &#39;start with &#39; || cast(c.StartValue as varchar(30))
   || &#39; increment by &#39; || cast(c.Increment as varchar(30))
   || case when (c.MinValue = -2147483647) then &#39; no minvalue&#39; else &#39; minvalue &#39; || cast(c.MinValue as varchar(30)) end
   || case when (c.MaxValue = 2147483647) then &#39; no maxvalue&#39; else &#39; maxvalue &#39; || cast(c.MaxValue as varchar(30)) end
   || case when (c.Cyc = &#39;Y&#39;) then &#39; cycle&#39; else &#39; no cycle&#39; end
from
   DBC.IdCol c
   join DBC.TVM t on (t.TVMId = c.TableId and t.DatabaseId = c.DatabaseId)
   join DBC.DBase d on (d.DatabaseId = c.DatabaseId)
where 1 = 1
[  and t.TVMName = %.q:TABLE%]
[  and d.DatabaseName = %.q:OWNER%]
</a:TextTargetItem.Value>
