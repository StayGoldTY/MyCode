<a:Name>Options</a:Name>
<a:TextTargetItem.Value>&lt;table_options&gt; : composite=yes, separator=yes
{
   queue
   &lt;fallback&gt; %s : list=fallback|fallback protection|no fallback|no fallback protection
   with journal table = %s
   &lt;log&gt; %s : list=log|no log
   &lt;before_journal&gt; %s : list=journal|before journal|no journal|no before journal|dual journal|dual before journal
   &lt;after_journal&gt; %s : list=after journal|no after journal|dual after journal|local after journal|not local after journal
   checksum = %d
   freespace = %d : composite=yes
   {
      percent
   }
   datablocksize = %d : composite=yes
   {
      &lt;unit&gt; %s : list=byte|kilobyte|kbyte|bytes|kilobytes|kbytes
   }
   maximum datablocksize
   minimum datablocksize
}</a:TextTargetItem.Value>
