<a:TextTargetItem.Value>{TABLOWNER, TABLE, OWNER, TRIGGER, TRGEVENT, TRGTIME}

select
   t.SubjectTableDataBaseName,
   t.TableName,
   t.DataBaseName,
   t.TriggerName,
   case(t.Event) when &#39;U&#39; then &#39;update&#39; when &#39;I&#39; then &#39;insert&#39; when &#39;D&#39; then &#39;delete&#39; else &#39;&#39; end as trgevent,
   case(t.EnabledFlag) when &#39;N&#39; then &#39;disabled &#39; else &#39;&#39; end
   ||case(t.ActionTime) when &#39;A&#39; then &#39;after&#39; when &#39;B&#39; then &#39;before&#39; when &#39;I&#39; then &#39;instead of&#39; else &#39;&#39; end as trgtime
from
   dbc.Triggers t
[where t.SubjectTableDataBaseName = %.q:SCHEMA%]
order by
   1,2,3,4
</a:TextTargetItem.Value>
