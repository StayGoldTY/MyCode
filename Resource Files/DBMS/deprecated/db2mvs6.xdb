<o:TextTargetItem Id="o195">
<a:ObjectID>7763E865-45F1-4BB4-954E-D0603E5832A2</a:ObjectID>
<a:Name>SqlAttrQuery</a:Name>
<a:TextTargetItem.Value>{DATABASE ID, TABLESPACE ID, OPTIONS ...}

select
   t.dbname,
   t.name,
   case(t.dbname) when &#39;DSNDB04&#39; then &#39;&#39; else &#39; in &#39; concat t.dbname end
   concat case(p.stortype) when &#39;E&#39; then &#39; using vcat &#39; concat p.vcatname when &#39;I&#39; then &#39; using stogroup &#39; concat p.storname else &#39;&#39; end
   concat case(p.pqty) when 0 then &#39;&#39; else &#39; priqty &#39; concat varchar(integer(p.pqty)) end
   concat case(p.sqty) when 0 then &#39;&#39; else &#39; secqty &#39; concat varchar(integer(p.sqty)) end
   concat case(t.eraserule) when &#39;Y&#39; then &#39; erase yes&#39; else &#39;&#39; end
   concat case(p.freepage) when 0 then &#39;&#39; else &#39; freepage &#39; concat varchar(integer(p.freepage)) end
   concat case(p.pctfree) when 5 then &#39;&#39; else &#39; pctfree &#39; concat varchar(integer(p.pctfree)) end
   concat case(p.gbpcache) when &#39;A&#39; then &#39; gbpcache all&#39; when &#39;N&#39; then &#39; gbpcache none&#39; when &#39;S&#39; then &#39; gbpcache system&#39; else &#39;&#39; end
   concat case(p.trackmod) when &#39;N&#39; then &#39; trackmod no&#39; else &#39;&#39;end
   concat case(t.log) when &#39;N&#39; then &#39; log no&#39; else &#39;&#39; end
   concat case(t.type) when &#39;I&#39; then &#39; member cluster&#39; when &#39;K&#39; then &#39; member cluster&#39; else &#39;&#39;end
   concat case(t.segsize) when 0 then &#39;&#39; else &#39; segsize &#39; concat varchar(integer(t.segsize)) end
   concat &#39; bufferpool &#39; concat t.bpool
   concat case(t.lockrule) when &#39;S&#39; then &#39; locksize tablespace&#39; when &#39;T&#39; then &#39; locksize table&#39; when &#39;P&#39; then &#39; locksize page&#39; when &#39;R&#39; then &#39; locksize row&#39; when &#39;L&#39; then &#39; locksize lob&#39; else &#39;&#39; end
   concat case(t.lockmax) when -1 then &#39; lockmax system&#39; when 0 then &#39;&#39; else &#39; lockmax &#39; concat varchar(integer(t.lockmax)) end
   concat case(t.closerule) when &#39;N&#39; then &#39; close no&#39; else &#39;&#39; end
   concat case(p.compress) when &#39;Y&#39; then &#39; compress yes&#39; else &#39;&#39; end
   concat case(t.encoding_scheme) when &#39;A&#39; then &#39; ccsid ascii&#39; when &#39;E&#39; then &#39; ccsid ebcdic&#39; else &#39;&#39; end
   concat case(t.lockpart) when &#39;Y&#39; then &#39; lockpart yes&#39; else &#39;&#39; end
   concat case(t.maxrows) when 255 then &#39;&#39; else &#39; maxrows &#39; concat varchar(integer(t.maxrows)) end as coln
   , -2
from
     sysibm.systablespace t, sysibm.systablepart p
where
   implicit=&#39;N&#39;
   and p.tsname=t.name
   and p.dbname=t.dbname
   and t.partitions=0
   [and t.name=%.q:TABLESPACE%]
   [and t.dbname=%.q:CATALOG%]
union all select
   t.dbname,
   t.name,
   &#39; in &#39; concat t.dbname
   concat case(t.eraserule) when &#39;Y&#39; then &#39; erase yes&#39; else &#39;&#39; end
   concat case(t.log) when &#39;N&#39; then &#39; log no&#39; else &#39;&#39; end
   concat &#39; numparts &#39; concat varchar(integer(t.partitions))
   concat &#39; ( &#39;
   , -1
from
     sysibm.systablespace t
where
   implicit=&#39;N&#39;
   and t.partitions &gt; 0
   [and t.name=%.q:TABLESPACE%]
   [and t.dbname=%.q:CATALOG%]
union all select
   t.dbname,
   t.name,
   case(p.partition) when 1 then &#39; &#39; else &#39;,&#39; end
   concat &#39; part &#39; concat varchar(integer(p.partition))
   concat case(p.stortype) when &#39;E&#39; then &#39; using vcat &#39; concat p.vcatname when &#39;I&#39; then &#39; using stogroup &#39; concat p.storname else &#39;&#39; end
   concat case(p.pqty) when 0 then &#39;&#39; else &#39; priqty &#39; concat varchar(integer(p.pqty)) end
   concat case(p.sqty) when 0 then &#39;&#39; else &#39; secqty &#39; concat varchar(integer(p.sqty)) end
   concat case(p.freepage) when 0 then &#39;&#39; else &#39; freepage &#39; concat varchar(integer(p.freepage)) end
   concat case(p.pctfree) when 5 then &#39;&#39; else &#39; pctfree &#39; concat varchar(integer(p.pctfree)) end
   concat case(p.gbpcache) when &#39;A&#39; then &#39; gbpcache all&#39; when &#39;N&#39; then &#39; gbpcache none&#39; when &#39;S&#39; then &#39; gbpcache system&#39; else &#39;&#39; end
   concat case(p.trackmod) when &#39;N&#39; then &#39; trackmod no&#39; else &#39;&#39; end
   concat case(p.compress) when &#39;Y&#39; then &#39; compress yes&#39; else &#39;&#39; end
   , p.partition
from
     sysibm.systablespace t, sysibm.systablepart p
where
   implicit=&#39;N&#39;
   and p.tsname=t.name
   and p.dbname=t.dbname
   and t.partitions &gt; 0
   [and t.name=%.q:TABLESPACE%]
   [and t.dbname=%.q:CATALOG%]
union all select
   t.dbname,
   t.name,
   &#39;)&#39;
   concat case(t.type) when &#39;I&#39; then &#39; member cluster&#39; when &#39;K&#39; then &#39; member cluster&#39; else &#39;&#39;end
   concat case(t.segsize) when 0 then &#39;&#39; else &#39; segsize &#39; concat varchar(integer(t.segsize)) end
   concat &#39; bufferpool &#39; concat t.bpool
   concat case(t.lockrule) when &#39;S&#39; then &#39; locksize tablespace&#39; when &#39;T&#39; then &#39; locksize table&#39; when &#39;P&#39; then &#39; locksize page&#39; when &#39;R&#39; then &#39; locksize row&#39; when &#39;L&#39; then &#39; locksize lob&#39; else &#39;&#39; end
   concat case(t.lockmax) when -1 then &#39; lockmax system&#39; when 0 then &#39;&#39; else &#39; lockmax &#39; concat varchar(integer(t.lockmax)) end
   concat case(t.closerule) when &#39;N&#39; then &#39; close no&#39; else &#39;&#39; end
   concat case(t.encoding_scheme) when &#39;A&#39; then &#39; ccsid ascii&#39; when &#39;E&#39; then &#39; ccsid ebcdic&#39; else &#39;&#39; end
   concat case(t.lockpart) when &#39;Y&#39; then &#39; lockpart yes&#39; else &#39;&#39; end
   concat case(t.maxrows) when 255 then &#39;&#39; else &#39; maxrows &#39; concat varchar(integer(t.maxrows)) end as coln
   , 999
from
     sysibm.systablespace t
where
   implicit=&#39;N&#39;
   and t.partitions&gt;0
   [and t.name=%.q:TABLESPACE%]
   [and t.dbname=%.q:CATALOG%]
order by
   2, 4</a:TextTargetItem.Value>
