<o:TextTargetItem Id="o100">
<a:ObjectID>F44974C5-CECF-4A3A-89A1-886719550A07</a:ObjectID>
<a:Name>SqlOptsQuery</a:Name>
<a:TextTargetItem.Value>{TABLOWNER ID, TABLE ID, OWNER ID, INDEX ID, OPTIONS ...}

select
   i.tbcreator, i.tbname, i.creator, i.name, 
   case (p.partition)
      when 0 then &#39;&#39;
      when (pmin.partition) then &#39; cluster (part &#39; concat varchar(p.partition) concat &#39; values(&#39; concat t.limitkey concat &#39;)&#39;
      else &#39;, part &#39; concat varchar(p.partition) concat &#39; values(&#39; concat t.limitkey concat &#39;)&#39;
   end
   concat case p.stortype 
      when &#39;E&#39; then &#39; using vcat &#39; concat p.vcatname 
      when &#39;I&#39; then &#39; using stogroup &#39; concat p.storname 
         concat &#39; priqty &#39; concat varchar(p.pqty * 4)
         concat &#39; secqty &#39; concat varchar(p.sqty * 4)
         concat case i.eraserule when &#39;Y&#39; then &#39; erase yes&#39; when &#39;N&#39; then &#39; erase no&#39; else &#39;&#39; end
      else &#39;&#39; end
   concat &#39; freepage &#39; concat varchar(p.freepage)
   concat &#39; pctfree &#39; concat varchar(p.pctfree)
   concat &#39; gbpcache &#39; concat case p.gbpcache when &#39;A&#39; then &#39;all&#39; when &#39;N&#39; then &#39;none&#39; else &#39;changed&#39; end
   concat case(p.partition) when 0 then case i.clustering when &#39;Y&#39; then &#39; cluster&#39; else &#39;&#39; end when (pmax.partition) then &#39;)&#39; else &#39;&#39; end
   concat case(p.partition) when (pmax.partition) then
      &#39; bufferpool &#39; concat i.bpool 
      concat &#39; close &#39; concat case i.closerule when &#39;N&#39; then &#39;no&#39; else &#39;yes&#39; end 
      concat  case i.indextype  when &#39;2&#39; then &#39;&#39; else &#39; subpages &#39; concat char(int(i.pgsize/256)) end 
      concat  case i.dsetpass when &#39;nopassword&#39; then &#39;&#39; when &#39;&#39; then &#39;&#39; else &#39; password &#39; concat i.dsetpass end 
      concat  case i.piecesize when 0 then &#39;&#39; else &#39; piecesize &#39; concat varchar(i.piecesize) concat &#39;K&#39; end
   else &#39;&#39; end as coln,
   p.partition
from
   sysibm.sysindexes i
      left join sysibm.sysindexpart p on (p.ixname=i.name and p.ixcreator=i.creator)
      left join sysibm.systablepart t on (t.ixname=i.name and t.ixcreator=i.creator and t.partition=p.partition)
      left join sysibm.sysindexpart pmin on (pmin.ixname=i.name and pmin.ixcreator=i.creator)
      left join sysibm.sysindexpart pmax on (pmax.ixname=i.name and pmax.ixcreator=i.creator)
where 1 = 1
  and (coalesce(pmin.partition, 0) in (select min(x.partition) from sysibm.sysindexpart x where x.ixname=i.name and x.ixcreator=i.creator))
  and (coalesce(pmax.partition, 0) in (select max(x.partition) from sysibm.sysindexpart x where x.ixname=i.name and x.ixcreator=i.creator))
[ and i.tbcreator = %.q:OWNER%]
[ and i.tbname = %.q:TABLE%]
[ and i.dbname=%.q:CATALOG%]
order by 1, 2, 3, 4, 6</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:Comment>SQL query to reverse object physical options</a:Comment>
</o:TextTargetItem>
