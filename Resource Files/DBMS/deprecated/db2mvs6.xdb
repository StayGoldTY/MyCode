<a:TextTargetItem.Value>{OWNER, VIEW, COMMENT}

select 
   v.creator,
   v.name,
   t.remarks
from 
   sysibm.sysviews v
   left join sysibm.systables t on (t.name = v.name and t.creator = v.creator)
where 
   v.seqno=1
[  and v.creator=%.q:SCHEMA%]
order by
   v.creator,
   v.name</a:TextTargetItem.Value>
