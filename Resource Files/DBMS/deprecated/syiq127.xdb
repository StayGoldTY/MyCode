<a:Name>SqlAttrQuery</a:Name>
<a:TextTargetItem.Value>{OWNER ID, TABLE ID, INDEX ID, CIDXLIST ...}

select
   u.user_name,
   t.table_name,
   i.index_name,
   d.column_name + case(c.\[order\]) when &#39;D&#39; then &#39; desc&#39; else &#39;&#39; end + &#39;, &#39;
from
   sys.sysindex i
      join sys.systable t on i.table_id = t.table_id
      join sys.sysuserperms u on  u.user_id = t.creator
      join sys.sysixcol c on c.table_id = t.table_id and c.index_id = i.index_id
      join sys.syscolumn d on d.table_id = t.table_id and d.column_id = c.column_id
where 1 = 1
[  and u.user_name=%.q:OWNER%]
[  and t.table_name=%.q:TABLE%]
[  and i.index_name = %.q:INDEX%]
order by 1, 2, 3, c.sequence</a:TextTargetItem.Value>
