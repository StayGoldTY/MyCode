<o:MethodTargetItem Id="o306">
<a:ObjectID>B12EE98A-0988-47BE-B62E-665909BC8323</a:ObjectID>
<a:Name>NewSchedule</a:Name>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:MethodScript>Sub %Method%(obj)
   If (obj.GetExtendedAttribute(&quot;ScheduledEvent&quot;)) then
      Dim pDlg : Set pDlg = obj.CreateCustomDialog(&quot;SchedulePage&quot;)
      If(pDlg.ShowDialog()) then
         Dim sSchedule : sSchedule = obj.GetExtendedAttribute(&quot;SchedulesText&quot;)
         If (sSchedule &lt;&gt; &quot;&quot;) Then sSchedule = sSchedule &amp; &quot;,&quot; &amp; vbCrLf

         sSchedule = sSchedule &amp; &quot;schedule &quot; &amp; pDlg.GetValue(&quot;Name&quot;)
         
         Dim sEndTime   : sEndTime   = pDlg.GetValue(&quot;EndTime&quot;)
         Dim sStartTime : sStartTime = pDlg.GetValue(&quot;StartTime&quot;)
         If (sEndTime &lt;&gt; &quot;&quot;) then
            sSchedule = sSchedule &amp; &quot; between &#39;&quot; &amp; sStartTime &amp; &quot;&#39; and &#39;&quot; &amp; sEndTime &amp; &quot;&#39;&quot;
         ElseIf (sStartTime &lt;&gt; &quot;&quot;) Then
            sSchedule = sSchedule &amp; &quot; start time &#39;&quot; &amp; sStartTime &amp; &quot;&#39;&quot;
         End If
         
         Dim sPeriod : sPeriod = pDlg.GetValue(&quot;Period&quot;)
         If (sPeriod &lt;&gt; &quot;&quot;) Then
            sSchedule = sSchedule &amp; &quot; every &quot; &amp; sPeriod &amp; &quot; &quot; &amp; pDlg.GetValue(&quot;PeriodUnit&quot;)
         End If
         
         Dim sDay : sDay = pDlg.GetValue(&quot;Day&quot;)
         If (sDay &lt;&gt; &quot;&quot;) Then
            sSchedule = sSchedule &amp; &quot; on (&quot; &amp; sDay &amp; &quot;)&quot;
         End If

         Dim sStartDate : sStartDate = pDlg.GetValue(&quot;StartDate&quot;)
         If (sStartDate &lt;&gt; &quot;&quot;) Then
            sSchedule = sSchedule &amp; &quot; start date &#39;&quot; &amp; sStartDate &amp; &quot;&#39;&quot;
         End If

         obj.SetExtendedAttribute &quot;SchedulesText&quot;, sSchedule
      End If
   End If
End Sub</a:MethodScript>
</o:MethodTargetItem>
<o:MethodTargetItem Id="o307">
<a:ObjectID>151219F2-0C5B-472E-8B78-39AAC18CA0CF</a:ObjectID>
<a:Name>ParseFullTextEvent</a:Name>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:MethodScript>Sub %Method%(obj)
   Dim sEventText
   sEventText = obj.GetExtendedAttribute(&quot;FullEventText&quot;)

   If (sEventText = &quot;&quot;) then
      Exit Sub
   End If
   Dim sEventTextClean : sEventTextClean = Trim(Replace(Replace(Replace(sEventText, vbCr, &quot; &quot;), vbLf, &quot; &quot;), vbTab, &quot; &quot;))
   Dim s : s = &quot;&quot;
   Dim pos : pos = InStrRev(LCase(sEventTextClean), &quot;handler &quot;)
   If pos &gt; 0 Then
      s = Mid(sEventText, pos + Len(&quot;handler&quot;)) &#39;sEventText still contains crlf &amp; indent as user entered them
      obj.SetExtendedAttribute &quot;%CurrentTargetCode%.Handler&quot;, Trim(s)
      sEventTextClean = Trim(Left(sEventTextClean, pos - 1))
   end if
   If (LCase(WordAt(sEventTextClean, -2)) = &quot;at&quot;) Then 
      s = WordAt(sEventTextClean, -1)
      obj.SetExtendedAttribute &quot;%CurrentTargetCode%.Database&quot;, s
      sEventTextClean = Trim(Left(sEventTextClean, Len(sEventTextClean) - Len(s)))
      sEventTextClean = Trim(Left(sEventTextClean, Len(sEventTextClean) - Len(&quot;at&quot;)))
   End If
   If (LCase(WordAt(sEventTextClean, -1)) = &quot;enable&quot;) Then 
      obj.SetExtendedAttribute &quot;%CurrentTargetCode%.Enable&quot;, True
      sEventTextClean = Trim(Left(sEventTextClean, Len(sEventTextClean) - Len(&quot;enable&quot;)))
   End If
   If (LCase(WordAt(sEventTextClean, -1)) = &quot;disable&quot;) Then 
      obj.SetExtendedAttribute &quot;%CurrentTargetCode%.Enable&quot;, False
      sEventTextClean = Trim(Left(sEventTextClean, Len(sEventTextClean) - Len(&quot;disable&quot;)))
   End If
   If (LCase(WordAt(sEventTextClean, 1)) = &quot;type&quot;) Then
      obj.SetExtendedAttribute &quot;%CurrentTargetCode%.ScheduledEvent&quot;, False
      s = WordAt(sEventTextClean, 2)
      obj.SetExtendedAttribute &quot;%CurrentTargetCode%.EventType&quot;, s
      sEventTextClean = Trim(Mid(sEventTextClean, Len(&quot;type&quot;) + 1))
      sEventTextClean = Trim(Mid(sEventTextClean, Len(s) + 1))
      If (LCase(WordAt(sEventTextClean, 1)) = &quot;where&quot;) Then
         obj.SetExtendedAttribute &quot;%CurrentTargetCode%.TriggerCondition&quot;, Trim(Mid(sEventTextClean, Len(&quot;where&quot;) + 1))
      End If
   Else
      obj.SetExtendedAttribute &quot;%CurrentTargetCode%.ScheduledEvent&quot;, True
      obj.SetExtendedAttribute &quot;%CurrentTargetCode%.SchedulesText&quot;, Left(sEventText, Len(sEventTextClean))
   End If

End Sub</a:MethodScript>
</o:MethodTargetItem>
</c:Categories>
</o:TypedCategoryTargetItem>
