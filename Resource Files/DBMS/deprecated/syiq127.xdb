<a:HandlerScript>Function %AfterDatabaseReverseEngineer%(model)
   Dim pEvent
   For each pEvent in model.ExtendedObjects
      If ((pEvent.Stereotype = &quot;Event&quot;) And (pEvent.HasExtendedAttribute(&quot;FullEventText&quot;))) Then
         pEvent.ExecuteCustomMethod &quot;%CurrentTargetCode%.ParseFullTextEvent&quot;
         pEvent.SetExtendedAttribute &quot;%CurrentTargetCode%.FullEventText&quot;, &quot;&quot;
      End If
   Next
   %AfterDatabaseReverseEngineer% = True
