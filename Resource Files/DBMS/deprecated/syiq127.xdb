<a:TextTargetItem.Value>{OWNER, VIEW, COMMENT} 

select 
  u.user_name, 
  t.table_name, 
  t.remarks 
from 
  sys.sysuserperms u, 
  sys.systable t 
where 
  t.creator=u.user_id 
  and t.table_type=&#39;VIEW&#39; 
[  and u.user_name=%.q:SCHEMA%] 
order by 1, 2 </a:TextTargetItem.Value>
