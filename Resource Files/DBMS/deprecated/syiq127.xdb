<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{ WEBSERVICE, WEBOPERATION,  WEBSERVICETYPE, WEBOPERATIONSQL,  COMMENT, WEBAUTHREQUIRED, WEBCNCTSECURED, WEBUSER, Url  }

select 
s.service_name as service_name, 
substr(m.service_name, locate(m.service_name, &#39;/&#39;, -1)+1) as operation_name, 
m.service_type as service_type,
m.statement,
m.remarks,
m.auth_required, 
m.secure_required, 
u.user_name,
m.url_path
from sys.syswebservice m left outer join sys.sysuserperms u on ( m.user_id = u.user_id ),
       sys.syswebservice s
where s.service_type=&#39;DISH&#39; and m.service_type=&#39;SOAP&#39; and ((locate(m.service_name, &#39;/&#39;, -1)&gt;1 and substr(m.service_name, 1, locate(m.service_name, &#39;/&#39;, -1)-1)=s.parameter) or 
(s.parameter is NULL and locate(m.service_name,&#39;/&#39;,-1)=0 ))
[  and s.service_name=%.q:WEBSERVICE%]
UNION
select 
if (locate(service_name, &#39;/&#39;) &gt; 1) then substr(service_name, 1, locate(service_name, &#39;/&#39;, -1)-1) else &#39;/&#39; endif as service_path, 
if (locate(m2.service_name, &#39;/&#39;) &gt; 1) then substr(m2.service_name, locate(m2.service_name, &#39;/&#39;, -1)+1) else m2.service_name endif as operation_name, 
m2.service_type,
m2.statement,
m2.remarks,
m2.auth_required, 
m2.secure_required, 
u.user_name,
case(m2.url_path) when &#39;Y&#39; then &#39;On&#39; when &#39;E&#39; then &#39;Elements&#39; else &#39;Off&#39; end
from sys.syswebservice m2 left outer join sys.sysuserperms u on ( m2.user_id = u.user_id )
where m2.service_type&lt;&gt;&#39;DISH&#39; and m2.service_type&lt;&gt;&#39;SOAP&#39;
[  and service_path=%.q:WEBSERVICEPATH% and m2.service_type=%.q:WEBSERVICETYPE% ]
order by 1, 2</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
