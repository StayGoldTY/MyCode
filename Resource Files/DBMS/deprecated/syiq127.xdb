<o:TextTargetItem Id="o98">
<a:ObjectID>B2888A33-A238-452B-856C-0CBEE4CC5292</a:ObjectID>
<a:Name>SqlChckQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, TABLE, COLUMN, CONSTRAINT}

if( db_property( &#39;NamedConstraints&#39; ) = &#39;ON&#39; ) then
select 
   u.user_name, 
   t.table_name, 
   c.column_name, 
   case(lcase(left(h.check_defn, 5))) when &#39;check&#39; then substring(h.check_defn, 6) else h.check_defn end
from 
   sys.sysuserperms u
      join sys.systable t on (t.creator=u.user_id)
      join sys.syscolumn c on (c.table_id=t.table_id)
      join sys.sysconstraint k on (k.table_id = t.table_id and k.column_id = c.column_id and k.constraint_type = &#39;C&#39;)
      join sys.syscheck h on (h.check_id = k.constraint_id)
where 1=1
[  and u.user_name=%.q:OWNER%]
[  and t.table_name=%.q:TABLE%]
[  and c.column_name=%.q:COLUMN%]
else
   select
      u.user_name,
      t.table_name,
      c.column_name,
      &#39;&#39;,
      case(lcase(left(c.&quot;check&quot;, 5))) when &#39;check&#39; then substring(c.&quot;check&quot;, 6) else c.&quot;check&quot; end
   from
      sys.sysuserperms u
         join sys.systable t on (t.creator=u.user_id)
         join sys.syscolumn c on (c.table_id=t.table_id)
   where
      c.&quot;check&quot; is not null
   [  and u.user_name=%.q:OWNER%]
   [  and t.table_name=%.q:TABLE%]
   [  and c.column_name=%.q:COLUMN%]
end if
</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:Comment>SQL query to reverse object check constraints</a:Comment>
</o:TextTargetItem>
