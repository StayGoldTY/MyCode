<a:TextTargetItem.Value>{OWNER, TABLE, OPTIONS}

select
   u.user_name,
   t.table_name,
   &#39;in &#39;+f.dbspace_name
from
   sys.sysuserperms u,
   sys.systable t,
   sys.sysfile f
where
   t.file_id = f.file_id
   and t.creator = u.user_id
[  and t.table_name=%.q:TABLE%]
[  and u.user_name=%.q:SCHEMA%]
