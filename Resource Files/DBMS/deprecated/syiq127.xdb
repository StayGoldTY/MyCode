<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, TABLE, INDEX, INDEXTYPE, UNIQUE, COMMENT}

select
   u.user_name,
   t.table_name,
   i.index_name,
   i.index_type,
   case(i.\[unique\]) when &#39;Y&#39; then &#39;True&#39; else &#39;False&#39; end,
   i.remarks
from
   sys.sysindex i
      join sys.systable t on i.table_id = t.table_id
      join sys.sysuserperms u on  u.user_id = t.creator
where 1 = 1
   and i.index_owner &lt;&gt; &#39;IQ&#39;
   and i.\[unique\] in (&#39;Y&#39;, &#39;N&#39;)
[  and u.user_name=%.q:OWNER%]
[  and t.table_name=%.q:TABLE%]
order by 1, 2, i.index_id</a:TextTargetItem.Value>
