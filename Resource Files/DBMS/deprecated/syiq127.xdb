<a:ObjectID>E2420A35-21DE-40DF-8E49-86B215333C1D</a:ObjectID>
<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, TABLE, COLUMN, DTTPCODE, LENGTH, PREC, NOTNULL, @OBJTLABL, DOMAIN, IDENTITY, DEFAULT, ExtIqUnicity}

select 
   u.user_name,
   t.table_name,
   c.column_name,
   d.domain_name,
   c.width,
   c.scale,
   case(c.nulls) when &#39;Y&#39; then &#39;NULL&#39; else &#39;NOT NULL&#39; end,
   c.remarks,
   x.type_name,
   case(lower(c.\[default\])) when &#39;autoincrement&#39; then &#39;identity&#39; else &#39;&#39; end,
   c.\[default\],
   case(i.cardinality)
      when 0 then case (i.approx_unique_count) 
         when 0 then null 
         else i.approx_unique_count end
      else i.cardinality end as coln
from
   sys.sysuserperms u 
      join sys.systable t on (t.creator=u.user_id)
      join sys.syscolumn c on (c.table_id=t.table_id)
      left join sys.sysusertype x on (x.type_id = c.user_type)
      join sys.sysdomain d on (d.domain_id=c.domain_id)
      left outer join sys.sysiqcolumn i on (i.table_id = t.table_id and i.column_id = c.column_id and (i.approx_unique_count+i.cardinality) &gt; 0)
where 
c.column_type in (&#39;C&#39;, &#39;R&#39;)
[  and u.user_name=%.q:OWNER%]
[  and t.table_name=%.q:TABLE%]
order by
   1, 2, c.column_id</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:Comment>SQL query to list objects</a:Comment>
</o:TextTargetItem>
<o:TextTargetItem Id="o101">
<a:ObjectID>1786425F-684F-4633-B097-3FD21485E697</a:ObjectID>
<a:Name>SqlPermQuery</a:Name>
<a:TextTargetItem.Value>{ GRANTEE ID, COLNPERMISSION ID, COLUMN ...}

select 
u.user_name grantee, 
case
when (p.privilege_type = 1 and p.is_grantable =&#39;Y&#39; ) then &#39;SELECT+&#39;
when (p.privilege_type = 1)  then &#39;SELECT&#39;
when (p.privilege_type = 8 and p.is_grantable =&#39;Y&#39; ) then &#39;UPDATE+&#39;
when (p.privilege_type = 8)  then &#39;UPDATE&#39;
when (p.privilege_type = 16 and p.is_grantable =&#39;Y&#39; ) then &#39;REFERENCES+&#39;
when (p.privilege_type = 16)  then &#39;REFERENCES&#39;
end as coln,
s.column_name + &#39;,&#39;
from sys.sysuserperms u, sys.sysuserperms u1, sys.syscolumn s, sys.syscolperm p, sys.systable t
where 
(t.table_name = %.q:TABLE%) and
[ (t.creator = u1.user_id) and (u1.user_name = %.q:OWNER% ) and ]
(t.table_id = p.table_id) and
(p.column_id = s.column_id) and
(s.table_id = t.table_id) and
(u.user_id = p.grantee)
</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:Comment>SQL query to reverse object permissions</a:Comment>
</o:TextTargetItem>
