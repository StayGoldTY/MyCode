<a:Comment>Type of the first index defined on the columns</a:Comment>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:ExtendedAttributeTargetItem.DataType>12</a:ExtendedAttributeTargetItem.DataType>
<a:Computed>1</a:Computed>
<a:ComputedAccessType>RO</a:ComputedAccessType>
<a:GetMethodScript>Function %Get%(obj)

   Dim sIndexType
   
   sIndexType = &quot;&quot;
   
   Dim indexColumn, index
   set indexColumn = nothing
   set index = nothing
   
   If (obj.IndexColumns.Count &gt; 0) Then
      Set indexColumn = obj.IndexColumns.Item(0)
   End If
   
   If Not (indexColumn is nothing) Then
      Set index = indexColumn.Index
   End If
   
   If Not (index is nothing) Then
      sIndexType = index.Type
   End If

   %Get% = sIndexType
   
End Function</a:GetMethodScript>
</o:ExtendedAttributeTargetItem>
