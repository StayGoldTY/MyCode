<o:TextTargetItem Id="o72">
<a:ObjectID>8591B65A-E342-49F6-A647-45C31CE4EF6C</a:ObjectID>
<a:Name>Drop</a:Name>
<a:TextTargetItem.Value>if exists (select 1
            from  sys.sysindex where  table_id    =    (select table_id from sys.systable
             where table_name=%.q:TABLE% [%QUALIFIER%?
            and creator=user_id(%.q:OWNER%)])
            and   index_name  = %.q:INDEX%
            and   index_type &lt;&gt; &#39;FP&#39;
            and   index_owner &lt;&gt; &#39;IQ&#39;
            and   index_id &gt; 0
            and   index_id &lt; 255 [%QUALIFIER%?
            and creator=user_id(%.q:OWNER%)])
then
   drop index [%QUALIFIER%]%TABLE%.%INDEX%
end if
</a:TextTargetItem.Value>
