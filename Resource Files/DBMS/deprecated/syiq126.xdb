<a:Name>SqlAttrQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, VIEW, SCRIPT}

select
   u.user_name,
   t.table_name,
   t.view_def
from
   sys.sysuserperms u,
   sys.systable t
where
   t.creator=u.user_id
   and t.table_type=&#39;VIEW&#39;
[  and u.user_name=%.q:OWNER%]
[  and t.table_name=%.q:VIEW%]</a:TextTargetItem.Value>
