<a:Name>SqlAttrQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, PROC, SCRIPT, @OBJTLABL}

select
   u.user_name,
   p.proc_name,
   p.proc_defn,
   p.remarks
from
   sys.sysuserperms u
      join sys.sysprocedure p on (p.creator=u.user_id)
where 1=1
[  and u.user_name=%.q:OWNER%]
[  and p.proc_name=%.q:PROC%]
</a:TextTargetItem.Value>
