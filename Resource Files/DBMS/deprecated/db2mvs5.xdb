<a:Name>RunSynonymActions</a:Name>
<a:TextTargetItem.Value>.foreach_item(Synonyms)
.if(%@1% == DROP)
%Drop%;\n
.elsif(%@1% == CREATE)
%Create%;\n
.else
-- WRONG CALL OF RunSynonymActions
.endif
.next()

</a:TextTargetItem.Value>
