<a:ObjectID>6524C5DB-A998-4A8C-92AE-FABF0CA0ADF9</a:ObjectID>
<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{COWNER, CHILD, FKCONSTRAINT, POWNER, PARENT, FKEYCOLUMNS, CKEYCOLUMNS}

select
   u.name,
   o.name,
   c.name,
   case when(r.pmrydbid = r.frgndbid) then (select v.name from dbo.sysusers v join dbo.sysobjects p on (p.id = r.reftabid and v.uid = p.uid)) else &#39;&#39; end,
   object_name(r.reftabid, r.pmrydbid),
   col_name(o.id, r.fokey1, r.frgndbid)+ &#39;,&#39; +
   col_name(o.id, r.fokey2, r.frgndbid)+ &#39;,&#39; +
   col_name(o.id, r.fokey3, r.frgndbid)+ &#39;,&#39; +
   col_name(o.id, r.fokey4, r.frgndbid)+ &#39;,&#39; +
   col_name(o.id, r.fokey5, r.frgndbid)+ &#39;,&#39; +
   col_name(o.id, r.fokey6, r.frgndbid)+ &#39;,&#39; +
   col_name(o.id, r.fokey7, r.frgndbid)+ &#39;,&#39; +
   col_name(o.id, r.fokey8, r.frgndbid)+ &#39;,&#39; +
   col_name(o.id, r.fokey9, r.frgndbid)+ &#39;,&#39; +
   col_name(o.id, r.fokey10, r.frgndbid) as colnA,
   col_name(r.reftabid, r.refkey1,r.pmrydbid)+ &#39;,&#39; +
   col_name(r.reftabid, r.refkey2,r.pmrydbid)+ &#39;,&#39; +
   col_name(r.reftabid, r.refkey3,r.pmrydbid)+ &#39;,&#39; +
   col_name(r.reftabid, r.refkey4,r.pmrydbid)+ &#39;,&#39; +
   col_name(r.reftabid, r.refkey5,r.pmrydbid)+ &#39;,&#39; +
   col_name(r.reftabid, r.refkey6,r.pmrydbid)+ &#39;,&#39; +
   col_name(r.reftabid, r.refkey7,r.pmrydbid)+ &#39;,&#39; +
   col_name(r.reftabid, r.refkey8,r.pmrydbid)+ &#39;,&#39; +
   col_name(r.reftabid, r.refkey9,r.pmrydbid)+ &#39;,&#39; +
   col_name(r.reftabid, r.refkey10,r.pmrydbid) as colnB
from
    [%CATALOG%.]dbo.sysobjects c
   join  [%CATALOG%.]dbo.sysconstraints s  on (s.constrid = c.id)
   join  [%CATALOG%.]dbo.sysobjects o on (o.id = s.tableid[  and o.name = %.q:TABLE%])
   join  [%CATALOG%.]dbo.sysusers u on (u.uid = o.uid[  and u.name = %.q:SCHEMA%])
   join  [%CATALOG%.]dbo.sysreferences r on (r.tableid = o.id and r.constrid = s.constrid)
where c.type = &#39;RI&#39;
union select
   u.name,
   o.name,
   &#39;&#39;,
   v.name,
   p.name,
   col_name(o.id, k.key1[,db_id(%.q:CATALOG%)])+ &#39;,&#39; +
   col_name(o.id, k.key2[,db_id(%.q:CATALOG%)])+ &#39;,&#39; +
   col_name(o.id, k.key3[,db_id(%.q:CATALOG%)])+ &#39;,&#39; +
   col_name(o.id, k.key4[,db_id(%.q:CATALOG%)])+ &#39;,&#39; +
   col_name(o.id, k.key5[,db_id(%.q:CATALOG%)])+ &#39;,&#39; +
   col_name(o.id, k.key6[,db_id(%.q:CATALOG%)])+ &#39;,&#39; +
   col_name(o.id, k.key7[,db_id(%.q:CATALOG%)])+ &#39;,&#39; +
   col_name(o.id, k.key8[,db_id(%.q:CATALOG%)]) as colnA,
   col_name(p.id, k.depkey1[,db_id(%.q:CATALOG%)])+ &#39;,&#39; +
   col_name(p.id, k.depkey2[,db_id(%.q:CATALOG%)])+ &#39;,&#39; +
   col_name(p.id, k.depkey3[,db_id(%.q:CATALOG%)])+ &#39;,&#39; +
   col_name(p.id, k.depkey4[,db_id(%.q:CATALOG%)])+ &#39;,&#39; +
   col_name(p.id, k.depkey5[,db_id(%.q:CATALOG%)])+ &#39;,&#39; +
   col_name(p.id, k.depkey6[,db_id(%.q:CATALOG%)])+ &#39;,&#39; +
   col_name(p.id, k.depkey7[,db_id(%.q:CATALOG%)])+ &#39;,&#39; +
   col_name(p.id, k.depkey8[,db_id(%.q:CATALOG%)]) as colnB
from
   [%CATALOG%.]dbo.syskeys k
   join [%CATALOG%.]dbo.sysobjects o on (o.id = k.id[  and o.name = %.q:TABLE%])
   join [%CATALOG%.]dbo.sysusers u on (u.uid = o.uid[  and u.name = %.q:SCHEMA%])
   join [%CATALOG%.]dbo.sysobjects p on (p.id = k.depid)
   join [%CATALOG%.]dbo.sysusers v on (v.uid = p.uid)
where k.type != 1
and not exists (select 1 from [%CATALOG%.]dbo.sysreferences r where r.tableid = o.id and r.reftabid = p.id)</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:Comment>SQL query to list objects</a:Comment>
</o:TextTargetItem>
