<o:TextTargetItem Id="o89">
<a:ObjectID>42F4979E-0EFA-4936-B515-AB80F510FC60</a:ObjectID>
<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, TABLE, INDEX, UNIQUE, CLUSTER, CIDXLIST}

select
   u.name,
   o.name,
   i.name,
   case (i.status &amp; 2) when 0 then &#39;&#39; else &#39;unique&#39; end,
   case ((i.status2&amp;512)+(i.status&amp;16)) when 0 then &#39;&#39; else &#39;cluster&#39; end,
   index_col(&#39;[%CATALOG%..]%TABLE%&#39;, indid, 1, o.uid) + &#39; &#39; + index_colorder(&#39;[%CATALOG%..]%TABLE%&#39;, indid, 1, o.uid) + &#39;, &#39; +
   index_col(&#39;[%CATALOG%..]%TABLE%&#39;, indid, 2, o.uid) + &#39; &#39; + index_colorder(&#39;[%CATALOG%..]%TABLE%&#39;, indid, 2, o.uid) + &#39;, &#39; +
   index_col(&#39;[%CATALOG%..]%TABLE%&#39;, indid, 3, o.uid) + &#39; &#39; + index_colorder(&#39;[%CATALOG%..]%TABLE%&#39;, indid, 3, o.uid) + &#39;, &#39; +
   index_col(&#39;[%CATALOG%..]%TABLE%&#39;, indid, 4, o.uid) + &#39; &#39; + index_colorder(&#39;[%CATALOG%..]%TABLE%&#39;, indid, 4, o.uid) + &#39;, &#39; +
   index_col(&#39;[%CATALOG%..]%TABLE%&#39;, indid, 5, o.uid) + &#39; &#39; + index_colorder(&#39;[%CATALOG%..]%TABLE%&#39;, indid, 5, o.uid) + &#39;, &#39; +
   index_col(&#39;[%CATALOG%..]%TABLE%&#39;, indid, 6, o.uid) + &#39; &#39; + index_colorder(&#39;[%CATALOG%..]%TABLE%&#39;, indid, 6, o.uid) + &#39;, &#39; +
   index_col(&#39;[%CATALOG%..]%TABLE%&#39;, indid, 7, o.uid) + &#39; &#39; + index_colorder(&#39;[%CATALOG%..]%TABLE%&#39;, indid, 7, o.uid) + &#39;, &#39; +
   index_col(&#39;[%CATALOG%..]%TABLE%&#39;, indid, 8, o.uid) + &#39; &#39; + index_colorder(&#39;[%CATALOG%..]%TABLE%&#39;, indid, 8, o.uid) + &#39;, &#39; +
   index_col(&#39;[%CATALOG%..]%TABLE%&#39;, indid, 9, o.uid) + &#39; &#39; + index_colorder(&#39;[%CATALOG%..]%TABLE%&#39;, indid, 9, o.uid) + &#39;, &#39; +
   index_col(&#39;[%CATALOG%..]%TABLE%&#39;, indid,10, o.uid) + &#39; &#39; + index_colorder(&#39;[%CATALOG%..]%TABLE%&#39;, indid,10, o.uid) + &#39;, &#39; +
   index_col(&#39;[%CATALOG%..]%TABLE%&#39;, indid,11, o.uid) + &#39; &#39; + index_colorder(&#39;[%CATALOG%..]%TABLE%&#39;, indid,11, o.uid) + &#39;, &#39; +
   index_col(&#39;[%CATALOG%..]%TABLE%&#39;, indid,12, o.uid) + &#39; &#39; + index_colorder(&#39;[%CATALOG%..]%TABLE%&#39;, indid,12, o.uid) + &#39;, &#39; +
   index_col(&#39;[%CATALOG%..]%TABLE%&#39;, indid,13, o.uid) + &#39; &#39; + index_colorder(&#39;[%CATALOG%..]%TABLE%&#39;, indid,13, o.uid) + &#39;, &#39; +
   index_col(&#39;[%CATALOG%..]%TABLE%&#39;, indid,14, o.uid) + &#39; &#39; + index_colorder(&#39;[%CATALOG%..]%TABLE%&#39;, indid,14, o.uid) + &#39;, &#39; +
   index_col(&#39;[%CATALOG%..]%TABLE%&#39;, indid,15, o.uid) + &#39; &#39; + index_colorder(&#39;[%CATALOG%..]%TABLE%&#39;, indid,15, o.uid) + &#39;, &#39; +
   index_col(&#39;[%CATALOG%..]%TABLE%&#39;, indid,16, o.uid) + &#39; &#39; + index_colorder(&#39;[%CATALOG%..]%TABLE%&#39;, indid,16, o.uid) as coln
from
   [%CATALOG%.]dbo.sysusers u,
   [%CATALOG%.]dbo.sysindexes i,
   [%CATALOG%.]dbo.sysobjects o
where
   u.uid = o.uid
   and i.id = o.id
   and i.indid != 0
   and (i.status2 &amp; 2) = 0
   and i.keycnt != 0
[  and u.name = %.q:SCHEMA%]
[  and o.name = %.q:TABLE%]
order by 1, 2, i.indid</a:TextTargetItem.Value>
