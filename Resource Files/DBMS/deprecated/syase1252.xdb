<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{DOMAIN, DTTPCODE, LENGTH, PREC, NOTNULL, IDENTITY}

select distinct
   d.name,
   t.name,
   case(t.type)
      when 135 then d.length/@@unicharsize
      when 155 then d.length/@@unicharsize
      else isnull(convert(int, d.prec), d.length)
   end as coln,
   isnull(d.scale, 0),
   case (d.allownulls) when 1 then &#39;NULL&#39; else &#39;NOT NULL&#39; end,
   case (d.ident) when 1 then &#39;identity&#39; else &#39;&#39; end
from
   [%CATALOG%.]dbo.systypes d
   join [%CATALOG%.]dbo.systypes t on (t.type=d.type and t.usertype &lt; 100 and t.usertype not in (18, 24, 25, 42, 80))
where
   d.usertype&gt;=100
order by
   1,2</a:TextTargetItem.Value>
