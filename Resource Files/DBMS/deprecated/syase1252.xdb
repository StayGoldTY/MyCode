<a:Name>SqlAttrQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, DEFAULT, BOUND_OBJECT}

select
   u.name,
   o.name,
   d.name
from
   [%CATALOG%.]dbo.sysusers        u
   join [%CATALOG%.]dbo.sysobjects o on (o.uid = u.uid and o.type = &#39;D&#39;[  and o.name = %.q:DEFAULT%])
   join [%CATALOG%.]dbo.systypes   d on (d.tdefault = o.id)
[where u.name = %.q:OWNER%]
union select 
   u.name,
   o.name,
   u.name||&#39;.&#39;||t.name||&#39;.&#39;||c.name
from
   [%CATALOG%.]dbo.sysusers        u
   join [%CATALOG%.]dbo.sysobjects o on (o.uid = u.uid and o.type = &#39;D&#39;[  and o.name = %.q:DEFAULT%])
   join [%CATALOG%.]dbo.syscolumns c on (c.cdefault = o.id)
   join [%CATALOG%.]dbo.sysobjects t on (t.id = c.id)
[where u.name = %.q:OWNER%]
order by
   1, 2</a:TextTargetItem.Value>
