<a:Name>SqlAttrQuery</a:Name>
<a:TextTargetItem.Value>{DOMAIN, DEFAULTNAME (&#39;0&#39;=&#39;&#39;), RULENAME (&#39;0&#39;=&#39;&#39;)}

select distinct
   d.name,
   convert(char(20), isnull(d.tdefault, 0)),
   convert(char(20), isnull(d.domain, 0))
from
   [%CATALOG%.]dbo.systypes d
   join [%CATALOG%.]dbo.systypes t on (t.type=d.type and t.usertype &lt; 100 and t.usertype not in (18, 24, 25, 42, 80))
where
   d.usertype&gt;=100
[  and d.name = %.q:DOMAIN%]
order by
   1,2</a:TextTargetItem.Value>
