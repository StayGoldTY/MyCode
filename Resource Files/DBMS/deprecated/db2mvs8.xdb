<o:TemplateTargetItem Id="o513">
<a:ObjectID>265D7184-9B57-4BE0-B0B0-66FF93D49A5D</a:ObjectID>
<a:Name>alterPartitionScript</a:Name>
<a:TemplateTargetItem.Value>.vbscript(%NEW.PartitionList%, %OLD.PartitionList%, %QualifiedCode%)

Dim Part, sAlter, OldPart, NewPart


For each NewPart in Split(ScriptInputArray(0), vbCrLf)
   OldPart = GetPart(ScriptInputArray(1), PartNum(NewPart))
   If (OldPart = &quot;&quot;) then &#39;New
      sAlter = sAlter &amp; vbCrLf &amp; &quot;alter table &quot; &amp; ScriptInputArray(2) &amp; vbCrLf &amp; &quot;   add partition &quot; &amp; PartDef(NewPart) &amp; &quot;;&quot; &amp; vbCrLf 
   elseif (NewPart &lt;&gt; OldPart) then
      sAlter = sAlter &amp; vbCrLf &amp; &quot;alter table &quot; &amp; ScriptInputArray(2) &amp; vbCrLf &amp; &quot;   alter partition &quot; &amp; PartNum(NewPart) &amp; &quot; &quot; &amp; PartDef(NewPart) &amp; &quot;;&quot; &amp; vbCrLf 
   end if
next
ScriptResult = Mid(sAlter, 3)

function GetPart(PartList, Num)
   Dim Part
   For each Part in Split(PartList, vbCrLf)
      if (PartNum(Part) = CStr(Num)) then
         GetPart = Part
         Exit Function
      end if
   Next
   GetPart = &quot;&quot;
end function

function PartNum(StrPart)
   Dim PartArray
   PartArray = Split(StrPart, vbTab)
   if (UBound(PartArray) &lt;&gt; 2) then
      PartNum = &quot;&quot;
   else
      PartNum = PartArray(0)
   end if
end function

function PartDef(StrPart)
   Dim PartArray
   PartArray = Split(StrPart, vbTab)
   if (UBound(PartArray) &lt;&gt; 2) then
      PartDef = &quot;&quot;
   else
      if (CBool(PartArray(2))) then
         PartDef = PartArray(1) &amp; &quot; inclusive&quot;
      else
         PartDef = PartArray(1)
      end if
   end if
end function
.endvbscript</a:TemplateTargetItem.Value>
