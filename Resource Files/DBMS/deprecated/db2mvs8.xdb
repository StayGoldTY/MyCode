<a:ObjectID>B3481C3A-D23A-4EC4-937F-CB95478FF13A</a:ObjectID>
<a:Name>ValidatePhysicalOptions</a:Name>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>1254729562</a:ModificationDate>
<a:Modifier>mledier</a:Modifier>
<a:Comment>Clean up physical options giving priority to &quot;no&quot; option against valued ones. Ex. if &quot;no minvalue&quot; is checked, this method will erase the minvalue option.</a:Comment>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:MethodScript>Sub %Method%(obj)
   if(obj.Identity) then
      if (obj.GetExtendedAttribute(&quot;%CurrentTargetCode%.IdentityOptionsMaxvalueNoMaxvalue&quot;)) then
         obj.DeletePhysicalOption &quot;&lt;identity_options&gt;/&lt;maxvalue&gt;/maxvalue&quot;
      end if
      if (obj.GetExtendedAttribute(&quot;%CurrentTargetCode%.IdentityOptionsMinvalueNoMinvalue&quot;)) then
         obj.DeletePhysicalOption &quot;&lt;identity_options&gt;/&lt;minvalue&gt;/minvalue&quot;
      end if
      if (obj.GetExtendedAttribute(&quot;%CurrentTargetCode%.IdentityOptionsCacheNoCache&quot;)) then
         obj.DeletePhysicalOption &quot;&lt;identity_options&gt;/&lt;cache&gt;/cache&quot;
      end if
   end if   
End Sub</a:MethodScript>
</o:MethodTargetItem>
</c:Categories>
</o:TypedCategoryTargetItem>
