<o:TextTargetItem Id="o114">
<a:ObjectID>E963C24E-6B40-4D87-BD19-349C895057CF</a:ObjectID>
<a:Name>SqlTblMultiPartDef</a:Name>
<a:TextTargetItem.Value>{VAR ID, VAL EX ...}

select
   rtrim(i.creator) concat rtrim(i.name),
   case when (p.partition &gt; 1) then &#39;, &#39; else &#39;&#39; end
   concat &#39;partition &#39; concat varchar(p.partition) concat &#39; ending at (&#39; concat varchar(p.limitkey) concat &#39;)&#39;
   concat &#39; %SqlIdxSinglePartDef.&#39; concat rtrim(i.creator) concat rtrim(i.name) concat varchar(p.partition) concat &#39;%&#39; as coln
from
   sysibm.systablepart p
   left join sysibm.systables t on (t.tsname=p.tsname and t.dbname = p.dbname)
   left join sysibm.sysindexes i on (i.tbname=t.name and i.tbcreator=t.creator)
where i.indextype = &#39;P&#39;
[ and i.creator = %.q:OWNER%]
[ and i.tbname = %.q:TABLE%]
[ and i.name = %.q:INDEX%]
[ and i.dbname=%.q:CATALOG%]
order by 1, p.partition</a:TextTargetItem.Value>
