<a:Name>AfterDatabaseReverseEngineer</a:Name>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>1286898944</a:ModificationDate>
<a:Modifier>mledier</a:Modifier>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:HandlerScript>Function %AfterDatabaseReverseEngineer%(model)

   Dim O
   For each O in model.GetCollectionByStereotype(&quot;MaterializedView&quot;)
      O.Type = &quot;MATERIALIZED&quot;
   Next

   %AfterDatabaseReverseEngineer% = True
End Function</a:HandlerScript>
</o:EventHandlerTargetItem>
<o:EventHandlerTargetItem Id="o341">
<a:ObjectID>720455C1-A621-47DE-99BD-6D3B774CE3DE</a:ObjectID>
<a:Name>OnLanguageChanging</a:Name>
<a:CreationDate>1281616993</a:CreationDate>
<a:Creator>mledier</a:Creator>
<a:ModificationDate>1286984944</a:ModificationDate>
<a:Modifier>mledier</a:Modifier>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:HandlerScript>Function %OnLanguageChanging%(model, oldLanguage)

   if (oldLanguage.Code = &quot;DB2MVS7&quot;) then &#39; upgrade index partition options
      model.ExecuteCustomMethod(&quot;%CurrentTargetCode%.IndexUpdatePartitionOptions&quot;)
   end if

   if Not(model.GetExtendedAttribute(&quot;AutoFixMaterializedViewDone&quot;)) then
      model.ExecuteCustomMethod(&quot;%CurrentTargetCode%.AutoFixMaterializedViews&quot;)
      model.SetExtendedAttributeText &quot;AutoFixMaterializedViewDone&quot;, &quot;True&quot;
   end if

   %OnLanguageChanging% = True
End Function

</a:HandlerScript>
</o:EventHandlerTargetItem>
