<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, PROC, FUNC}

select
   p.schema,
   p.name,
   case when (p.routinetype=&#39;F&#39;) then p.name else &#39;&#39; end
from
   sysibm.sysroutines p
where p.routinetype in (&#39;P&#39;, &#39;F&#39;)
   and p.origin in (&#39;E&#39;, &#39;Q&#39;, &#39;U&#39;)
[  and p.schema=%.q:SCHEMA%]
order by 1, 2

</a:TextTargetItem.Value>
