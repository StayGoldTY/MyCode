<o:MethodTargetItem Id="o591">
<a:ObjectID>988A11C2-3D27-45B8-81E7-B69C1E1B04C7</a:ObjectID>
<a:Name>UpdatePartitionOptions</a:Name>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:MethodScript>Sub %Method%(obj)

   Dim S, pos
   S = obj.PhysicalOptions
   pos = InStr(LCase(S), &quot;cluster&quot;)

   if (pos &gt; 0) then
      Dim sPartOptions, sNewPhysOptions
      sNewPhysOptions = Left(S, pos - 1) &amp; &quot;##PartitionOptions##&quot;
      sPartOptions = Mid(S, pos + Len(&quot;cluster&quot;))
      pos = InStr(sPartOptions, &quot;(&quot;) &#39; Start of partition options
      if (pos &gt; 0) then
         Dim n
         n = 1
         sPartOptions = Mid(sPartOptions, pos + 1)
         For pos = 1 to Len(sPartOptions)
            if (Mid(sPartOptions, pos, 1) = &quot;(&quot;) then n = n + 1 
            if (Mid(sPartOptions, pos, 1) = &quot;)&quot;) then n = n - 1 
            if (n = 0) then Exit For
         Next
         if (n &gt; 0) then  
            output &quot;Missing closing parenthesis... Exiting transformation&quot; 
            exit sub 
         end if
         sNewPhysOptions = sNewPhysOptions &amp; Mid(sPartOptions, pos + 1)
         sPartOptions = Left(sPartOptions, pos - 1)
         Dim sPart, sNewPart
         sNewPart = &quot;&quot;
         For each sPart in Split(sPartOptions, &quot;,&quot;)
            pos = InStr(LCase(sPart), &quot;part&quot;)
            if (pos &gt; 0) then
               if (LCase(Mid(sPart, pos, Len(&quot;partition&quot;))) &lt;&gt; &quot;partition&quot;) then
                  sPart = Left(sPart, pos - 1) &amp; &quot;partition&quot; &amp; Mid(sPart, pos + Len(&quot;part&quot;))
               end if

               pos = InStr(LCase(sPart), &quot;values&quot;)
               if (pos &gt; 0) then
                  sPart = Left(sPart, pos - 1) &amp; &quot;ending at&quot; &amp; Mid(sPart, pos + Len(&quot;values&quot;))
               end if
            end if
            sNewPart = sNewPart &amp; &quot;, &quot; &amp; sPart
         Next
         sNewPart = &quot;partition by range (&quot; &amp; Mid(sNewPart, 3) &amp; &quot;)&quot;
         sNewPhysOptions = Replace(sNewPhysOptions, &quot;##PartitionOptions##&quot;, sNewPart)
         obj.PhysicalOptions = sNewPhysOptions
      end if
   end if

End Sub</a:MethodScript>
</o:MethodTargetItem>
</c:Categories>
</o:TypedCategoryTargetItem>
