<o:TemplateTargetItem Id="o512">
<a:ObjectID>77F0F3E9-3B2F-48C2-B15B-F010D86CBE4A</a:ObjectID>
<a:Name>alterOptionsTemplate</a:Name>
<a:TemplateTargetItem.Value>alter table [%QUALIFIER%]%TABLE%\n
.if(%NEW.WithRestrictOnDrop%!=%OLD.WithRestrictOnDrop%)
.if(%NEW.WithRestrictOnDrop%==&quot;&quot;)
   drop restrict on drop
.else
   add restrict on drop
.endif(&quot;\n&quot;)
.endif(&quot;\n&quot;)
.if(%NEW.Validproc%!=%OLD.Validproc%)
.if(%NEW.Validproc%==&quot;&quot;)
   validproc null
.else
   validproc %Validproc%
.endif(&quot;\n&quot;)
.endif(&quot;\n&quot;)
.if(%NEW.Audit%!=%OLD.Audit%)
   audit %Audit%
.endif(&quot;\n&quot;)
.if(%NEW.DataCapture%!=%OLD.DataCapture%)
   data capture %DataCapture%
.endif(&quot;\n&quot;)
</a:TemplateTargetItem.Value>
