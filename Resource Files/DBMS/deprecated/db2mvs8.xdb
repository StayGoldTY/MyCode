<o:TextTargetItem Id="o82">
<a:ObjectID>0A61AECB-08C5-447B-AE02-88BBBDBA3A21</a:ObjectID>
<a:Name>SqlColPartDef</a:Name>
<a:TextTargetItem.Value>{VAR ID, VAL ...}

select
   t.creator concat t.name,
   case(c.partkey_colseq) when 1 then &#39;&#39; else &#39;, &#39; end
   concat c.name 
   concat case(c.partkey_ordering) when &#39;A&#39; then &#39; asc&#39; else &#39; desc&#39; end as coln
from 
   sysibm.systables t
   join sysibm.syscolumns c on (c.tbname = t.name and c.tbcreator = t.creator and c.partkey_colseq &gt; 0)
where 1 = 1
[  and t.name = %.q:TABLE%]
[  and t.creator = %.q:OWNER%]
order by 1, c.partkey_colseq</a:TextTargetItem.Value>
