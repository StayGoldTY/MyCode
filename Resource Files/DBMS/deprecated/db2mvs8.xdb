<o:MethodTargetItem Id="o348">
<a:ObjectID>F38E995C-8150-48B2-A186-5A67A8CD33D4</a:ObjectID>
<a:Name>AutoFixMaterializedViews</a:Name>
<a:CreationDate>1286799775</a:CreationDate>
<a:Creator>mledier</a:Creator>
<a:ModificationDate>1286898944</a:ModificationDate>
<a:Modifier>mledier</a:Modifier>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:MethodScript>Sub %Method%(obj)

   dim S
   for each S in obj.Views
      if Not(S.IsShortcut or S.Replica) then
         if (S.Type = &quot;MATERIALIZED&quot;) then
            S.Stereotype = &quot;MaterializedView&quot;
         end if
      end if
   next

   For each S in obj.Packages
      if Not(S.IsShortcut or S.Replica) then
         %Method% S
      end if
   next
   
End Sub


</a:MethodScript>
</o:MethodTargetItem>
<o:MethodTargetItem Id="o349">
<a:ObjectID>10BB8E5C-8523-4CDA-9870-9D2992717E82</a:ObjectID>
<a:Name>IndexUpdatePartitionOptions</a:Name>
<a:CreationDate>1286810351</a:CreationDate>
<a:Creator>mledier</a:Creator>
<a:ModificationDate>1286898944</a:ModificationDate>
<a:Modifier>mledier</a:Modifier>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:MethodScript>Sub %Method%(obj)

   Dim P, T, I
   For each T in obj.Tables
      if Not(T.IsShortcut or T.Replica) then
         For each I in T.Indexes
            if (I.HasPhysicalOption(&quot;cluster&quot;)) then 
               I.ExecuteCustomMethod(&quot;%CurrentTargetCode%.UpdatePartitionOptions&quot;)
            end if
         Next
      end if
   Next
   
   For each P in obj.Packages
      if Not(P.IsShortcut or P.Replica) then
         %Method% P
      end if
   Next

End Sub</a:MethodScript>
</o:MethodTargetItem>
