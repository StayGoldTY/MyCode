<a:Computed>1</a:Computed>
<a:GetMethodScript>Function %Get%(obj)
   Dim sDttp, pos1, pos2
   sDttp = obj.GetExtendedAttribute(&quot;As&quot;)
   pos1 = InStr(sDttp, &quot;(&quot;)
   pos2 = InStrRev(sDttp, &quot;)&quot;)
   
   if (pos1 = 0) or (pos2 = 0) then
      %Get% = &quot;&quot;
   else
      %Get% = Mid(sDttp, pos1 + 1, pos2 - pos1 - 1)
   end if
End Function</a:GetMethodScript>
<a:SetMethodScript>Sub %Set%(obj, value)
   Dim D, pos
   D = obj.GetExtendedAttribute(&quot;As&quot;)
   pos = InStr(D, &quot;(&quot;)
   
   if ((pos = 0) and (value &lt;&gt; &quot;&quot;)) then
      obj.SetExtendedAttribute &quot;As&quot;, D &amp; &quot;(&quot; &amp; value &amp; &quot;)&quot;
   elseif ((pos &gt; 0) and (value &lt;&gt; &quot;&quot;)) then
      obj.SetExtendedAttribute &quot;As&quot;, Left(D, pos) &amp; value &amp; &quot;)&quot;
   elseif ((pos &gt; 0) and (value = &quot;&quot;)) then
      obj.SetExtendedAttribute &quot;As&quot;, Left(D, pos - 1)
   end if
End Sub</a:SetMethodScript>
<a:ValidateDataType>0</a:ValidateDataType>
