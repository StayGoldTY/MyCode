<a:ObjectID>20E61AC8-7695-4D00-A4E6-96902DB8FE66</a:ObjectID>
<a:Name>SqlIdxSinglePartDef</a:Name>
<a:TextTargetItem.Value>{VAR, VAL}

select
   rtrim(i.creator) concat rtrim(i.name) concat varchar(p.partition),
   case p.stortype 
      when &#39;E&#39; then &#39; using vcat &#39; concat p.vcatname 
      when &#39;I&#39; then &#39; using stogroup &#39; concat p.storname 
         concat case(p.pqty) when -1 then &#39;&#39; else &#39; priqty &#39; concat varchar(p.pqty * 4) end
         concat case(p.sqty) when -1 then &#39;&#39; else &#39; secqty &#39; concat varchar(p.sqty * 4) end
         concat case i.eraserule when &#39;Y&#39; then &#39; erase yes&#39; when &#39;N&#39; then &#39; erase no&#39; else &#39;&#39; end
      else &#39;&#39; end
   concat &#39; freepage &#39; concat varchar(p.freepage)
   concat &#39; pctfree &#39; concat varchar(p.pctfree)
   concat &#39; gbpcache &#39; concat case p.gbpcache when &#39;A&#39; then &#39;all&#39; when &#39;N&#39; then &#39;none&#39; else &#39;changed&#39; end
   concat case when ((i.clustering = &#39;Y&#39;) and (p.partition = 0)) then &#39; cluster&#39; else &#39;&#39; end as coln
from 
   sysibm.sysindexes i
      left join sysibm.sysindexpart p on (p.ixname=i.name and p.ixcreator=i.creator)
where 1 = 1
[ and i.tbcreator = %.q:OWNER%]
[ and i.tbname = %.q:TABLE%]
[ and i.name = %.q:INDEX%]
[ and i.dbname=%.q:CATALOG%]
order by 1</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
</o:TextTargetItem>
<o:TextTargetItem Id="o113">
<a:ObjectID>C8FB6E1F-3AB8-42D0-8F8C-057020F10D45</a:ObjectID>
<a:Name>SqlIdxMultiPartDef</a:Name>
<a:TextTargetItem.Value>{VAR ID, VAL EX ...}

select
   rtrim(i.creator) concat rtrim(i.name),
   case when (t.partition &gt; 1) then &#39;, &#39; else &#39;&#39; end
   concat &#39;partition &#39; concat varchar(t.partition) concat &#39; ending at (&#39; concat varchar(t.limitkey) concat &#39;)&#39;
   concat &#39; %SqlIdxSinglePartDef.&#39; concat rtrim(i.creator) concat rtrim(i.name) concat varchar(t.partition) concat &#39;%&#39; as coln
from 
   sysibm.sysindexes i
   join sysibm.systablepart t on (t.ixname=i.name and t.ixcreator=i.creator)
where i.indextype != &#39;P&#39;
[ and i.tbcreator = %.q:OWNER%]
[ and i.tbname = %.q:TABLE%]
[ and i.name = %.q:INDEX%]
[ and i.dbname=%.q:CATALOG%]
order by 1, t.partition</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
