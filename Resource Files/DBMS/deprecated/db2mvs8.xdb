<o:TextTargetItem Id="o273">
<a:ObjectID>2CCFEA98-F996-46D2-8559-2C076A695845</a:ObjectID>
<a:Name>SqlProcParamsList</a:Name>
<a:TextTargetItem.Value>{VAR ID, VAL ...}

select
   replace(p.schema concat &#39;.&#39; concat p.name concat &#39;.&#39; concat p.specificname, &#39; &#39;, &#39;&#39;),
   case when (q.ordinal = 1) then &#39;&#39; else &#39;, &#39; end
   concat case when (p.routinetype = &#39;P&#39;) then case (q.rowtype) when &#39;P&#39; then &#39;in &#39; when &#39;O&#39; then &#39;out &#39; when &#39;B&#39; then &#39;inout &#39; else &#39;&#39; end else &#39;&#39; end 
   concat coalesce(q.parmname concat &#39; &#39;, &#39;&#39;) 
   concat q.typename
   concat case when (d.length = 0) then &#39;(&#39; concat rtrim(char(q.length)) concat rtrim(coalesce(&#39;, &#39; concat char(nullif(q.scale, 0)), &#39;&#39;)) concat &#39;)&#39; else &#39;&#39; end
from
   sysibm.sysroutines p
   left outer join sysibm.sysparms q on (p.schema=q.schema and p.name=q.name and p.specificname=q.specificname)
   left outer join sysibm.sysdatatypes d on (d.name = q.typename and d.schema = q.typeschema)
where
   q.rowtype in (&#39;P&#39;, &#39;O&#39;, &#39;B&#39;)
[  and p.schema = %.q:OWNER%]
[  and p.name = %.q:PROC%]
order by 1, q.ordinal
</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
</o:TextTargetItem>
</c:Categories>
