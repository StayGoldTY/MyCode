<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, SQNC, OPTIONS}

select
   s.schema,
   s.name,
   &#39;as &#39; concat case(s.datatypeid) 
    when 500 then &#39;smallint&#39; 
    when 492 then &#39;bigint&#39; 
    when 484 then &#39;decimal(&#39; concat rtrim(char(int(s.precision))) concat &#39;)&#39;
    when 496 then &#39;integer&#39; 
    else &#39;integer&#39; 
   end
   concat &#39; increment by &#39; concat rtrim(char(int(s.increment)))
   concat &#39; start with &#39; concat rtrim(char(int(s.start)))
   concat &#39; maxvalue &#39; concat rtrim(char(int(s.maxvalue)))
   concat &#39; minvalue &#39; concat rtrim(char(int(s.minvalue)))
   concat case s.cycle when &#39;N&#39; then &#39; no cycle&#39; else &#39; cycle&#39; end
   concat case s.cache when 0 then &#39; no cache&#39; else &#39; cache &#39; concat rtrim(char(int(s.cache))) end
   concat case s.order when &#39;N&#39; then &#39; no order&#39; else &#39; order&#39; end as coln
from
   sysibm.syssequences s
where s.seqtype = &#39;S&#39;
[  and s.schema = %.q:SCHEMA%]
order by 1, 2
</a:TextTargetItem.Value>
