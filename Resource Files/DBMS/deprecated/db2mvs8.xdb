<a:Name>SqlStatistics</a:Name>
<a:TextTargetItem.Value>[%ISLONGDTTP%?{ NullValuesRate,  AverageLength }
select
[%ISMAND%?0: (count(*) - count(%COLUMN%)) * 100 / count(*)] as null_values,
avg(length(%COLUMN%)) as average_length
from [%QUALIFIER%]%TABLE%
:{ NullValuesRate, DistinctValues, AverageLength }
select
[%ISMAND%?       0: (count(*) - count(%COLUMN%)) * 100 / count(*)] as null_values,
[%ISBLOBDTTP%?0:count(distinct %COLUMN%)] as distinct_values,
[%ISVARDTTP%? avg(length(%COLUMN%)):[%ISBLOBDTTP%?avg(length(%COLUMN%)):0]] as average_length
from [%QUALIFIER%]%TABLE%]</a:TextTargetItem.Value>
