<o:TextTargetItem Id="o134">
<a:ObjectID>85E2144B-6289-4E6F-A507-148AB6F0BFF5</a:ObjectID>
<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, TABLE, COLUMN, DOMAIN, DTTPCODE, LENGTH, PREC, NOTNULL (N=&#39;NOT NULL&#39;, *=NULL), WITHDEFAULT, DEFAULT, IDENTITY, ExtGeneratedAs, @OBJTLABL, SecurityLabel, NO}

select
   c.tbcreator,
   c.tbname,
   c.name,
   case c.coltype when &#39;DISTINCT&#39; then c.typename else &#39;&#39; end,
   case c.coltype when &#39;DISTINCT&#39; then (select d.sourcetype from sysibm.sysdatatypes d where metatype=&#39;T&#39; 
      and d.owner = c.typeschema and d.name = c.typename) else c.coltype end,
   case c.length2 when 0 then c.length else c.length2 end,
   c.scale,
   c.nulls,
   case(c.nulls concat c.default)
      when &#39;NN&#39; then &#39;&#39;
      when &#39;YY&#39; then &#39;&#39;
      when &#39;YN&#39; then &#39;&#39;
      when &#39;NA&#39; then &#39;&#39;
      when &#39;ND&#39; then &#39;&#39;
      when &#39;NI&#39; then &#39;&#39;
      when &#39;NJ&#39; then &#39;&#39;
      else &#39;WITH DEFAULT&#39;
   end as colnA,
   case when (c.default = &#39;1&#39; or c.default = &#39;5&#39;) then &#39;&#39;&#39;&#39; concat varchar(c.defaultvalue, 250) concat &#39;&#39;&#39;&#39; else varchar(c.defaultvalue, 250) end as colnB,
   case when (c.default = &#39;I&#39; or c.default = &#39;J&#39;) then &#39;identity&#39; else &#39;&#39; end,
   case when (c.default = &#39;D&#39; or c.default = &#39;J&#39;) then &#39;by default&#39; when (c.default = &#39;I&#39; or c.default = &#39;A&#39;) then &#39;always&#39; else &#39;&#39; end as colnC,
   c.remarks,
   case when (c.default = &#39;L&#39;) then &#39;True&#39; else &#39;False&#39; end,
   c.colno
from
   sysibm.syscolumns c
where
   c.colno &gt; 0
   [and c.tbcreator=%.q:OWNER%]
   [and c.tbname=%.q:TABLE%]
order by
   1, 2, c.colno
</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>1254729562</a:ModificationDate>
<a:Modifier>mledier</a:Modifier>
<a:Comment>SQL query to list objects</a:Comment>
</o:TextTargetItem>
