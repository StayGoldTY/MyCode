<a:GetMethodScript>Function %Get%(obj)
   Dim sDttp, pos

   sDttp = obj.GetExtendedAttribute(&quot;As&quot;)
   pos = InStr(sDttp, &quot;(&quot;)
   if (pos = 0) then
      %Get% = sDttp
   else
      %Get% = Trim(Left(sDttp, pos - 1))
   end if

End Function</a:GetMethodScript>
<a:SetMethodScript>Sub %Set%(obj, value)
   Dim sOldDttp, sOldLength, bAllowLength, sNewDttp

   sNewDttp = value
   sOldDttp = obj.GetExtendedAttribute(&quot;As&quot;) &#39;current datatype
   bAllowLength = obj.EvaluateTextFor(&quot;%allowLength%&quot;)
   if (bAllowLength) then
      sOldLength = obj.GetExtendedAttribute(&quot;AsDatatypeLength&quot;)
   else
      sOldLength = &quot;&quot;
   end if
   
   &#39; set new type (1/2)
   obj.SetExtendedAttribute &quot;As&quot;, sNewDttp
   if (bAllowLength) then
      &#39; evaluate with new type
      bAllowLength = obj.EvaluateTextFor(&quot;%allowLength%&quot;)
      if ((bAllowLength) and (sOldLength &lt;&gt; &quot;&quot;)) then
         obj.SetExtendedAttribute &quot;As&quot;, sNewDttp &amp; &quot;(&quot; &amp; sOldLength &amp; &quot;)&quot;
      end if
   end if

End Sub</a:SetMethodScript>
