<a:Name>SqlPartitionDef</a:Name>
<a:TextTargetItem.Value>{VAR ID, VAL ...}

select
   t.creator concat t.name,
   case(p.partition) when 1 then &#39;&#39; else &#39;,&#39; end
   concat &#39; partition &#39; concat varchar(p.partition)
   concat &#39; ending at &#39; concat &#39;(&#39; concat p.limitkey concat &#39;)&#39; as coln
from 
   sysibm.systables t
   join sysibm.systablepart p on (p.tsname = t.tsname and p.dbname = t.dbname and p.ixname = &#39;&#39; and p.partition &gt; 0)
where 1 = 1
[  and t.name = %.q:TABLE%]
[  and t.creator = %.q:OWNER%]
order by 1, p.partition
</a:TextTargetItem.Value>
