<a:Name>alterPartitionScript</a:Name>
<a:TemplateTargetItem.Value>.vbscript(%NEW.Numparts%, %NEW.NumpartsPart%, %OLD.NumpartsPart%)

Dim part, OldDef, NewDef, sAlter
sAlter = &quot;&quot;
for part = 1 to CLng(ScriptInputArray(0))
   NewDef = GetPart(ScriptInputArray(1), part)
   OldDef = GetPart(ScriptInputArray(2), part)
   if (NewDef &lt;&gt; OldDef) then
      sAlter = sAlter &amp; vbCrLf &amp; PartDef(NewDef)
   end if
next
ScriptResult = sAlter

function GetPart(PartList, Num)
   Dim Part, PartDefArray
   For each Part in Split(PartList, vbCrLf)
      PartDefArray = Split(Part, vbTab)
      if (PartDefArray(0) = CStr(Num)) then
         GetPart = Part
         Exit Function
      end if
   Next
   GetPart = &quot;&quot;
end function

function PartDef(StrPart)
   Dim PartArray
   PartArray = Split(StrPart, vbTab)
   if (UBound(PartArray) &lt;&gt; 6) then
      PartDef = &quot;&quot;
   else
      Dim sRes
      sRes = &quot;   alter partition &quot; &amp; PartArray(0)
      if (PartArray(1) &lt;&gt; &quot;&quot;) then sRes = sRes &amp; &quot; &quot; &amp; replace(replace(PartArray(1), &quot;\n&quot;, &quot; &quot;), &quot;\r&quot;, &quot;&quot;)
      if (PartArray(2) &lt;&gt; &quot;&quot;) then sRes = sRes &amp; &quot; freepage &quot; &amp; PartArray(2)
      if (PartArray(3) &lt;&gt; &quot;&quot;) then sRes = sRes &amp; &quot; pctfree &quot; &amp; PartArray(3)
      if (PartArray(4) &lt;&gt; &quot;&quot;) then sRes = sRes &amp; &quot; gbpcache &quot; &amp; PartArray(4)
      if (PartArray(5) &lt;&gt; &quot;&quot;) then sRes = sRes &amp; &quot; trackmod &quot; &amp; PartArray(5)
      if (PartArray(6) &lt;&gt; &quot;&quot;) then sRes = sRes &amp; &quot; compress &quot; &amp; PartArray(6)
      PartDef = sRes
   end if
end function
.endvbscript</a:TemplateTargetItem.Value>
