        .if (%.U:DataType% != LONG VARCHAR) and (%.U:DataType% != LONG VARGRAPHIC) and (%.U:DataType% != DATALINK) and (%Identity% == false)
IN p_%Code% %DataType%
        .endif
    .next(&quot;,&quot;)
