<a:Name>SqlPermQuery</a:Name>
<a:TextTargetItem.Value>{GRANTEE ID, COLNPERMISSION ID, COLUMN ...}
select 
  s.grantee, 
  (case s.privtype 
    when &#39;U&#39; then &#39;UPDATE&#39;
    when &#39;R&#39; then &#39;REFERENCES&#39;
    end) || 
  (case s.grantable
    when &#39;G&#39; then &#39;+&#39;
    when &#39;N&#39; then &#39;&#39;
    end) as coln,
  s.colname || &#39;,&#39;
from SYSCAT.COLAUTH s
where s.tabschema &lt;&gt; s.grantee
[and s.tabname = %.q:TABLE%]
[and s.tabschema = %.q:OWNER%]
order by 2</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:Comment>SQL query to reverse object permissions</a:Comment>
</o:TextTargetItem>
<o:TextTargetItem Id="o115">
<a:ObjectID>C4F05B29-86A0-47F2-9DFD-FDF99F2B22B2</a:ObjectID>
<a:Name>SqlChckQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, TABLE, COLUMN, CONSTNAME, CONSTRAINT}

select
   k.tbcreator,
   k.tbname,
   min(c.colname),
   k.name,
   char(k.text)
from
   sysibm.syschecks k
   join sysibm.syscolchecks c on (c.tbcreator=k.tbcreator and c.tbname=k.tbname and c.constname=k.name)
where
   k.type=&#39;C&#39;
[  and k.tbcreator = %.q:SCHEMA%]
[  and k.tbname = %.q:TABLE%]
group by k.tbcreator, k.tbname, k.name, char(k.text)
having count(*) = 1
order by 1, 2, 3
</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:Comment>SQL query to reverse object check constraints</a:Comment>
</o:TextTargetItem>
