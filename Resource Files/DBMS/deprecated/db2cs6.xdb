<a:Name>DeleteProcedure</a:Name>
<a:Code>DeleteProcedure</a:Code>
<a:Text>.if (%canDefineDeleteProcedure%)
create procedure [%QUALIFIER%]%PROC% 
    .foreach_item(Table.Columns,&quot; (&quot;,&quot;)\n&quot;, %Primary%)
IN p_%Code% %DataType%
    .next(&quot;,&quot;)
    RESULT SETS 1
    LANGUAGE SQL
BEGIN
delete from 
    .if(%Owner%)
%Owner.Code%.%Table.GeneratedName%
    .else
%Table.GeneratedName%
    .endif(&quot;\n&quot;)
    .foreach_item(Table.Columns,&quot;where &quot;,&quot;;\n&quot;, %Primary%)
%GeneratedName% = p_%Code%
    .next(&quot; and &quot;)
END
.endif</a:Text>
<a:CreationDate>1138617083</a:CreationDate>
<a:Creator>fhillard</a:Creator>
<a:ModificationDate>1152528956</a:ModificationDate>
<a:Modifier>fhillard</a:Modifier>
<a:Comment>Procedure template that generates a SQL delete statement.</a:Comment>
<a:ProcedureName>%Table.Name%_%ProcedureTemplate.Name%</a:ProcedureName>
