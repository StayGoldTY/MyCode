<o:MethodTargetItem Id="o282">
<a:ObjectID>8C4C4930-076A-4271-A88D-CA022AB17B6F</a:ObjectID>
<a:Name>IncludeColumnListColumnEdit</a:Name>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:MethodScript>Sub %Method%(obj)
  &#39; Initialization
  dim Model: set Model = obj.Model
  dim Table: set Table = obj.Table
  dim Column
 
  &#39; Build seleted list of column
  dim oldVal
  oldVal = obj.GetExtendedAttribute(&quot;%CurrentTargetCode%.IncludeColumnListColumn&quot;)
  dim Selected: set Selected = Model.CreateSelection
  dim ColumnArray
  ColumnArray = split(oldVal, &quot;,&quot;)
  dim iterColumn
  for each iterColumn in ColumnArray
    for each Column in Table.Columns
      if (Column.Code = Trim(iterColumn)) then
        Selected.Objects.Add(Column)
      end if
    next
  next

  &#39; Build seletion list of column
  dim Selection: set Selection = Model.CreateSelection
  dim IndexColumn
  dim found
  for each Column in Table.Columns
    found = 0
    for each IndexColumn in obj.IndexColumns
      if (Column = IndexColumn.Column) then
        found = 1
      end if
    next
    if (found = 0) then
      Selection.Objects.Add(Column)
    end if
  next

  Selected.ShowObjMultiSelection Selection, &quot;Objects&quot;
  
  &#39; browse new selected column and build the new value
  dim newVal
  dim FisrtLoop
  FisrtLoop = 1
  for each Column in Selected.Objects
    if (FisrtLoop = 1) then
      FisrtLoop = 0
    else
      newVal = newVal &amp; &quot;,&quot;
    end if
    newVal = newVal &amp; Column.Code
  next
  obj.SetExtendedAttribute &quot;%CurrentTargetCode%.IncludeColumnListColumn&quot;, newVal
 
End Sub</a:MethodScript>
</o:MethodTargetItem>
</c:Categories>
</o:TypedCategoryTargetItem>
</c:Categories>
