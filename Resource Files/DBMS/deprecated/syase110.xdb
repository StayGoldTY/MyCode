<a:ObjectID>FDEB92F7-FFE5-427B-8771-CC07FDFF74EB</a:ObjectID>
<a:Name>SqlOptsQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, TABLE, CONSTNAME, OPTIONS}

select
   u.name,
   o.name,
   i.name,
   &#39; on &#39; + s.name
from
   [%CATALOG%.]dbo.sysusers u,
   [%CATALOG%.]dbo.sysobjects o,
   [%CATALOG%.]dbo.sysindexes i,
   [%CATALOG%.]dbo.syssegments s
where (i.status2&amp;2) = 2
   and (u.uid = o.uid)
   and (i.id = o.id)
   and (i.segment = s.segment)
[  and u.name = %.q:SCHEMA%]
[  and o.name = %.q:TABLE%]
</a:TextTargetItem.Value>
