<o:TextTargetItem Id="o125">
<a:ObjectID>3901355F-03D1-4244-9782-638D3804CD29</a:ObjectID>
<a:Name>SqlPermQuery</a:Name>
<a:TextTargetItem.Value>{GRANTEE, BITMAPCOLN, COLNPERMISSION (204193=&#39;SELECT+&#39;, 204197=&#39;UPDATE+&#39;, 205193=&#39;SELECT&#39;, 205197=&#39;UPDATE&#39;, *=&#39;&#39;)}

select 
   g.name,
   s.action + 1000*s.protecttype,
   convert(integer, s.columns)
from 
   [%CATALOG%.]dbo.sysprotects s, 
   [%CATALOG%.]dbo.sysusers u, 
   [%CATALOG%.]dbo.sysusers g, 
   [%CATALOG%.]dbo.sysobjects o
where
   s.action in (193, 197)
   and s.columns &lt;&gt; 1 
   and s.id = o.id 
   and g.uid = s.uid
   and o.uid = u.uid
[  and  o.name = %.q:TABLE%]
[  and u.name = %.q:OWNER%]
order by g.name
</a:TextTargetItem.Value>
