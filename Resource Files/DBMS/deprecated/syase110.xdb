<a:ObjectID>E8B1B384-A54C-430D-A27F-E271BAD40B6E</a:ObjectID>
<a:Name>SqlPermQuery</a:Name>
<a:TextTargetItem.Value>{GROUP ID, PRIVILEGE ...(198=&#39;CREATE TABLE+,&#39;, 203=&#39;CREATE DATABASE+,&#39;, 207=&#39;CREATE VIEW+,&#39;, 221=&#39;CREATE TRIGGER+,&#39;, 222=&#39;CREATE PROCEDURE+,&#39;, 233=&#39;CREATE DEFAULT+,&#39;, 236=&#39;CREATE RULE+,&#39;, 1198=&#39;CREATE TABLE,&#39;, 1203=&#39;CREATE DATABASE,&#39;, 1207=&#39;CREATE VIEW,&#39;, 1221=&#39;CREATE TRIGGER,&#39;, 1222=&#39;CREATE PROCEDURE,&#39;, 1233=&#39;CREATE DEFAULT,&#39;, 1236=&#39;CREATE RULE,&#39;, 2198=&#39;CREATE TABLE-,&#39;, 2203=&#39;CREATE DATABASE-,&#39;, 2207=&#39;CREATE VIEW-,&#39;, 2221=&#39;CREATE TRIGGER-,&#39;, 2222=&#39;CREATE PROCEDURE-,&#39;, 2233=&#39;CREATE DEFAULT-,&#39;, 2236=&#39;CREATE RULE-,&#39;, *=&#39;&#39;)}

select g.name, s.action + 1000*s.protecttype
from 
   [%CATALOG%.]dbo.sysprotects s, 
   [%CATALOG%.]dbo.sysusers g
where 
   s.action in (198, 203, 207, 221, 222, 233, 236)
   and g.uid = g.gid
   and g.gid not in (select r.lrid from [%CATALOG%.]dbo.sysroles r)
   and g.uid = s.uid
order by g.name
</a:TextTargetItem.Value>
