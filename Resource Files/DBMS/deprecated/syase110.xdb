<a:Text>.if (%canDefineDeleteProcedure%)
create procedure [%QUALIFIER%]%PROC% 
    .foreach_item(Table.Columns,&quot; &quot;,&quot; as\n&quot;, %Primary%)
@%Code% %DataType%
