<a:Name>SqlOptsQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, TABLE, INDEX, OPTIONS}

select
   u.name,
   o.name,
   i.name,
    &#39; on &#39; + s.name
from
   master.dbo.syssegments s,
   [%CATALOG%.]dbo.sysindexes i,
   [%CATALOG%.]dbo.sysobjects o,
   [%CATALOG%.]dbo.sysusers u
where
   u.uid = o.uid
   and i.id = o.id and i.indid != 0
   and i.segment = s.segment
[  and u.name = %.q:SCHEMA%]
[  and o.name = %.q:TABLE%]
[  and i.name = %.q:INDEX%]
</a:TextTargetItem.Value>
