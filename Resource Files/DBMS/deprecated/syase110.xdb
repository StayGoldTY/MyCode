   u.scale,
   u.allownulls,
   u.ident
from
   [%CATALOG%.]dbo.systypes u,
   [%CATALOG%.]dbo.systypes i
where
   u.type=i.type
   and u.usertype&gt;=100
   and i.usertype&lt;100
   and i.usertype not in (18, 24, 25, 80)
order by
   1,2</a:TextTargetItem.Value>
