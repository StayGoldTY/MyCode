<a:ObjectID>F73F889A-A0BD-4D1A-891B-CDE2E85B3DBE</a:ObjectID>
<a:Name>SqlColnListQuery</a:Name>
<a:TextTargetItem.Value>{OWNER ID, VIEW ID, VIEWCOLN ...}

select
   u.name,
   o.name,
   c.name + &#39;,&#39;,
   c.colid
from
   [%CATALOG%.]dbo.sysusers u,
   [%CATALOG%.]dbo.sysobjects o,
   [%CATALOG%.]dbo.syscolumns c
where
   o.uid = u.uid and o.type=&#39;V&#39;
   and c.id = o.id
[  and u.name = %.q:SCHEMA%]
[  and o.name = %.q:VIEW%]
order by 1, 2, 4
</a:TextTargetItem.Value>
