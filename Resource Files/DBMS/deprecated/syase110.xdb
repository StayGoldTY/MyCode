<a:ObjectID>9BA47F82-E516-4C99-8CAE-46B705AEB24F</a:ObjectID>
<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, TABLE, S, COLUMN, DTTPCODE, LENGTH, SIZE, PREC, NOTNULL(8=&#39;NULL&#39;, *=&#39;NOTNULL&#39;), IDENTITY(128=&#39;identity&#39;, *=&#39;&#39;), DOMAIN}

select
   u.name,
   o.name,
   c.colid,
   c.name,
   s.name,
   c.prec,
   c.length,
   c.scale,
   c.status &amp; 8,
   c.status &amp; 128,
   &#39;&#39;
from
   [%CATALOG%.]dbo.sysusers u,
   [%CATALOG%.]dbo.sysobjects o,
   [%CATALOG%.]dbo.syscolumns c,
   [%CATALOG%.]dbo.systypes s
where
   u.uid = o.uid
   and c.id = o.id
   and c.usertype = s.usertype
   and s.usertype &lt; 100
   and c.usertype &gt;= 0
   and c.name is not NULL
[  and u.name = %.q:SCHEMA%]
[  and o.name=%.q:TABLE%]
union select
   u.name,
   o.name,
   c.colid,
   c.name,
   s2.name,
   c.prec,
   c.length,
   c.scale,
   c.status &amp; 8,
   c.status &amp; 128,
   s1.name
from
   [%CATALOG%.]dbo.sysusers u,
   [%CATALOG%.]dbo.sysobjects o,
   [%CATALOG%.]dbo.syscolumns c,
   [%CATALOG%.]dbo.systypes s1,
   [%CATALOG%.]dbo.systypes s2
where
   u.uid = o.uid
   and c.id = o.id
   and c.usertype = s1.usertype
   and s2.usertype = (select min(usertype) from [%CATALOG%.]dbo.systypes where type=s2.type)
   and s1.usertype &gt;= 100
   and s2.usertype &lt; 100
   and s2.type=s1.type
   and c.usertype &gt;= 0
   and c.name is not NULL
[  and u.name = %.q:SCHEMA%]
[  and o.name=%.q:TABLE%]
order by 1,2, 3</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:Comment>SQL query to list objects</a:Comment>
</o:TextTargetItem>
<o:TextTargetItem Id="o119">
<a:ObjectID>1959EB24-6374-4274-A3DB-52B182082984</a:ObjectID>
<a:Name>SqlChckQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, TABLE, COLUMN, CONSTNAME, CONSTRAINT, DEFAULTNAME (&#39;0&#39;=&#39;&#39;), RULENAME(&#39;0&#39;=&#39;&#39;)}

select distinct
   u.name,
   o.name,
   c.name,
   r.name,
   reverse(substring(reverse(t.text), 1, char_length(t.text)-char_length(r.name)-19)),
   convert(char(20), c.cdefault),
   convert(char(20), c.domain)
from
   [%CATALOG%.]dbo.sysusers u,
   [%CATALOG%.]dbo.sysobjects o,
   [%CATALOG%.]dbo.syscolumns c,
   [%CATALOG%.]dbo.sysconstraints k,
   [%CATALOG%.]dbo.sysobjects r,
   [%CATALOG%.]dbo.syscomments t
where
   r.id=k.constrid
   and t.id=r.id
   and k.tableid=o.id
   and k.colid=c.colid
   and u.uid = o.uid
   and c.id=o.id
   and (o.type=&#39;U&#39; or o.type=&#39;S&#39;)
   and ((c.cdefault != 0) or (c.domain != 0))
[  and u.name = %.q:SCHEMA%]
[  and o.name = %.q:TABLE%]
[  and c.name = %.q:COLUMN%]
order by 1,2,3, k.constrid</a:TextTargetItem.Value>
