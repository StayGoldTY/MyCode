<o:TextTargetItem Id="o254">
<a:ObjectID>E239DC63-C37D-4297-A976-E8573CEDB262</a:ObjectID>
<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{TABLOWNER ID, TABLE ID, OWNER ID, TRIGGER ID, TRGEVENT ...}

select
   user_name(tb.uid),
   tb.name,
   user_name(tr.uid),
   tr.name,
   &#39;delete,&#39;
from
   [%CATALOG%.]dbo.sysobjects tb,
   [%CATALOG%.]dbo.sysobjects tr
where
   tr.type = &#39;TR&#39; and tr.id = tb.deltrig
[  and user_name(tb.uid) = %.q:TABLOWNER%]
[  and user_name(tr.uid) = %.q:SCHEMA%]
[  and tb.name = %.q:TABLE%]
union select
   user_name(tb.uid),
   tb.name,
   user_name(tr.uid),
   tr.name,
   &#39;insert,&#39;
from
   [%CATALOG%.]dbo.sysobjects tb,
   [%CATALOG%.]dbo.sysobjects tr
where
   tr.type = &#39;TR&#39; and tr.id = tb.instrig
[  and user_name(tb.uid) = %.q:TABLOWNER%]
[  and user_name(tr.uid) = %.q:SCHEMA%]
[  and tb.name = %.q:TABLE%]
union select
   user_name(tb.uid),
   tb.name,
   user_name(tr.uid),
   tr.name,
   &#39;update,&#39;
from
   [%CATALOG%.]dbo.sysobjects tb,
   [%CATALOG%.]dbo.sysobjects tr
where
   tr.type = &#39;TR&#39; and tr.id = tb.updtrig
[  and user_name(tb.uid) = %.q:TABLOWNER%]
[  and user_name(tr.uid) = %.q:SCHEMA%]
[  and tb.name = %.q:TABLE%]
order by 1, 2, 3, 4</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:Comment>SQL query to list objects</a:Comment>
</o:TextTargetItem>
<o:TextTargetItem Id="o255">
<a:ObjectID>7F49338D-AA84-45E2-ABDB-636A43F933E3</a:ObjectID>
<a:Name>SqlAttrQuery</a:Name>
<a:TextTargetItem.Value>{TABLOWNER ID, TABLE ID, OWNER ID, TRIGGER ID, SCRIPT ...}

select
   user_name(o.uid),
   o.name,
   user_name(t.uid),
   t.name,
   convert(char(255), c.text)
from
   [%CATALOG%.]dbo.sysobjects o,
   [%CATALOG%.]dbo.sysobjects t,
   [%CATALOG%.]dbo.syscomments c
where
   c.id = t.id
   and t.type = &#39;TR&#39;
   and t.id in (o.deltrig, o.instrig, o.updtrig)
   and (t.deltrig!=0 or t.instrig!=0 or t.updtrig!=0)
[  and user_name(o.uid) = %.q:TABLOWNER%]
[  and user_name(t.uid) = %.q:OWNER%]
[  and o.name = %.q:TABLE%]
[  and t.name = %.q:TRIGGER%]
order by
   1, 2, 3, 4, c.number, c.colid2, c.colid</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:Comment>SQL query to reverse object attributes</a:Comment>
