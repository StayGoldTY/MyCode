<a:TextTargetItem.Value>{OWNER, ADT, TYPE}

select
   u.user_name,
   t.type_name,
   &#39;JAVA&#39;
from
   sys.sysusertype t,
   sys.sysuserperms u
where
   t.domain_id in (25, 26)
   and t.creator = u.user_id
[  and u.user_name = %.q:SCHEMA%]
order by 1,2</a:TextTargetItem.Value>
