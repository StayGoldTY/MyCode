begin
delete from 
    .if(%Owner%)
%Owner.Code%.%Table.GeneratedName%
    .else
%Table.GeneratedName%
    .endif(&quot;\n&quot;)
    .foreach_item(Table.Columns,&quot;where &quot;,&quot;\n&quot;, %Primary%)
%GeneratedName% = @%Code%
    .next(&quot; and &quot;)
end
