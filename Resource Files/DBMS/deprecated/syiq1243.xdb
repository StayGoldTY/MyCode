<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{DOMAIN, DTTPCODE, LENGTH, PREC, CONSTRAINT, Mandatory}

select
   d.type_name,
   t.domain_name,
   d.width,
   d.scale,
   d.&quot;check&quot;,
   case(d.nulls) when &#39;Y&#39; then &#39;False&#39; when &#39;N&#39; then &#39;True&#39; else 
   (select case(setting) when &#39;On&#39; then &#39;False&#39; else &#39;True&#39; end from sys.sysoptions where &quot;option&quot;=&#39;Allow_nulls_by_default&#39;) 
   end as coln
from
   sys.sysusertype d,
   sys.sysdomain t
where
   d.domain_id=t.domain_id
   and d.domain_id not in (25, 26)
   and creator &lt;&gt; 0
order by 1</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
