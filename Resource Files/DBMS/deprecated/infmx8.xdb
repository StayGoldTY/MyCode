<a:Name>SqlAttrQuery</a:Name>
<a:TextTargetItem.Value>{OWNER ID, VIEW ID, SCRIPT ...}

select
   t.owner,
   t.tabname,
   v.viewtext,
   v.seqno
from
   informix.systables t,
   informix.sysviews v
where
   t.tabtype=&#39;V&#39;
   and t.tabid=v.tabid
   [and t.owner=%.q:SCHEMA%]
order by
   t.owner,
   t.tabname,
   v.seqno
</a:TextTargetItem.Value>
