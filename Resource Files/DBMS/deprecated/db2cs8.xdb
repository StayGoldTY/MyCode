<o:TextTargetItem Id="o126">
<a:ObjectID>53C252FF-BEDA-42E0-A4B1-C5263065BFE4</a:ObjectID>
<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, TABLE, COLUMN, DTTPCODE, LENGTH, PREC, NOTNULL, WITHDEFAULT, DEFAULT, @OBJTLABL, NO, IDENTITY, ExtGenAlways, ExtGenExpr, ExtLobOption}

select
   c.tabschema,
   c.tabname,
   c.colname,
   c.typename,
   c.length,
   c.scale,
   case c.nulls when &#39;N&#39; then &#39;not null&#39; else &#39;null&#39; end,
   case coalesce(c.default, &#39;&#39;) when &#39;&#39; then &#39;&#39; else &#39;WITH DEFAULT&#39; end,
   c.default,
   c.remarks,
   c.colno,
   case c.identity when &#39;Y&#39; then &#39;identity&#39; else &#39;&#39; end,
   case c.generated when &#39;A&#39; then &#39;TRUE&#39; else &#39;&#39; end,
   cast(substr(c.text, 4) as varchar(200)),
   case (c.logged || c.compact) 
      when &#39;YY&#39; then &#39;logged compact&#39; when &#39;NY&#39; then &#39;not logged compact&#39;
      when &#39;YN&#39; then &#39;logged not compact&#39; when &#39;NN&#39; then &#39;not logged not compact&#39;
   else &#39;&#39; end as coln
from
   syscat.columns c
where 1=1
   [and c.tabschema=%.q:OWNER%]
   [and c.tabname=%.q:TABLE%]
order by
   1, 2, colno</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>1286794256</a:ModificationDate>
<a:Modifier>mledier</a:Modifier>
<a:Comment>SQL query to list objects</a:Comment>
</o:TextTargetItem>
<o:TextTargetItem Id="o127">
<a:ObjectID>C432BD54-3C69-44BC-8F45-73B1019271D4</a:ObjectID>
<a:Name>SqlChckQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, TABLE, COLUMN, CONSTNAME, CONSTRAINT}

select
   k.tbcreator,
   k.tbname,
   min(c.colname),
   k.name,
   char(k.text)
from
   sysibm.syschecks k
   join sysibm.syscolchecks c on (c.tbcreator=k.tbcreator and c.tbname=k.tbname and c.constname=k.name)
where
   k.type=&#39;C&#39;
[  and k.tbcreator = %.q:SCHEMA%]
[  and k.tbname = %.q:TABLE%]
group by k.tbcreator, k.tbname, k.name, char(k.text)
having count(*) = 1
order by 1, 2, 3
</a:TextTargetItem.Value>
