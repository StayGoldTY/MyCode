case s.dbadmauth when &#39;Y&#39; then &#39;DBADM&#39; else &#39;&#39; end ||&#39;,&#39;||
case s.createtabauth when &#39;Y&#39; then &#39;CREATETAB&#39; else &#39;&#39; end ||&#39;,&#39;||
