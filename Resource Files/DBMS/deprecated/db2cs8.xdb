   k.tbcreator,
   k.tbname,
   k.name,
   char(k.text)
from
   sysibm.syschecks k
where
   k.type=&#39;C&#39;
   and (select count(*) from sysibm.syscolchecks c
    where c.tbcreator=k.tbcreator and c.tbname=k.tbname and c.constname=k.name) &lt;&gt; 1
[ and k.tbcreator = %.q:OWNER%]
[ and k.tbname = %.q:TABLE%]
order by 1, 2, 3
