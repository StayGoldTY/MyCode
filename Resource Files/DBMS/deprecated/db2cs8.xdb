<a:Name>CanAlterManagement</a:Name>
<a:TextTargetItem.Value>.vbscript(%OLD.DatabaseUsing%, %NEW.DatabaseUsing%)
   Dim sOld, sNew
   sOld = ScriptInputArray(0)
   sNew = ScriptInputArray(1)

   Dim NewArray, OldArray
   NewArray = Split(sNew, vbTab) &#39; (0) = database containers
   OldArray = Split(sOld, vbTab) &#39; (1) = db partition clause
   
   ScriptResult = CStr(NewArray(1) = OldArray(1))
.endvbscript
</a:TextTargetItem.Value>
