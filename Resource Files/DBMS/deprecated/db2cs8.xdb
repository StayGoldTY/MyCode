<a:TextTargetItem.Value>create trigger, end, 1
{
   begin, , 2
   {
      begin, end, 3
      case, end case, 4
      if, end if, 4
      for, end for, 4
      loop, end loop, 4
      repeat, end repeat, 4
      while, end while, 4
   }
}
create procedure, %TERMINATOR%, 1
{
   begin, end, 2
   case, end case, 4
   if, end if, 4
   for, end for, 4
   loop, end loop, 4
   repeat, end repeat, 4
   while, end while, 4
}</a:TextTargetItem.Value>
