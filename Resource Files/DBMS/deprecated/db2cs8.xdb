<o:TextTargetItem Id="o249">
<a:ObjectID>9CD82563-72B5-4D24-AEB5-EF2B2E5F73D0</a:ObjectID>
<a:Name>SqlProcParamsList</a:Name>
<a:TextTargetItem.Value>{VAR ID, VAL ...}

select
   replace(p.routineschema concat &#39;.&#39; concat p.routinename concat &#39;.&#39; concat p.specificname, &#39; &#39;, &#39;&#39;),
   case when p.routinetype = &#39;P&#39; then
      case (q.rowtype) when &#39;P&#39; then &#39;in &#39; when &#39;O&#39; then &#39;out &#39; when &#39;B&#39; then &#39;inout &#39; else &#39;&#39; end
   else &#39;&#39; end 
   concat coalesce(q.parmname concat &#39; &#39;, &#39;&#39;) 
   concat q.typename
   concat case (d.length)
     when 0 then &#39;(&#39; concat rtrim(char(q.length)) concat rtrim(coalesce(&#39;, &#39; concat char(nullif(q.scale, 0)), &#39;&#39;)) concat &#39;)&#39;
     else &#39;&#39; end
   concat &#39;, &#39; 
from
   syscat.routines p
   join syscat.routineparms q on (p.routineschema=q.routineschema and p.routinename=q.routinename and p.specificname=q.specificname)
   join syscat.datatypes d on (d.typename = q.typename and d.typeschema = q.typeschema)
where
   q.rowtype in (&#39;P&#39;, &#39;O&#39;, &#39;B&#39;)
[  and p.routineschema = %.q:OWNER%]
[  and p.routinename = %.q:PROC%]
order by p.routineschema, p.routinename, p.specificname, q.ordinal</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
</o:TextTargetItem>
<o:TextTargetItem Id="o250">
<a:ObjectID>3F529225-E5DD-4825-9421-2930C3D71EF5</a:ObjectID>
<a:Name>SqlProcReturnTable</a:Name>
<a:TextTargetItem.Value>{VAR ID, VAL ...}

select
   replace(p.routineschema concat &#39;.&#39; concat p.routinename concat &#39;.&#39; concat p.specificname, &#39; &#39;, &#39;&#39;),
   case when p.routinetype = &#39;P&#39; then
      case (q.rowtype) when &#39;P&#39; then &#39;in &#39; when &#39;O&#39; then &#39;out &#39; when &#39;B&#39; then &#39;inout &#39; else &#39;&#39; end
   else &#39;&#39; end 
   concat coalesce(q.parmname concat &#39; &#39;, &#39;&#39;) 
   concat coalesce(nullif(rtrim(q.typeschema), &#39;SYSIBM&#39;) concat &#39;.&#39;, &#39;&#39;) concat q.typename
   concat case (d.length)
     when 0 then &#39;(&#39; concat rtrim(char(q.length)) concat rtrim(coalesce(&#39;, &#39; concat char(nullif(q.scale, 0)), &#39;&#39;)) concat &#39;)&#39;
     else &#39;&#39; end
   concat &#39;, &#39; 
from
   syscat.routines p
   join syscat.routineparms q on (p.routineschema=q.routineschema and p.routinename=q.routinename and p.specificname=q.specificname)
   join syscat.datatypes d on (d.typename = q.typename and d.typeschema = q.typeschema)
where
   q.rowtype in (&#39;R&#39;)
[  and p.routineschema = %.q:OWNER%]
[  and p.routinename = %.q:PROC%]
order by 1, q.ordinal</a:TextTargetItem.Value>
