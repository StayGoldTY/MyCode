<a:Name>dropTempTableConstraints</a:Name>
<a:TemplateTargetItem.Value>.if(%NEW.Code% == %OLD.Code%)
.if(%OLDTABL% != %NEWTABL%)
.set_value(TempName, &quot;TMP_%OLD.Code%&quot;)
.if(%.QU:NEWTABL% == %.QU:TempName%)
.foreach_item(Columns)
.if(%ExtGenAlways%)
alter table %Table.Code% 
   alter column %Code% drop expression
.endif(&quot;;\n\n&quot;)
.next
.endif()
.endif()
.endif()
