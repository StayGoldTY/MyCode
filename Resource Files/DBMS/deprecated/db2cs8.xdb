<a:ObjectID>5166A72C-B33F-4422-902A-F623C9B6C9D6</a:ObjectID>
<a:Name>alterOptions</a:Name>
<a:TemplateTargetItem.Value>alter table [%QUALIFIER%]%TABLE%\n
.if(%OLD.NotLoggedInitially%!=%NEW.NotLoggedInitially%)
   activate not logged initially
.endif(&quot;\n&quot;)
.if(%OLD.DataCapture%!=%NEW.DataCapture%)
[%DataCapture%?   data capture %DataCapture%:   data capture none]
.endif(&quot;\n&quot;)
.if(%OLD.ValueCompression%!=%NEW.ValueCompression%)
.if(%NEW.ValueCompression%)
   activate value compression
.else
   deactivate value compression
.endif()
.endif(&quot;\n&quot;)
.if(%OLD.WithRestrictOnDrop%!=%NEW.WithRestrictOnDrop%)
.if(%NEW.WithRestrictOnDrop%)
   add restrict on drop
.else
   drop restrict on drop
.endif()
.endif(&quot;\n&quot;)
</a:TemplateTargetItem.Value>
