<o:TextTargetItem Id="o281">
<a:ObjectID>36E41E89-003D-4C5E-9A81-7F49A7B039FE</a:ObjectID>
<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, SQNC, OPTIONS}

select
   s.seqschema,
   s.seqname,
   &#39;as &#39; concat (select d.name 
   concat case when (s.datatypeid = 16) then &#39;(&#39; concat replace(replace(rtrim(ltrim(replace(char(decimal(s.precision)), &#39;0&#39;, &#39; &#39;))), &#39; &#39;, &#39;0&#39;), &#39;.&#39;, &#39;&#39;) concat &#39;)&#39; else &#39;&#39; end
   from sysibm.sysdatatypes d where d.typeid = s.datatypeid)
   concat &#39; increment by &#39; concat rtrim(char(int(s.increment)))
   concat &#39; start with &#39; concat rtrim(char(int(s.start)))
   concat case s.maxvalue when 9223372036854775807 then &#39; no maxvalue&#39; else &#39; maxvalue &#39; concat replace(replace(rtrim(ltrim(replace(char(s.maxvalue), &#39;0&#39;, &#39; &#39;))), &#39; &#39;, &#39;0&#39;), &#39;.&#39;, &#39;&#39;) end
   concat &#39; minvalue &#39; concat rtrim(char(int(s.minvalue)))
   concat case s.cycle when &#39;N&#39; then &#39; no cycle&#39; else &#39; cycle&#39; end
   concat case s.cache when 0 then &#39; no cache&#39; else &#39; cache &#39; concat rtrim(char(int(s.cache))) end
   concat case s.order when &#39;N&#39; then &#39; no order&#39; else &#39; order&#39; end as coln
from
   sysibm.syssequences s
[where s.seqschema = %.q:SCHEMA%]
order by 1, 2
</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>1244640447</a:ModificationDate>
<a:Modifier>mledier</a:Modifier>
<a:Comment>SQL query to list objects</a:Comment>
</o:TextTargetItem>
