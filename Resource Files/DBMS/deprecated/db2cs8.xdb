<a:Name>SqlPermQuery</a:Name>
<a:TextTargetItem.Value>{OWNER ID, SQNC ID, GRANTEE ID, PERMISSION ...}

select
   s.seqschema,
   s.seqname,
   s.grantee,
   case s.alterauth when &#39;Y&#39; then &#39;alter&#39;   else &#39;&#39; end ||&#39;,&#39;||
   case s.alterauth when &#39;G&#39; then &#39;alter+&#39;  else &#39;&#39; end ||&#39;,&#39;||
   case s.usageauth when &#39;Y&#39; then &#39;usage&#39;  else &#39;&#39; end ||&#39;,&#39;||
   case s.usageauth when &#39;G&#39; then &#39;usage+&#39; else &#39;&#39; end ||&#39;,&#39;
   as coln
from syscat.sequenceauth s
where s.seqschema &lt;&gt; s.grantee
[and s.seqname = %.q:SQNC%]
[and s.seqschema = %.q:OWNER%]
</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
