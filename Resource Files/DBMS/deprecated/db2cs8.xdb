   case(c.colseq) when 1 then c.colname else &#39; , &#39; concat c.colname end
from
   sysibm.syscoluse c
where c.type = &#39;C&#39;
[  and c.tabschema=%.q:OWNER%]
[  and c.tabname=%.q:TABLE%]
order by 1, c.colseq
