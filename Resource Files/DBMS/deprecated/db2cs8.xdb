<o:TextTargetItem Id="o246">
<a:ObjectID>8F57C978-19D2-4B29-8476-6B0FB905DBE9</a:ObjectID>
<a:Name>SqlAttrQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, PROC, SCRIPT EX}

select 
   p.routineschema,
   p.routinename,
   case when (p.language=&#39;SQL&#39;) then varchar(p.text) else 
      &#39;create &#39;
      concat case(p.routinetype) when &#39;P&#39; then &#39;procedure &#39; else &#39;function &#39; end
      concat p.routinename
      concat &#39;(%SqlProcParamsList.&#39; concat replace(p.routineschema concat &#39;.&#39; concat p.routinename concat &#39;.&#39; concat p.specificname, &#39; &#39;, &#39;&#39;) concat &#39;%)&#39;
      concat case (p.functiontype) 
         when &#39;T&#39; then &#39; returns table(%SqlProcReturnTable.&#39; concat replace(p.routineschema concat &#39;.&#39; concat p.routinename concat &#39;.&#39; concat p.specificname, &#39; &#39;, &#39;&#39;) concat &#39;%)&#39;
         when &#39;S&#39; then &#39; returns &#39; concat coalesce(nullif(rtrim(p.return_typeschema), &#39;SYSIBM&#39;) concat &#39;.&#39;, &#39;&#39;) concat p.return_typename
         when &#39;R&#39; then &#39; returns &#39; concat coalesce(nullif(rtrim(p.return_typeschema), &#39;SYSIBM&#39;) concat &#39;.&#39;, &#39;&#39;) concat p.return_typename
         when &#39;C&#39; then &#39; returns &#39; concat coalesce(nullif(rtrim(p.return_typeschema), &#39;SYSIBM&#39;) concat &#39;.&#39;, &#39;&#39;) concat p.return_typename
         else &#39;&#39; end
      concat chr(13) concat chr(10)
      concat case when left(p.specificname, 3) = &#39;SQL&#39; then &#39;&#39; else &#39; specific &#39; concat p.specificname end
      concat chr(13) concat chr(10)
      concat case when p.origin in (&#39;Q&#39;, &#39;E&#39;) then &#39; language &#39; concat lcase(p.language) else &#39;&#39; end
      concat case when p.origin in (&#39;E&#39;) then &#39; parameter style &#39; concat p.parameter_style else &#39;&#39; end
      concat case when p.origin in (&#39;E&#39;) then 
         &#39; external name &#39;&#39;&#39;
         concat case when p.language = &#39;JAVA&#39; 
                then p.class concat &#39;.&#39; concat left(p.implementation, locate(&#39;(&#39;, p.implementation) - 1)
                else p.implementation end
         concat &#39;&#39;&#39;&#39;
         else &#39;&#39; end
      concat case when p.origin in (&#39;U&#39;) then 
         &#39; source &#39; concat p.implementation
         concat chr(13) concat chr(10)
        else &#39;&#39; end
      concat chr(13) concat chr(10)
      concat case when p.origin in (&#39;Q&#39;, &#39;E&#39;) then case(p.deterministic) when &#39;Y&#39; then &#39; deterministic&#39; else &#39; not deterministic&#39; end else &#39;&#39; end
      concat chr(13) concat chr(10)
      concat case when p.origin in (&#39;Q&#39;, &#39;E&#39;) then case(p.external_action) when &#39;E&#39; then &#39; external action&#39; else &#39; no external action&#39; end else &#39;&#39; end
      concat chr(13) concat chr(10)
      concat case when p.origin in (&#39;Q&#39;, &#39;E&#39;) then case(p.nullcall) when &#39;Y&#39; then &#39; called on null input&#39; else &#39; returns null on null input&#39; end else &#39;&#39; end
      concat chr(13) concat chr(10)
      concat case when p.origin in (&#39;E&#39;) then case(p.scratchpad) when &#39;N&#39; then &#39; no scratchpad&#39; else &#39; scratchpad &#39; concat case(p.scratchpad_length) when -1 then &#39;&#39; else char(p.scratchpad_length) end end else &#39;&#39; end
      concat chr(13) concat chr(10)
      concat case when p.origin in (&#39;E&#39;) then case(p.finalcall) when &#39;N&#39; then &#39; no final call&#39; else &#39; final call&#39; end else &#39;&#39; end
      concat chr(13) concat chr(10)
      concat case when p.origin in (&#39;E&#39;) then case(p.parallel) when &#39;N&#39; then &#39; disallow parallel&#39; else &#39; allow parallel&#39; end else &#39;&#39; end
      concat chr(13) concat chr(10)
      concat case when p.origin in (&#39;E&#39;) then case(p.fenced) when &#39;N&#39; then &#39; not fenced&#39; else &#39; fenced&#39; end else &#39;&#39; end
      concat case when p.origin in (&#39;E&#39;) then case(p.threadsafe) when &#39;N&#39; then &#39; not threadsafe&#39; else &#39; threadsafe&#39; end else &#39;&#39; end
      concat chr(13) concat chr(10)
      concat case when p.origin in (&#39;Q&#39;, &#39;E&#39;) then case(p.sql_data_access) when &#39;C&#39; then &#39; contains sql&#39; when &#39;M&#39; then &#39; modifies sql data&#39; when &#39;N&#39; then &#39; no sql&#39; when &#39;C&#39; then &#39; reads sql data&#39; else &#39;&#39; end else &#39;&#39; end
      concat chr(13) concat chr(10)
      concat case when p.origin in (&#39;E&#39;) then case(p.dbinfo) when &#39;N&#39; then &#39; no dbinfo&#39; else &#39; dbinfo&#39; end else &#39;&#39; end
      concat chr(13) concat chr(10)
      concat case when p.origin in (&#39;Q&#39;, &#39;E&#39;) then case(p.spec_reg) when &#39;I&#39; then &#39; inherit special registers&#39; else &#39;&#39; end else &#39;&#39; end
      concat chr(13) concat chr(10)
      concat case when (p.functiontype = &#39;T&#39; and p.cardinality &gt; 0) then &#39; cardinality &#39; concat char(p.cardinality) else &#39;&#39; end
   end as coln
from
   syscat.routines p
where p.routinetype in (&#39;P&#39;, &#39;F&#39;)
   and p.origin in (&#39;E&#39;, &#39;Q&#39;, &#39;U&#39;)
[  and p.routineschema = %.q:OWNER%]
[  and p.routinename = %.q:PROC%]
</a:TextTargetItem.Value>
