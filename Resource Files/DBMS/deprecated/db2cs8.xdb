<o:TemplateTargetItem Id="o444">
<a:ObjectID>4A30DAB9-8E14-4CAE-929B-44A125102BD0</a:ObjectID>
<a:Name>alterManagementAddFile</a:Name>
<a:TemplateTargetItem.Value>alter tablespace %TABLESPACE%
.vbscript(%OLD.DatabaseUsing%, %NEW.DatabaseUsing%)
Dim sOld, sNew
sOld = ScriptInputArray(0)
sNew = ScriptInputArray(1)

   Dim NewArray, OldArray
   NewArray = Split(sNew, vbTab) &#39; (0) = database containers
   OldArray = Split(sOld, vbTab) &#39; (1) = db partition clause
   
   Dim OldContArray, OldCont, OldContItems
   Dim NewContArray, NewCont, NewContItems

   OldContArray = Split(OldArray(0), &quot;,\r\n&quot;)
   NewContArray = Split(NewArray(0), &quot;,\r\n&quot;)

   For each NewCont in NewContArray
      NewContItems = Split(NewCont, &quot;\r\n&quot;)
      For each OldCont in OldContArray
         if (NewCont = OldCont) then
            Exit For
         end if
         OldContItems = Split(OldCont, &quot;\r\n&quot;)
         if (NewContItems(0) = OldContItems(0)) then
            Exit For
         end if
      Next
      if (OldCont = &quot;&quot;) then
         sRes = sRes &amp; vbCrLf &amp; &quot;   add (&quot; &amp; NewCont &amp; &quot;) &quot; &amp; NewArray(1)
      end if
   Next

   ScriptResult = replace(replace(sRes, vbTab, &quot; &quot;), &quot;\r\n&quot;, &quot; &quot;)
.endvbscript</a:TemplateTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
</o:TemplateTargetItem>
<o:TemplateTargetItem Id="o445">
<a:ObjectID>2D698AE6-CD65-44F1-B85B-832263DFF38C</a:ObjectID>
<a:Name>alterManagementDropOrResizeFile</a:Name>
<a:TemplateTargetItem.Value>alter tablespace %TABLESPACE%
.vbscript(%OLD.DatabaseUsing%, %NEW.DatabaseUsing%)
Dim sOld, sNew
sOld = ScriptInputArray(0)
sNew = ScriptInputArray(1)

   Dim NewArray, OldArray
   NewArray = Split(sNew, vbTab) &#39; (0) = database containers
   OldArray = Split(sOld, vbTab) &#39; (1) = db partition clause
   
   Dim OldContArray, OldCont, OldContItems
   Dim NewContArray, NewCont, NewContItems

   OldContArray = Split(OldArray(0), &quot;,\r\n&quot;)
   NewContArray = Split(NewArray(0), &quot;,\r\n&quot;)

   For each OldCont in OldContArray
      OldContItems = Split(OldCont, &quot;\r\n&quot;)
      For each NewCont in NewContArray
         if (NewCont = OldCont) then
            Exit For &#39;Found a matching container, nothing to do
         End if
         NewContItems = Split(NewCont, &quot;\r\n&quot;)
         if (NewContItems(0) = OldContItems(0)) then
            sRes = sRes &amp; vbCrLf &amp; &quot;   resize (&quot; &amp; NewCont &amp; &quot;) &quot; &amp; NewArray(1)
            Exit For
         end if
      Next
      if (NewCont = &quot;&quot;) then
         sRes = sRes &amp; vbCrLf &amp; &quot;   drop (&quot; &amp; OldContItems(0) &amp; &quot;) &quot; &amp; NewArray(1)
      end if
   Next
   ScriptResult = replace(replace(sRes, vbTab, &quot; &quot;), &quot;\r\n&quot;, &quot; &quot;)
.endvbscript</a:TemplateTargetItem.Value>
