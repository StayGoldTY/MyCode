<o:TextTargetItem Id="o305">
<a:ObjectID>DD490E80-7E2B-479D-9E5E-3C23DE5F8E8D</a:ObjectID>
<a:Name>SqlAttrQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, MATERIALIZEDVIEW, Create TPL, COMMENT}

select
   v.viewschema,
   v.viewname,
   cast(v.text as long varchar),
   t.remarks
from 
   syscat.tables t
   join syscat.views v on (t.tabschema = v.viewschema and t.tabname = v.viewname)
where
   t.type in (&#39;S&#39;)
[  and t.tabschema = %.q:OWNER%]
[  and t.tabname = %.q:MATERIALIZEDVIEW%]</a:TextTargetItem.Value>
