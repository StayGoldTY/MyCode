<a:Name>SqlDimOptsQuery</a:Name>
<a:TextTargetItem.Value>{VAR ID, VAL EX ...}

select
    c.tabschema concat c.tabname,
    case(select count(*) from sysibm.syscoluse c1 where c1.tabname = c.tabname and c1.dimension = c.dimension) 
        when 1 then c.colname 
        else &#39;(%SqlHierarchyQry.&#39; concat c.tabschema concat c.tabname concat &#39;d&#39; concat rtrim(char(c.dimension)) concat &#39;%)&#39; end
    concat case(c.dimension) when 1 then &#39;&#39; else &#39; , &#39; end as coln
from 
   sysibm.syscoluse c
where c.colseq=1
   and c.type = &#39;C&#39;
[  and c.tabschema=%.q:OWNER%]
[  and c.tabname=%.q:TABLE%]
order by 1, c.dimension desc
</a:TextTargetItem.Value>
