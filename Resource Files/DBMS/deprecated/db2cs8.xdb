<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{OWNER ID, TABLE ID, ISPKEY ID, CONSTNAME ID, COLUMNS ...}

select
   k.tbcreator,
   k.tbname,
   case(k.constraintyp) when &#39;P&#39; then &#39;yes&#39; else &#39;&#39; end,
   k.name,
   case c.colseq when 1 then &#39;&#39; else &#39;, &#39; end concat c.colname
from 
   sysibm.systabconst k
   join sysibm.syskeycoluse c on (c.tbcreator=k.tbcreator and c.tbname=k.tbname and c.constname = k.name)
where 
   k.constraintyp in (&#39;P&#39;, &#39;U&#39;)
[  and k.tbcreator = %.q:SCHEMA%]
[  and k.tbname = %.q:TABLE%]
order by 1, 2, 3, 4, c.colseq</a:TextTargetItem.Value>
