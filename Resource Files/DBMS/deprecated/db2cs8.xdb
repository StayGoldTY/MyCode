<a:Name>alterDistribution</a:Name>
<a:TemplateTargetItem.Value>.set_value(OldOption, &quot;[%OLD.Replicated%?replicated:%OLD.PartitioningKeyColumnListColumn%]&quot;)
.set_value(NewOption, &quot;[%NEW.Replicated%?replicated:%NEW.PartitioningKeyColumnListColumn%]&quot;)

.if(%OldOption%!=%NewOption%)
.if(%OldOption%!=&quot;&quot;)
alter table [%QUALIFIER%]%TABLE%
   drop partitioning key
.endif(&quot;;\n&quot;)
.if(%NewOption%!=&quot;&quot;)
alter table [%QUALIFIER%]%TABLE%\n
   add partitioning key (%NewOption%)[%NEW.PartitioningKeyUsingHashing%? using hashing]
.endif(&quot;;\n&quot;)
.endif(&quot;\n&quot;)
