<a:Name>SqlStatistics</a:Name>
<a:TextTargetItem.Value>{ NullValuesRate, DistinctValues, AverageLength }
select
[%ISMAND%?null:(count(*) - count(%COLUMN%)) * 100 / count(*)] as null_values,
count(distinct %COLUMN%) as distinct_values,
[%ISVARDTTP%?avg(length(%COLUMN%)) :[%ISLONGDTTP%?avg(length(%COLUMN%)) :null]] as average_length
from [%QUALIFIER%]%TABLE%</a:TextTargetItem.Value>
