<o:TextTargetItem Id="o82">
<a:ObjectID>05752125-D6C2-4DD0-84D0-E090618E1CDF</a:ObjectID>
<a:Name>SqlOptsQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, TABLE, INDEX, CLUSTER (0=&#39;&#39;), OPTIONS}

select
   u.name,
   o.name,
   i.name,
   (i.status &amp; 16),
   case ((i.status &amp; 257) + i.OrigFillFactor) when 0 then &#39;&#39; else &#39;with &#39; + replace(ltrim(&#39;&#39;
      + case when (i.status&amp;256&lt;&gt; 0) then &#39; pad_index&#39; else &#39;&#39; end
      + case (i.OrigFillFactor) when 0 then &#39;&#39; else &#39; fillfactor=&#39; + convert(varchar, i.OrigFillFactor) end
      + case when (i.status&amp;1  &lt;&gt; 0) then &#39; ignore_dup_key&#39; else &#39;&#39; end
      ), &#39; &#39;, &#39;, &#39;) + &#39; &#39; end
   + &#39;on &#39; + (select f.groupname from sysfilegroups f where f.groupid=i.groupid) as coln
from
   sysindexes i,
   sysobjects o,
   sysusers u
where
   i.id = o.id and u.uid = o.uid
   and o.type = &#39;U&#39;
   [and o.name = %.q:TABLE%]
   [and u.name=%.q:OWNER%]
   [and i.name=%.q:INDEX%]</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:Comment>SQL query to reverse object physical options</a:Comment>
</o:TextTargetItem>
