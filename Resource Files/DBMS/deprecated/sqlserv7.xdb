from
   sysindexes i,
   sysobjects o,
   sysusers u
where
   i.id = o.id and u.uid = o.uid
   and o.type = &#39;U&#39;
   and (i.status &amp; 6144) &gt;0
   [and o.name = %.q:TABLE%]
   [and u.name=%.q:OWNER%]
</a:TextTargetItem.Value>
