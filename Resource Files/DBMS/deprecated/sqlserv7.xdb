<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, DOMAIN, DTTPCODE, LENGTH, PREC, NOTNULL}

select
   u.name,
   d.name,
   i.name,
   isnull(d.prec, d.length),
   d.scale,
   case(d.allownulls) when 0 then &#39;NOTNULL&#39; else &#39;NULL&#39; end
from
   dbo.sysusers u
      join dbo.systypes d on (d.uid = u.uid and d.usertype &gt;= 100)
      join dbo.systypes i on (d.type = i.type and i.usertype &lt; 100)
where
   i.usertype not in (0, 18, 80)
[  and u.name = %.q:SCHEMA%]
order by 1
</a:TextTargetItem.Value>
