<a:Name>SqlChckQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, TABLE, COLUMN, CONSTNAME, CONSTRAINT}

select
   u.name,
   o.name,
   c.name,
   k.name,
   replace(w.text, &#39;[&#39; + c.name + &#39;]&#39;, c.name)
from
   dbo.sysusers u
      join dbo.sysobjects o on (o.uid = u.uid and o.type in (&#39;U&#39;, &#39;S&#39;))
      join dbo.syscolumns c on (c.id = o.id)
      join dbo.sysobjects k on (k.parent_obj = o.id and k.info = c.colid and k.type = &#39;C&#39;)
      join dbo.syscomments w on (w.id = k.id)
where 1 = 1
[  and u.name = %.q:OWNER%]
[  and o.name = %.q:TABLE%]
[  and c.name = %.q:COLUMN%]
order by u.name, o.name, c.name, k.name</a:TextTargetItem.Value>
