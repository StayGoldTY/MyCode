<a:ObjectID>A9B3DB14-E570-4B79-9198-E4AE80867076</a:ObjectID>
<a:Name>SqlAttrQuery</a:Name>
<a:TextTargetItem.Value>{TABLOWNER ID, TABLE ID, OWNER ID, TRIGGER ID, SCRIPT ...}

select
   ub.name,
   tb.name,
   ur.name,
   tr.name,
   cm.text
from
   sysusers ub,   sysobjects tb,   sysobjects tr, sysusers ur, syscomments cm
where
   ur.uid = tr.uid 
   and ub.uid = tb.uid
   and tr.type = &#39;TR&#39;
   and tr.parent_obj = tb.id
   and cm.id = tr.id
[  and ub.name =%.q:TABLOWNER%]
[  and tb.name = %.q:TABLE%]
[  and ur.name =%.q:OWNER%]
[  and tr.name =%.q:TRIGGER%]
order by
   1,2,3,4,
   cm.number,
   cm.colid
</a:TextTargetItem.Value>
