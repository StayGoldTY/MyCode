<o:TextTargetItem Id="o244">
<a:ObjectID>935C6F74-37AF-494B-B122-769E3D490423</a:ObjectID>
<a:Name>SqlPermQuery</a:Name>
<a:TextTargetItem.Value>{GRANTEE ID, PERMISSION ...}

select u1.name grantee,
case
    when (s.action = 193) then &#39;SELECT&#39;
    when (s.action = 195) then &#39;INSERT&#39;
    when (s.action = 196) then &#39;DELETE&#39;
    when (s.action = 197) then &#39;UPDATE&#39;
end +
case
    when (s.protecttype = 204) then &#39;+&#39;
    when (s.protecttype = 205) then &#39;&#39;
    when (s.protecttype = 206) then &#39;-&#39;
end
+ &#39;,&#39; as coln
from sysprotects s, sysusers u, sysusers u1, sysobjects o
where
  o.name = %.q:VIEW% and
  [u.name = %.q:OWNER% and]
  o.uid = u.uid and
  s.id = o.id and
  u1.uid = s.uid
order by grantee</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:Comment>SQL query to reverse object permissions</a:Comment>
</o:TextTargetItem>
