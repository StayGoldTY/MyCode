<a:TextTargetItem.Value>[%ISLONGDTTP%?{ AverageLength }
select [%ISLONGDTTP%?[%ISSTRDTTP%?avg(char_length(%COLUMN%)):avg(datalength(%COLUMN%))]:null] as average_length
from [%QUALIFIER%]%TABLE%
:{ NullValuesRate, DistinctValues, AverageLength }
select
[%COLTYPE%=&quot;uniqueidentifier&quot;?null:[%ISMAND%?null:(count(*) - count(%COLUMN%)) * 100 / count(*)]] as average_length,
[%COLTYPE%=&quot;uniqueidentifier&quot;?null:count(distinct %COLUMN%)] as distinct_values,
[%ISVARDTTP%?[%ISSTRDTTP%?avg(char_length(%COLUMN%)):avg(datalength(%COLUMN%))]:null]  as average_length
from [%QUALIFIER%]%TABLE%]</a:TextTargetItem.Value>
