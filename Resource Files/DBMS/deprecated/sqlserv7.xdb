<a:Name>SqlListChildrenQuery</a:Name>
<a:TextTargetItem.Value>{COWNER, CHILD, FKCONSTRAINT, FKEYCOLUMN, CKEYCOLUMN}

select
   u.name,
   t.name,
   o.name,
   c.name,
   p.name
from
   dbo.sysforeignkeys j
      join dbo.sysobjects o on(o.id = j.constid)
      join dbo.syscolumns p on (p.id = j.rkeyid and p.colid = j.rkey)
      join dbo.syscolumns c on (c.id = j.fkeyid and c.colid = j.fkey)
      join dbo.sysobjects t on (t.id = j.fkeyid)
      join dbo.sysusers u on (u.uid = t.uid)
where
   o.type = &#39;F&#39;
[   and o.name = %.q:FKCONSTRAINT%]
[  and u.name = %.q:COWNER%]
[  and t.name = %.q:TABLE%]
order by 1, 2, 3, j.keyno</a:TextTargetItem.Value>
