<a:ObjectID>FE9C8030-4141-4EE0-9795-D5DB8FD65606</a:ObjectID>
<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, TABLE, @OBJTCODE, CONSTNAME, ISPKEY, CLUSTER}

select
   u.name, t.name, k.name, k.name,
   case (k.xtype) when &#39;PK&#39; then 1 else 0 end,
   case (k.status&amp;16) when (16) then &#39;YES&#39; else &#39;NO&#39; end
from
   sysusers u,
   sysobjects t,
   sysobjects k
where
   u.uid=t.uid
   and (k.parent_obj=t.id and k.type=&#39;K&#39;)
   [and t.name=%.q:TABLE%]
   [and u.name=%.q:OWNER%]
order by
   u.name,
   t.name,
   k.name</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:Comment>SQL query to list objects</a:Comment>
</o:TextTargetItem>
<o:TextTargetItem Id="o178">
<a:ObjectID>B3A5DAFE-5864-405F-ACF8-EAC6FA6D11DE</a:ObjectID>
<a:Name>SqlOptsQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, TABLE, @OBJTCODE, OPTIONS}

select
   u.name,
   t.name,
   k.name,
   case ((i.status &amp; 512) + i.OrigFillFactor) when 0 then &#39;&#39; else &#39;with &#39; + replace(ltrim(&#39;&#39;
      + case (i.OrigFillFactor) when 0 then &#39;&#39; else &#39; fillfactor=&#39; + convert(varchar, i.OrigFillFactor) end
      + case when (i.status&amp;512&lt;&gt; 0) then case (i.status&amp;8192) when 0 then &#39; sorted_data&#39; else &#39; sorted_data_reorg&#39; end else &#39;&#39; end
      ), &#39; &#39;, &#39;, &#39;) + &#39; &#39; end
   + &#39; on &#39; + (select f.groupname from sysfilegroups f where f.groupid=i.groupid) as coln
from
   sysusers u,
   sysobjects t,
   sysobjects k,
   sysindexes i
where
   u.uid=t.uid
   and (i.id=t.id)
   and (k.parent_obj=t.id and k.type=&#39;K&#39; and k.name=i.name)
[  and u.name=%.q:OWNER%]
[  and t.name=%.q:TABLE%]
[  and k.name=%.q:KEY%]
</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:Comment>SQL query to reverse object physical options</a:Comment>
</o:TextTargetItem>
<o:TextTargetItem Id="o179">
<a:ObjectID>315942BD-F4AD-48C6-85FA-4E04F8FB26DC</a:ObjectID>
<a:Name>SqlAttrQuery</a:Name>
<a:TextTargetItem.Value>{OWNER ID, TABLE ID, @OBJTCODE ID, COLUMNS ...}

select
   u.name, t.name, k.name, c.name + &#39;, &#39;
from
   sysusers u,
   sysobjects t,
   sysobjects k,
   sysindexkeys x,
   syscolumns c,
   sysindexes i
where
   u.uid=t.uid
   and (i.id=t.id)
   and (k.parent_obj=t.id and k.type=&#39;K&#39; and k.name=i.name)
   and (x.id = t.id and x.indid=i.indid)
   and (c.id=x.id and c.colid=x.colid)
[  and u.name=%.q:OWNER%]
[  and t.name=%.q:TABLE%]
[  and k.name=%.q:KEY%]
order by
   u.name,
   t.name,
   x.indid,
   x.keyno</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:Comment>SQL query to reverse object attributes</a:Comment>
</o:TextTargetItem>
