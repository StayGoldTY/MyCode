<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, DEFAULT, SCRIPT}

select
   user_name(o.uid),
   o.name,
   t.text
from 
   dbo.sysobjects o,
   dbo.syscomments t
where
   o.type = &#39;D&#39;
   and t.id = o.id
   and (o.parent_obj is null or o.parent_obj = 0)
[  and user_name(o.uid) = %.q:SCHEMA%]
order by
   1, 2</a:TextTargetItem.Value>
