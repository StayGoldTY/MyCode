<a:Name>SqlAttrQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, DEFAULT, BOUND_OBJECT}

select
   user_name(o.uid),
   o.name,
   d.name
from
   dbo.sysobjects o,
   dbo.systypes d 
where
   o.type = &#39;D&#39; 
   and d.tdefault = o.id
[  and o.name = %.q:DEFAULT%]
[  and user_name(o.uid) = %.q:OWNER%]
union select 
   user_name(o.uid),
   o.name,
   user_name(t.uid)+&#39;.&#39;+t.name+&#39;.&#39;+c.name
from
   dbo.sysobjects o,
   dbo.syscolumns c,
   dbo.sysobjects t 
where
   o.type = &#39;D&#39;
   and c.cdefault = o.id
   and t.id = c.id
[  and o.name = %.q:DEFAULT%]
[  and user_name(o.uid) = %.q:OWNER%]
order by
   1, 2</a:TextTargetItem.Value>
