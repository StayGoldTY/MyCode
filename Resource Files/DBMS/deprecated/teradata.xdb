<o:TextTargetItem Id="o180">
<a:ObjectID>68712496-DE07-4343-A329-32E4E96767A2</a:ObjectID>
<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{TABLOWNER, TABLE, OWNER, TRIGGER, TRGEVENT, TRGTIME}

select
   t.DataBaseName,
   t.TableName,
   t.DataBaseName,
   t.TriggerName,
   case(t.Event) when &#39;U&#39; then &#39;update&#39; when &#39;I&#39; then &#39;insert&#39; when &#39;D&#39; then &#39;delete&#39; else &#39;&#39; end as trgevent,
   (case(t.EnabledFlag) when &#39;N&#39; then &#39;disabled &#39; else &#39;&#39; end)
   ||(case(t.ActionTime) when &#39;A&#39; then &#39;after&#39; when &#39;B&#39; then &#39;before&#39; when &#39;I&#39; then &#39;instead of&#39; else &#39;&#39; end) as trgtime
from
   dbc.Triggers t
[where t.DataBaseName = %.q:SCHEMA%]
order by
   1,2,3,4
</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>1254729749</a:ModificationDate>
<a:Modifier>mledier</a:Modifier>
<a:Comment>SQL query to list objects</a:Comment>
</o:TextTargetItem>
<o:TextTargetItem Id="o181">
<a:ObjectID>DCDC31F0-9381-40E4-9C2D-867CE0422C35</a:ObjectID>
<a:Name>SqlAttrQuery</a:Name>
<a:TextTargetItem.Value>{TABLOWNER, TABLE, OWNER, TRIGGER, SCRIPT}

select
   t.DataBaseName,
   t.TableName,
   t.DataBaseName,
   t.TriggerName,
   t.RequestText
from
   dbc.Triggers t
where 1=1
[  and t.TableName = %.q:TABLE%]
[  and t.DataBaseName = %.q:OWNER%]
[  and t.TriggerName = %.q:TRIGGER%]
order by 1, 2, 3, 4
</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>1254729749</a:ModificationDate>
<a:Modifier>mledier</a:Modifier>
<a:Comment>SQL query to reverse object attributes</a:Comment>
</o:TextTargetItem>
