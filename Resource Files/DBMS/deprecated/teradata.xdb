<o:TextTargetItem Id="o100">
<a:ObjectID>480012C8-83AE-495A-9916-0274241BFF86</a:ObjectID>
<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, TABLE, COLUMN, COLNNAME, DATATYPE, NOTNULL, IDENTITY, ExtGenAlways, DEFAULT, COMMENT, FORMAT, CharacterSet, Compress}

select
   c.DatabaseName,
   c.TableName,
   c.ColumnName,
   c.ColumnTitle,
   c.ColumnType || (case 
      when (c.columntype in (&#39;TS&#39;)) then
        &#39;(&#39; || trim(c.DecimalFractionalDigits) || &#39;)&#39;
      when (c.columntype in (&#39;YR&#39;, &#39;MO&#39;, &#39;HR&#39;, &#39;MI&#39;, &#39;DY&#39;)) then
        &#39;(&#39; || trim(c.DecimalTotalDigits) || &#39;)&#39;
      when (c.columntype in (&#39;CF&#39;, &#39;CV&#39;, &#39;BV&#39;, &#39;BF&#39;)) then 
        &#39;(&#39; || trim(case when (c.CharType in (2, 4)) then (c.ColumnLength/2) else (c.ColumnLength) end) || &#39;)&#39;
      when (c.columntype in (&#39;D&#39;, &#39;SC&#39;)) then 
        &#39;(&#39; || trim(c.DecimalTotalDigits) || (case (c.DecimalFractionalDigits) when 0 then &#39;&#39; else &#39;,&#39; || trim(c.DecimalFractionalDigits) end) || &#39;)&#39;
      else &#39;&#39;
   end) as colnA,
   case when (c.Nullable = &#39;N&#39;) then &#39;NOT NULL&#39; else &#39;NULL&#39; end,
   case(c.idColType) when &#39;GD&#39; then &#39;identity&#39; when &#39;GA&#39; then &#39;identity&#39; else &#39;&#39; end,
   case(c.idColType) when &#39;GD&#39; then &#39;false&#39; else &#39;true&#39; end,
   case when (
         (length(c.defaultvalue) &gt; 2) and 
         (substring(c.ColumnType from 1 for 1) = &#39;C&#39;) and
         (trim(substring(c.defaultvalue from 2 for length(c.defaultvalue) - 2)) = &#39;&#39;)) then &#39;~&#39; || c.defaultvalue || &#39;~&#39;
        else c.defaultvalue
   end as colnB,
   c.CommentString,
   c.ColumnFormat,
   case (c.CharType)
      when 1 then &#39;latin&#39;
      when 2 then &#39;unicode&#39;
      when 3 then &#39;kanjisjis&#39;
      when 4 then &#39;graphic&#39;
      when 5 then &#39;kanji1&#39;
      else &#39;&#39;
   end as colnC,
   c.CompressValueList
from
   DBC.Columns c
where 1=1
[  and c.TableName = %.q:TABLE%]
[  and c.DatabaseName = %.q:OWNER%]
order by
   1, 2, c.ColumnId</a:TextTargetItem.Value>
