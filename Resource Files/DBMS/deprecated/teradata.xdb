<o:TextTargetItem Id="o59">
<a:ObjectID>4D78B1D4-3965-42C0-98A0-B9528E3E9745</a:ObjectID>
<a:Name>SqlOptsQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, TABLE, OPTIONS}

select 
   t.DatabaseName,
   t.TableName,
   (case(t.protectiontype) when &#39;F&#39; then &#39;fallback&#39; else &#39;no fallback&#39; end)
   ||(case when (j.journalname is null) then &#39;&#39; else &#39;, with journal table &#39;||trim(j.Journals_DB) || &#39;.&#39; || trim(j.JournalName) end)
   ||(case (substring(t.journalflag from 1 for 1))
       when &#39;N&#39; then (case substring(t.journalflag from 2 for 1) when &#39;N&#39; then &#39;, no journal&#39; else &#39;&#39;               end)
       when &#39;S&#39; then (case substring(t.journalflag from 2 for 1) when &#39;S&#39; then &#39;, journal&#39;  else &#39;, before journal&#39; end)
       when &#39;D&#39; then (case substring(t.journalflag from 2 for 1) when &#39;D&#39; then &#39;, dual journal&#39; else &#39;&#39;             end)
       else &#39;&#39;  end)
   ||(case (substring(t.journalflag from 2 for 1))
       when &#39;N&#39; then (case substring(t.journalflag from 1 for 1) when &#39;N&#39; then &#39;&#39; else &#39;, no after journal&#39;   end)
       when &#39;S&#39; then (case substring(t.journalflag from 1 for 1) when &#39;S&#39; then &#39;&#39; else &#39;, after journal&#39;      end)
       when &#39;D&#39; then (case substring(t.journalflag from 1 for 1) when &#39;D&#39; then &#39;&#39; else &#39;, dual after journal&#39; end)
       when &#39;L&#39; then &#39;, local after journal&#39;
       else &#39;&#39;  end) as coln
from 
   dbc.tables t 
   left outer join dbc.journals j on (j.Tables_DB = t.DatabaseName and j.TableName = t.TableName)
where 1=1
[  and t.tablename = %.q:TABLE%]
[  and t.DatabaseName = %.q:OWNER%]
order by t.tablename
</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>0</a:ModificationDate>
<a:Modifier/>
<a:Comment>SQL query to reverse object physical options</a:Comment>
</o:TextTargetItem>
