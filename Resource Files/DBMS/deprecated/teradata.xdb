<a:ObjectID>A49E0E8E-71E4-4C11-9E02-62A4A69E3D2B</a:ObjectID>
<a:Name>SqlOptsQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, TABLE, INDEX, OPTIONS}

select
   i.DatabaseName,
   i.TableName,
   coalesce(i.IndexName, &#39;Index_&#39; || trim(i.IndexNumber)),
   (case when (x.ConstraintType = &#39;Q&#39;) then
      &#39;partition by &#39;||substring(substring(x.ConstraintText from 9) from 1 for character_length(substring(x.ConstraintText from 9)) - 22)
   else &#39;&#39; end)
   ||(case 
      when (i.IndexType in (&#39;S&#39;, &#39;H&#39;)) then &#39; order by hash ( &#39; || trim(j.ColumnName) || &#39; )&#39;
      when (i.IndexType in (&#39;V&#39;, &#39;O&#39;)) then &#39; order by values ( &#39; || trim(j.ColumnName) || &#39; )&#39;
     else &#39;&#39; end)
from
   dbc.Indices i
   left outer join dbc.IndexConstraints x on (x.TableName = i.TableName and x.DatabaseName = i.DatabaseName and coalesce(x.IndexName, &#39;&#39;) = coalesce(i.IndexName, &#39;&#39;))
   left outer join dbc.Indices j on (j.TableName = i.TableName and j.DatabaseName = i.DatabaseName and j.IndexNumber = (i.IndexNumber + 4) and j.IndexName is null and j.IndexType = &#39;I&#39;)
where i.columnPosition = 1 and i.IndexType != &#39;I&#39;
[  and i.DatabaseName = %.q:OWNER%]
[  and i.TableName = %.q:TABLE%]
[  and coalesce(i.IndexName, &#39;Index_&#39; || trim(i.IndexNumber)) = %.q:INDEX%]

</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>1254729749</a:ModificationDate>
<a:Modifier>mledier</a:Modifier>
<a:Comment>SQL query to reverse object physical options</a:Comment>
</o:TextTargetItem>
