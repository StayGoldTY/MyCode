<a:CheckGlobalScript>&#39;******************************************************************************
&#39;* Purpose:  This VB-Script holds global definitions shared by all the custom-
&#39;*            checks scripts of the model extension.
&#39;******************************************************************************

Option Explicit &#39; This is to ensure all used variables are defined

Sub ParseText(pIndex, sIndexSpec)
   Dim pos : pos = InStr(sIndexSpec, &quot;,&quot;)
   Dim sCurIdxCol
   If (pos &gt; 0) Then
      sCurIdxCol = Trim(Left(sIndexSpec, pos - 1))
   Else
      sCurIdxCol = Trim(sIndexSpec)
   End If
   Dim p2, sFunc, ncol, bAsc
   If (Left(sCurIdxCol, 1) = &quot;&lt;&quot;) Then
      p2 = InStr(sCurIdxCol, &quot;&gt;&quot;)
      If (p2 = 0) Then Exit Sub
      sFunc = Mid(Left(sCurIdxCol, p2 - 1), 2)
      Dim pFunc
      For each pFunc in pIndex.Model.Procedures
         If (CStr(pFunc.GetExtendedAttribute(&quot;InternalID&quot;)) = sFunc) Then
            sFunc = pFunc.Code
            Exit For
         End If
      Next
      sCurIdxCol = Replace(Replace(Mid(sCurIdxCol, p2 + 1), &quot;(&quot;, &quot;&quot;), &quot;)&quot;, &quot;&quot;)
   Else
      sFunc = &quot;&quot;
   End If
   p2 = InStr(sCurIdxCol, &quot;[&quot;)
   If (p2 = 0) Then Exit Sub
   ncol = CInt(Left(sCurIdxCol, p2 - 1))
   If ncol &lt; 0 Then
      bAsc = False
      ncol = -ncol
   Else
      bAsc = True
   End If
   Dim pIdxCol : Set pIdxCol = pIndex.IndexColumns.CreateNew()
   If (sFunc = &quot;&quot;) Then
      Set pIdxCol.Column = pIndex.Table.Columns.Item(ncol - 1)
   Else
      pIdxCol.Expression = sFunc &amp; &quot;(&quot; &amp; pIndex.Table.Columns.Item(ncol - 1).Code &amp; &quot;)&quot;
   End If
   pIdxCol.Ascending = bAsc
   If (pos &gt; 0) Then ParseText pIndex, Mid(sIndexSpec, pos + 1)
End Sub</a:CheckGlobalScript>
