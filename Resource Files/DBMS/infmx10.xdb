<a:ObjectID>B48F110D-DA6A-48C4-8281-7A37F04E7BEA</a:ObjectID>
<a:Name>SqlChckQuery</a:Name>
<a:TextTargetItem.Value>{OWNER ID, TABLE ID, COLUMN ID, S ID, CONSTNAME ID, CONSTRAINT ...}

select
   t.owner,
   t.tabname,
   c.colname,
   k.constrid,
   k.constrname,
   v.checktext,
   v.seqno
from sysconstraints k
   join syscoldepend d  on (d.constrid = k.constrid)
   join syscolumns c on (c.colno = d.colno and c.tabid = d.tabid)
   join systables t on (t.tabid = c.tabid )
   join syschecks v on (v.constrid=k.constrid and v.type=&#39;T&#39;)
where k.constrtype = &#39;C&#39;
   and 1 = (select count(*) from syscoldepend d2 where d2.constrid = k.constrid) 
[  and t.owner = %.q:OWNER%]
[  and t.tabname=%.q:TABLE%]
[  and c.colname=%.q:COLUMN%]
order by
   1, 2, 3, 4, 7</a:TextTargetItem.Value>
