<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{TABLOWNER, TABLE, INDEX, UNIQUE, CLUSTER, IndexSpec}

select
   t.owner,
   t.tabname,
   i.idxname,
   case(i.idxtype) when &#39;U&#39; then &#39;unique&#39; else &#39;&#39; end,
   case(i.clustered) when &#39;C&#39; then &#39;TRUE&#39; else &#39;FALSE&#39; end,
   indexkeyarray_out(i.indexkeys)
from
   informix.systables t
   join informix.sysindices i on (i.tabid = t.tabid)   
where 1=1
[  and t.owner = %.q:SCHEMA%]
[  and t.tabname = %.q:TABLE%]

