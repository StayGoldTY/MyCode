<a:MethodScript>Sub %Method%(obj, path)
   if Not(obj.GetExtendedAttribute(&quot;%CurrentTargetCode%.HashPartition&quot;)) then
      Exit Sub
   end if
   if (obj.IndexColumns.Count = 0) then
      MsgBox &quot;There is no column to select in &quot; &amp; obj
      Exit Sub
   end if
   
   Dim ColList, Selected
   Set Selected = obj.Model.CreateSelection()
   if (Selected is Nothing) then
      MsgBox &quot;Unable to create a selection&quot;
      exit sub
   end if
   ColList = replace(obj.GetPhysicalOptionSql(path &amp; &quot;/&quot;), vbCrLf, &quot;&quot;)

   if (Trim(ColList) &lt;&gt; &quot;&quot;) then
      Dim pCol, sCol
      for each sCol in Split(ColList, &quot;,&quot;)
         Set pCol = obj.FindChildByCode(Trim(sCol), cls_IndexColumn, &quot;&quot;, Nothing, False)
         if Not(pCol is Nothing) then
            Selected.Objects.Add(pCol)
         end if
      next
   end if
   ColList = &quot;&quot;
   
   Selected.ShowObjMultiSelection obj, &quot;IndexColumns&quot;
   for each pCol in Selected.objects
      ColList = ColList &amp; &quot;, &quot; &amp; pCol.Code
   Next
   if (ColList &lt;&gt; &quot;&quot;) then
      ColList = Mid(ColList, 3)
   end if
   obj.SetPhysicalOptionSql path , ColList
End Sub</a:MethodScript>
</o:MethodTargetItem>
</c:Categories>
