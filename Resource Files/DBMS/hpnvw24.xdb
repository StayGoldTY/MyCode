<o:TextTargetItem Id="o74">
<a:ObjectID>667B500C-0F36-4B24-8FB5-EAE7CAC51147</a:ObjectID>
<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{OWNER ID, TABLE ID, INDEX ID, CIDXLIST ...}

select 
   S.object_name,
   T.object_name,
   X.object_name,
   C.column_name || case when (R.ordering = &#39;A&#39;) then &#39;,&#39; else &#39; desc,&#39; end
from definition_schema_version_2400.objects X
join definition_schema_version_2400.access_paths P on (P.access_path_uid = X.object_uid)
join definition_schema_version_2400.objects T on (T.object_uid = P.table_uid and T.object_type = &#39;BT&#39; and T.object_name_space = &#39;TA&#39;)
join definition_schema_version_2400.objects S on (S.schema_uid = T.schema_uid and S.object_type = &#39;SL&#39;)
join definition_schema_version_2400.access_path_cols R on (R.access_path_uid = X.object_uid and R.system_added_column = &#39;N&#39;)
join definition_schema_version_2400.cols C on (C.object_uid = T.object_uid and C.column_number = R.column_number)
where X.object_type = &#39;IX&#39;
[  and S.object_name = %.q:OWNER%]
[  and T.object_name = %.q:TABLE%]
order by 1, 2, 3, R.clustering_key_seq_num</a:TextTargetItem.Value>
