<a:GetMethodScript>Function %Get%(obj)
   dim def, att
   def = &quot;&quot;
   Select case(Replace(LCase(obj.GetExtendedAttributeText(&quot;RefreshType&quot;)), &quot; &quot;, &quot;&quot;))
   Case &quot;recompute&quot;:
      def = def &amp; &quot; recompute&quot;
   Case &quot;onstatement&quot;:
      def = def &amp; &quot; refresh on statement&quot;
   Case &quot;onrequest&quot;:
      def = def &amp; &quot; refresh on request&quot;
      att = obj.GetExtendedAttributeText(&quot;IgnoreChangesOn&quot;)
      if (att &lt;&gt; &quot;&quot;) then
         def = def &amp; &quot; ignore changes on &quot; &amp; att
      end if
   End Select
   Select case(Replace(LCase(obj.GetExtendedAttributeText(&quot;Initialize&quot;)), &quot; &quot;, &quot;&quot;))
      Case &quot;onrefresh&quot;
         def = def &amp; &quot; initialize on refresh&quot;
      Case &quot;oncreate&quot;
         def = def &amp; &quot; initialize on create&quot;
   end Select
   att = obj.GetExtendedAttributeText(&quot;Clustering&quot;)
   if (att &lt;&gt; &quot;&quot;) then
      def = def &amp; &quot; store by (&quot; &amp; att &amp; &quot;)&quot;
   end if
   att = LCase(obj.GetExtendedAttributeText(&quot;HashPartition&quot;))
   if (att = &quot;true&quot;) then
      def = def &amp; &quot; hash partition&quot;
      att = obj.GetExtendedAttributeText(&quot;PartitionColumnList&quot;)
      if (att &lt;&gt; &quot;&quot;) then
         def = def &amp; &quot; by (&quot; &amp; att &amp; &quot;)&quot;
      end if
   end if
   att = obj.GetExtendedAttributeText(&quot;MVAttribute&quot;)
   if (att &lt;&gt; &quot;&quot;) then
      def = def &amp; &quot; mvattribute commit refresh each &quot; &amp; att
   end if
   %Get% = trim(def)
End Function</a:GetMethodScript>
<a:SetMethodScript>Sub %Set%(obj, value)
   
   
   Dim rxp, itm, mch, rst
   Set rxp = new RegExp
   rxp.IgnoreCase = True
   rxp.Global = True
   itm = Replace(Replace(value, vbCr, &quot; &quot;), vbLf, &quot; &quot;)
   Dim M, S

   rxp.Pattern = &quot;initialize\s+on\s+(refresh|create)&quot;
   For each mch in rxp.Execute(itm)
      Select Case(LCase(rxp.Replace(mch.Value, &quot;$1&quot;)))
      Case &quot;refresh&quot;:
         obj.SetExtendedAttributeText &quot;Initialize&quot;, &quot;on refresh&quot;
      Case &quot;create&quot;
         obj.SetExtendedAttributeText &quot;Initialize&quot;, &quot;on create&quot;
      End Select
      itm = replace(itm, CStr(mch), &quot;&quot;)
      exit for
   Next
   obj.SetExtendedAttributeText &quot;Clustering&quot;, &quot;&quot;
   obj.SetExtendedAttributeText &quot;HashPartition&quot;, &quot;True&quot;
   obj.SetExtendedAttributeText &quot;PartitionColumnList&quot;, &quot;&quot;
   obj.SetExtendedAttributeText &quot;HashPartition&quot;, &quot;False&quot;
   obj.SetExtendedAttributeText &quot;MVAttribute&quot;, &quot;&quot;
   rxp.Pattern = &quot;(store|hash\s+partition)\s+by\s*\(([^\)]*)\)&quot;
   For each mch in rxp.Execute(itm)
      S = LCase(Replace(rxp.Replace(mch.Value, &quot;$1&quot;), &quot; &quot;, &quot;&quot;))
      M = rxp.Replace(mch.Value, &quot;$2&quot;)
      if (S = &quot;store&quot;) then
         obj.SetExtendedAttributeText &quot;Clustering&quot;, M
      elseif (S = &quot;hashpartition&quot;) then
         obj.SetExtendedAttributeText &quot;HashPartition&quot;, &quot;True&quot;
         if (M &lt;&gt; &quot;&quot;) then
            obj.SetExtendedAttributeText &quot;PartitionColumnList&quot;, M
         end if
      end if
      itm = replace(itm, CStr(mch), &quot;&quot;)
   Next

   rxp.Pattern = &quot;(mvattribute|mvattributes)\s+commit\s+refresh\s+each\s+(\d+)&quot;
   For each mch in rxp.Execute(itm)
      M = LCase(rxp.Replace(mch.Value, &quot;$2&quot;))
      obj.SetExtendedAttributeText &quot;MVAttribute&quot;, M
      itm = replace(itm, CStr(mch), &quot;&quot;)
      exit for
   Next

   rxp.Pattern = &quot;(recompute|refresh\s+on\s+statement|refresh\s+on\s+request)&quot;
   For each mch in rxp.Execute(itm)
      itm = replace(itm, CStr(mch), &quot;&quot;)
      Select Case(Replace(LCase(rxp.Replace(mch.Value, &quot;$1&quot;)), &quot; &quot;, &quot;&quot;))
         Case &quot;recompute&quot;:
            obj.SetExtendedAttributeText &quot;RefreshType&quot;, &quot;recompute&quot;
         Case &quot;refreshonstatement&quot;
            obj.SetExtendedAttributeText &quot;RefreshType&quot;, &quot;on statement&quot;
         Case &quot;refreshonrequest&quot;
            obj.SetExtendedAttributeText &quot;RefreshType&quot;, &quot;on request&quot;
            obj.SetExtendedAttributeText &quot;IgnoreChangesOn&quot;, &quot;&quot;
            rst = LCase(Replace(itm, &quot; &quot;, &quot;&quot;))
            if (InStr(rst, &quot;ignorechangeson&quot;) &gt; 0) then
               obj.SetExtendedAttributeText &quot;IgnoreChangesOn&quot;, Mid(rst, InStr(rst, &quot;ignorechangeson&quot;) + Len(&quot;ignorechangeson&quot;))
            end if
      End Select
      exit for
   Next

End Sub</a:SetMethodScript>
<a:ValidateDataType>0</a:ValidateDataType>
</o:ExtendedAttributeTargetItem>
