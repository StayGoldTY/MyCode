<o:TextTargetItem Id="o111">
<a:ObjectID>EF0448A7-01E1-409A-8058-E868B73C1090</a:ObjectID>
<a:Name>SqlChckQuery</a:Name>
<a:TextTargetItem.Value>{OWNER ID, TABLE ID, COLUMN ID, CONSTNAME ID, CONSTRAINT ...}

select 
   S.object_name,
   T.object_name,
   C.column_name,
   K.object_name,
   Z.text
from 
   definition_schema_version_2400.ck_col_usage U
   join definition_schema_version_2400.objects K on (K.object_uid = U.constraint_uid)
   join definition_schema_version_2400.cols C on (C.object_uid = U.table_uid and C.column_number = U.column_number)
   join definition_schema_version_2400.objects T on (T.object_uid = C.object_uid and T.object_type = &#39;BT&#39; and T.object_name_space = &#39;TA&#39;)
   join definition_schema_version_2400.objects S on (S.schema_uid = T.schema_uid and S.object_type = &#39;SL&#39;)
   join definition_schema_version_2400.text Z on (Z.object_uid = K.object_uid)
where  
   K.object_type = &#39;CC&#39;
   and (select count(*) from definition_schema_version_2400.ck_col_usage W where W.constraint_uid = U.constraint_uid) = 1
[  and S.object_name = %.q:OWNER%]
[  and T.object_name = %.q:TABLE%]
[  and C.column_name = %.q:COLUMN%]
order by 1, 2, 3, 4, Z.sequence_num</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>1286526569</a:ModificationDate>
<a:Modifier>llane</a:Modifier>
<a:Comment>SQL query to reverse object check constraints</a:Comment>
</o:TextTargetItem>
<o:TextTargetItem Id="o112">
<a:ObjectID>FFBA249F-B8F9-4EBB-B2C9-42972CA7CD0A</a:ObjectID>
<a:Name>SqlIdentityDef</a:Name>
<a:TextTargetItem.Value>{VAR, VAL}

select
   cast (C.object_uid as varchar(100)) || &#39;.&#39; || cast (C.column_number as varchar(30)),
   &#39;start with &#39; || cast(G.start_value as varchar(30))
   || &#39; increment by &#39; || cast(G.increment as varchar(30))
   || &#39; maxvalue &#39; || cast(G.max_value as varchar(30))
   || &#39; minvalue &#39; || cast(G.min_value as varchar(30))
   || case when (G.cycle_option = &#39;N&#39;) then &#39; no cycle&#39; else &#39; cycle&#39; end
from
   definition_schema_version_2400.cols C
   join definition_schema_version_2400.objects T on (T.object_uid = C.object_uid and T.object_type = &#39;BT&#39; and T.object_name_space = &#39;TA&#39;)
   join definition_schema_version_2400.objects S on (S.schema_uid = T.schema_uid and S.object_type = &#39;SL&#39;)
   join definition_schema_version_2400.access_path_cols X on (X.access_path_uid = C.object_uid and X.column_number = C.column_number and C.default_class in (&#39;IA&#39;, &#39;ID&#39;))
   join definition_schema_version_2400.sg_usage Y on (Y.using_object_obj_uid = X.access_path_uid)
   join definition_schema_version_2400.sequence_generators G on (G.object_uid = Y.sg_object_uid)
where  1 = 1
[  and S.object_name = %.q:OWNER%]
[  and T.object_name = %.q:TABLE%]
[  and C.column_name = %.q:COLUMN%]
order by 1, 2, C.column_number
</a:TextTargetItem.Value>
<a:CreationDate>1277734155</a:CreationDate>
<a:Creator>llane</a:Creator>
<a:ModificationDate>1286526569</a:ModificationDate>
<a:Modifier>llane</a:Modifier>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
</o:TextTargetItem>
<o:TextTargetItem Id="o113">
<a:ObjectID>F84F97A2-D7A3-4FF2-9CCC-6FF21E7BA0FF</a:ObjectID>
<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, TABLE, COLUMN, NOTNULL, DATATYPE, Unsigned, DEFAULT, Charset, Upshift, Identity, IdentityType, IdentityDefinition EX}

select 
   S.object_name,
   T.object_name,
   C.column_name,
   (select &#39;not null&#39;
      from 
         definition_schema_version_2400.ck_col_usage U where U.table_uid = C.object_uid and U.column_number = C.column_number
         and exists (select 1 from definition_schema_version_2400.objects K where K.object_uid = U.constraint_uid and K.object_type = &#39;NN&#39;)
         ),
   case when (C.fs_data_type &lt; 128) then
      case when (bitand(C.fs_data_type, 2) = 2) then &#39;n&#39; else &#39;&#39; end
      ||&#39;char&#39;
      ||case when (bitand(C.fs_data_type, 64) = 64) then &#39; varying&#39; else &#39;&#39; end
      ||&#39;(&#39;
      ||case when ( bitand(C.fs_data_type, 2) = 2) then cast(cast(C.column_size / 2 as integer) as varchar(5)) else cast(C.column_size as varchar(5)) end
      ||&#39;)&#39;
   else
      case when (C.col_precision = 0) then
         case (C.fs_data_type)
            when 130 then &#39;smallint&#39;
            when 131 then &#39;smallint&#39;
            when 132 then &#39;integer&#39;
            when 133 then &#39;integer&#39;
            when 134 then &#39;largeint&#39;
            when 142 then &#39;real&#39;
            when 143 then &#39;double precision&#39;
            else trim(C.sql_data_type) || &#39; &#39; || C.datetime_qualifier
         end
      else
         case (C.fs_data_type)
            when 132 then &#39;numeric&#39;
            when 152 then &#39;decimal&#39;
            when 156 then &#39;numeric&#39;
            else trim(decode(sign(C.fs_data_type - 143), -1, &#39;numeric&#39;, 0, &#39;float&#39;, &#39;decimal&#39;))
         end
         || &#39;(&#39; ||cast(C.col_precision as varchar(5)) ||case when (C.col_scale != 0) then &#39;, &#39; || cast(C.col_scale as varchar(5)) else &#39;&#39; end ||&#39;)&#39;
      end
   end as &quot;datatype&quot;,
   case when (locate(&#39;UNSIGNED&#39;, C.sql_data_type) = 1) then &#39;true&#39; else &#39;false&#39; end,
   C.default_value,
   C.character_set,
   case when (C.upshifted = &#39;Y&#39;) then &#39;true&#39; else &#39;false&#39; end,
   case when (C.default_class in (&#39;IA&#39;, &#39;ID&#39;)) then &#39;true&#39; else &#39;false&#39; end,
   case (C.default_class) 
      when &#39;IA&#39; then &#39;always&#39;
      when &#39;ID&#39; then &#39;by default&#39;
      else &#39;&#39;
   end,
   case when (C.default_class in (&#39;IA&#39;, &#39;ID&#39;)) then &#39;(%SqlIdentityDef.&#39; || cast (C.object_uid as varchar(100)) || &#39;.&#39; || cast (C.column_number as varchar(30)) || &#39;%)&#39; else &#39;&#39; end
from
   definition_schema_version_2400.cols C
   join definition_schema_version_2400.objects T on (T.object_uid = C.object_uid and T.object_type = &#39;BT&#39; and T.object_name_space = &#39;TA&#39;)
   join definition_schema_version_2400.objects S on (S.schema_uid = T.schema_uid and S.object_type = &#39;SL&#39;)
where  1 = 1
[  and S.object_name = %.q:OWNER%]
[  and T.object_name = %.q:TABLE%]
order by 1, 2, C.column_number</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>1308296303</a:ModificationDate>
<a:Modifier>mledier</a:Modifier>
<a:Comment>SQL query to list objects</a:Comment>
</o:TextTargetItem>
</c:Categories>
</o:TargetCategory>
