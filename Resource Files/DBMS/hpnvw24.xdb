<a:Name>SqlPartKeys</a:Name>
<a:TextTargetItem.Value>{VAR ID, VAL ...}

select 
   cast(T.object_uid as varchar(30)),
   C.column_name || &#39;,&#39;
from definition_schema_version_2400.access_path_cols K
   join definition_schema_version_2400.access_paths D on (D.access_path_uid = K.access_path_uid and D.access_path_type = &#39;BT&#39; and D.partitioning_scheme != &#39;N&#39;)
   join definition_schema_version_2400.objects T on (T.object_uid = D.table_uid and T.object_type = &#39;BT&#39; and T.object_name_space = &#39;TA&#39;)
   join definition_schema_version_2400.cols C on (C.object_uid = T.object_uid and C.column_number = K.column_number)
where k.part_key_seq_num &gt; 0   
[  and T.object_name = %.q:TABLE%]
order by 1, K.part_key_seq_num
</a:TextTargetItem.Value>
