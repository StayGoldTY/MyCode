<a:HandlerScript>Function %GetEstimatedSize%(obj, ByRef message)
   Dim S, C, N, V

   Dim ColSizes
   Set ColSizes = CreateObject(&quot;Scripting.Dictionary&quot;)
   
   S = 4
   N = 0
   V = 0
   for each C in obj.Columns
      if (InStr(C.ConceptualDataType, &quot;V&quot;) &gt; 0) then V = V + 1
      ColSizes.Add C, C.GetEstimatedSize(message, True)
      S = S + ColSizes.Item(C)
   next
   S = S + (N / 32 + 1) * 4
   S = S + V * 4
   
   S = S
   Dim X, XMsg, XS
   XMsg = &quot;&quot;
   for each X in obj.Indexes
      XS = 0
      For each C in X.IndexColumns
         XS = XS + ColSizes.Item(C.Column)
      next
      XMsg = XMsg &amp; CStr(XS * obj.Number) &amp; &quot;|&quot; &amp; X.ObjectID &amp; vbCrLf
      S = S + XS
   next
   
   N = obj.GetExtendedAttribute(&quot;Number&quot;)
   if (N = &quot;&quot;) then 
      if (obj.Tables.Count &gt; 0) then
         &#39; take minimal table count, most often, joins are natural
         Dim T
         N = 9999999999999999
         for each T in obj.Tables
            if T.Number &lt; N then N = T.Number
         next
      else
         N = 1
      end if
   end if
   S = S * N
   
   message = CStr(S) &amp; &quot;|&quot; &amp; obj.ObjectID &amp; vbCrLf &amp; XMsg
   
   %GetEstimatedSize% = S
   
End Function</a:HandlerScript>
</o:EventHandlerTargetItem>
