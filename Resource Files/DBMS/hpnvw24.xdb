<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, SYNONYM, BASEOWNER, BASEOBJECT}

select 
   S.object_name, 
   O.object_name,
   U.object_name,
   R.object_name
from 
   definition_schema_version_2400.objects O
   join definition_schema_version_2400.objects S on (S.schema_uid = O.schema_uid and S.object_type = &#39;SL&#39;)
   join definition_schema_version_2400.synonym_usage A on (A.object_uid = O.object_uid)
   join definition_schema_version_2400.objects R on (R.object_uid = A.underlying_obj_uid)
   join definition_schema_version_2400.objects U on (U.schema_uid = R.schema_uid and U.object_type = &#39;SL&#39;)
where O.object_type in (&#39;SY&#39;)
[  and S.object_name = %.q:SCHEMA%]
</a:TextTargetItem.Value>
