<a:Name>SqlChckQuery</a:Name>
<a:TextTargetItem.Value>{OWNER ID, TABLE ID, CONSTNAME ID, CONSTRAINT ...}

select 
   S.object_name,
   T.object_name,
   K.object_name,
   Z.text
from 
   definition_schema_version_2400.ck_tbl_usage U
   join definition_schema_version_2400.objects K on (K.object_uid = U.constraint_uid)
   join definition_schema_version_2400.objects T on (T.object_uid = U.table_uid and T.object_type = &#39;BT&#39; and T.object_name_space = &#39;TA&#39;)
   join definition_schema_version_2400.objects S on (S.schema_uid = T.schema_uid and S.object_type = &#39;SL&#39;)
   join definition_schema_version_2400.text Z on (Z.object_uid = K.object_uid)
where  
   K.object_type =  &#39;CC&#39;
   and (select count(*) from definition_schema_version_2400.ck_col_usage W where W.constraint_uid = U.constraint_uid) &gt; 1
[  and S.object_name = %.q:OWNER%]
[  and T.object_name = %.q:TABLE%]
order by 1, 2, 3, Z.sequence_num</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>1286526569</a:ModificationDate>
<a:Modifier>llane</a:Modifier>
<a:Comment>SQL query to reverse object check constraints</a:Comment>
</o:TextTargetItem>
