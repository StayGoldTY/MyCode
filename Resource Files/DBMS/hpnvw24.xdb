<a:ObjectID>2E6ED744-F735-47D0-9880-911C059AB449</a:ObjectID>
<a:Name>SqlAttrQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, TABLE, INDEX, UNIQUE, OPTIONS EX}

select 
   S.object_name,
   T.object_name,
   X.object_name,
   case(P.uniques) when &#39;Y&#39; then &#39;unique&#39; else &#39;&#39; end,
   case (P.valid_data) when &#39;N&#39; then &#39;no populate&#39; else &#39;&#39; end
   ||case(P.partitioning_scheme) when &#39;H2&#39; then 
      &#39; hash partition by (%SqlPartKeys.&#39; || cast(X.object_uid as varchar(30)) || &#39;%)&#39;
   else &#39;&#39; end
from
   definition_schema_version_2400.objects X
   join definition_schema_version_2400.access_paths P on (P.access_path_uid = X.object_uid)
   join definition_schema_version_2400.objects T on (T.object_uid = P.table_uid and T.object_type = &#39;BT&#39; and T.object_name_space = &#39;TA&#39;)
   join definition_schema_version_2400.objects S on (S.schema_uid = T.schema_uid and S.object_type = &#39;SL&#39;)
where X.object_type = &#39;IX&#39;
[  and S.object_name = %.q:OWNER%]
[  and T.object_name = %.q:TABLE%]
order by 1, 2, 3</a:TextTargetItem.Value>
