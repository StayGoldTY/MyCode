<o:TextTargetItem Id="o174">
<a:ObjectID>5DA7F5A2-72A3-4AEE-9295-9BAD2BDAF5E4</a:ObjectID>
<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{COWNER ID, CHILD ID, FKCONSTRAINT ID, POWNER ID, PARENT ID, PKCONSTRAINT ID, FKEYCOLUMNS ..., CKEYCOLUMNS ...}

select
   CS.object_name,
   CT.object_name,
   CK.object_name,
   PS.object_name,
   PT.object_name,
   PK.object_name,
   CC.column_name || &#39;,&#39;,
   PC.column_name || &#39;,&#39;
from 
   definition_schema_version_2400.ref_constraints F
   join definition_schema_version_2400.tbl_constraints R on (R.constraint_uid = F.constraint_uid and R.constraint_type = &#39;F&#39;)
   join definition_schema_version_2400.objects CK on (CK.object_uid = R.constraint_uid)
   join definition_schema_version_2400.objects CT on (CT.object_uid = R.table_uid and CT.object_type = &#39;BT&#39; and CT.object_name_space = &#39;TA&#39;)
   join definition_schema_version_2400.objects CS on (CS.schema_uid = CT.schema_uid and CS.object_type = &#39;SL&#39;)
   join definition_schema_version_2400.tbl_constraints K on (K.constraint_uid = F.unique_constraint_uid and K.constraint_type in (&#39;P&#39;, &#39;U&#39;))
   join definition_schema_version_2400.objects PK on (PK.object_uid = K.constraint_uid)
   join definition_schema_version_2400.objects PT on (PT.object_uid = K.table_uid and PT.object_type = &#39;BT&#39; and PT.object_name_space = &#39;TA&#39;)
   join definition_schema_version_2400.objects PS on (PS.schema_uid = PT.schema_uid and PS.object_type = &#39;SL&#39;)
   join definition_schema_version_2400.key_col_usage U on (U.constraint_uid = K.constraint_uid)
   join definition_schema_version_2400.cols PC on (PC.object_uid = PT.object_uid and PC.column_number = U.column_number)
   join definition_schema_version_2400.key_col_usage A on (A.constraint_uid = R.constraint_uid and A.ordinal_position = U.ordinal_position)
   join definition_schema_version_2400.cols CC on (CC.object_uid = CT.object_uid and CC.column_number = A.column_number)
where 1 = 1
[  and CS.object_name = %.q:OWNER%]
[  and CT.object_name = %.q:TABLE%]
order by 1, 2, 3, U.ordinal_position</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>1286526132</a:ModificationDate>
<a:Modifier>llane</a:Modifier>
<a:Comment>SQL query to list objects</a:Comment>
</o:TextTargetItem>
