<a:Name>Options</a:Name>
<a:TextTargetItem.Value>&lt;like-spec&gt; : composite=yes
{
   like %s : category=Model.Tables, composite=yes
   {
      with constraints
      with partitions
   }
}
&lt;table-spec&gt; : composite=yes
{
   max table size %d
   disk pool %d
   attribute %s : list=no insertlog|insertlog, default=no insertlog
}
&lt;partitioning&gt; : composite=yes
{
   no partition
   hash partition by : composite=yes, parenthesis=yes, separator=yes
   {
      &lt;partitioning-column&gt; %s : multiple=yes, category=Columns
   }
}
&lt;populate-with&gt; : composite=yes
{
   as %s
}</a:TextTargetItem.Value>
