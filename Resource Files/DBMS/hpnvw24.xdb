<a:CreationDate>1278084850</a:CreationDate>
<a:Creator>mledier</a:Creator>
<a:ModificationDate>1278084972</a:ModificationDate>
<a:Modifier>mledier</a:Modifier>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:HandlerScript>Function %GetEstimatedSize%(obj, ByRef message)

   Dim S, X, C, D, N, I
   Set X = obj.Model.DBMS.TargetObject
   if Not(X is Nothing) then
      &#39; Get datatype
      C = obj.ConceptualDataType
      for I = 1 to Len(C)
         if (InStr(&quot;0123456789&quot;, Mid(C, I, 1)) &gt; 0) then
            Exit For
         end if
         D = D &amp; Mid(C, I, 1)
      next
      N = Mid(C, I)
      &#39; Case of numeric
      if (D = &quot;N&quot;) then
         N = CInt(Split(N, &quot;,&quot;)(0))
         if (N &lt; 5) then
            %GetEstimatedSize% = 2
         elseif (N &lt; 10) then
            %GetEstimatedSize% = 4
         else
            %GetEstimatedSize% = 8
         end if
         Exit Function
      end if
      &#39; case of VA
      if (D = &quot;VA&quot;) then
         %GetEstimatedSize% = obj.AverageLength
         Exit Function
      end if
      
      if (N &lt;&gt; &quot;&quot;) then
         S = X.GetPhysicalDataTypeSize(D &amp; &quot;%n&quot;)
         if (S &lt;&gt; &quot;&quot;) then
            %GetEstimatedSize% = Eval(Replace(S, &quot;n&quot;, Split(N, &quot;,&quot;)(0)))
            Exit Function
         end if
      else
         S = X.GetPhysicalDataTypeSize(D)
         if (S &lt;&gt; &quot;&quot;) then
            %GetEstimatedSize% = CLng(S)
            Exit Function
         end if
      end if
   end if

   %GetEstimatedSize% = 10 &#39;internal default that does not exist for view column
End Function</a:HandlerScript>
</o:EventHandlerTargetItem>
