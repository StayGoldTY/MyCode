<o:MethodTargetItem Id="o232">
<a:ObjectID>D93BDFDE-3AD1-410F-A461-AC915864E1C2</a:ObjectID>
<a:Name>HashPartitionColumnsEdit</a:Name>
<a:CreationDate>1259928852</a:CreationDate>
<a:Creator>mledier</a:Creator>
<a:ModificationDate>1284631600</a:ModificationDate>
<a:Modifier>mledier</a:Modifier>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:MethodScript>Sub %Method%(obj, path)
   if (obj.PrimaryKey is Nothing) then
      MsgBox &quot;Table must have a primary key.&quot;, 0, CStr(obj)
      Exit Sub
   end if
   if (obj.PrimaryKey.Columns.Count = 0) then
      MsgBox &quot;There is no column in primary key&quot;, 0, CStr(obj)
      Exit Sub
   end if
   
   Dim ColList, Selected
   
   Set Selected = obj.Model.CreateSelection()
   if (Selected is Nothing) then
      MsgBox &quot;Unable to create a selection&quot;
      exit sub
   end if
   ColList = replace(obj.GetPhysicalOptionSql(path &amp; &quot;/&quot;), vbCrLf, &quot;&quot;)
   
   if (Trim(ColList) &lt;&gt; &quot;&quot;) then
      Dim pCol, sCol
      for each sCol in Split(ColList, &quot;,&quot;)
         Set pCol = obj.FindChildByCode(Trim(sCol), cls_Column, &quot;&quot;, Nothing, False)
         if Not(pCol is Nothing) then
            Selected.Objects.Add(pCol)
         end if
      next
   end if
   ColList = &quot;&quot;
   
   Selected.ShowObjMultiSelection obj.PrimaryKey, &quot;Columns&quot;
   for each pCol in Selected.objects
      ColList = ColList &amp; &quot;, &quot; &amp; pCol.Code
   Next
   if (ColList &lt;&gt; &quot;&quot;) then
      ColList = Mid(ColList, 3)
   end if
   obj.SetPhysicalOptionSql path , ColList

End Sub</a:MethodScript>
</o:MethodTargetItem>
</c:Categories>
</o:TypedCategoryTargetItem>
