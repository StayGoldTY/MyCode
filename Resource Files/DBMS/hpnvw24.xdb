<a:GetMethodScript>Function %Get%(obj)
   Dim mv, sget
   sget = &quot;&quot;
   For each mv in obj.GetCollectionByName(&quot;MaterializedViews&quot;)
      sget = sget &amp; &quot;,&quot; &amp; mv.Code
   Next
   %Get% = Mid(sget, 2)
End Function</a:GetMethodScript>
<a:SetMethodScript>Sub %Set%(obj, value)
   
   dim mv, s, coll
   Set coll = obj.GetCollectionByName(&quot;MaterializedViews&quot;)
   coll.Clear()
   if (coll is Nothing) then
      output &quot;Error: Unable to get MVGroup materialized views collection&quot;
      Exit Sub
   end if
   for each s in Split(value, &quot;,&quot;)
      Set mv = obj.Model.FindChildByCode(Trim(s), cls_View, &quot;MaterializedView&quot;, Nothing, False, False)
      if (mv is Nothing) then
         Set mv = ActiveModel.FindChildByCode(Trim(s), cls_View, &quot;MaterializedView&quot;, Nothing, False, False)
      end if
      if not(mv is Nothing) then
         coll.Add(mv.CreateShortcut(obj.Folder))
      end if
   next

End Sub</a:SetMethodScript>
