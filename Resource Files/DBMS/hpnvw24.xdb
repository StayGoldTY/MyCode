<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:MethodScript>Sub %Method%(obj)
   if (obj.Tables.Count = 0) then
      MsgBox &quot;There is no table to select in &quot; &amp; obj
      Exit Sub
   end if
   Dim TabList, Selected
   Set Selected = obj.Model.CreateSelection()
   if (Selected is Nothing) then
      MsgBox &quot;Unable to create a selection&quot;
      exit sub
   end if
   TabList = obj.GetExtendedAttribute(&quot;IgnoreChangesOn&quot;)
   
   if (Trim(TabList) &lt;&gt; &quot;&quot;) then
      Dim pTab, sTab
      for each sTab in Split(TabList, &quot;,&quot;)
         for each pTab in obj.Tables
            if (lCase(pTab.Code) = Trim(lCase(sTab))) then Exit For
         next
         if (IsObject(pTab)) then
            Selected.Objects.Add(pTab)
         end if
      next
   end if
   TabList = &quot;&quot;
   Selected.ShowObjMultiSelection obj, &quot;Tables&quot;
   for each pTab in Selected.objects
      TabList = TabList &amp; &quot;, &quot; &amp; pTab.Code
   Next
   if (TabList &lt;&gt; &quot;&quot;) then
      TabList = Mid(TabList, 3)
   end if

   obj.SetExtendedAttributeText &quot;IgnoreChangesOn&quot;, TabList

End Sub</a:MethodScript>
</o:MethodTargetItem>
