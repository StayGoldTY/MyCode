<o:TextTargetItem Id="o93">
<a:ObjectID>CEE13FF5-FA23-493E-974E-3277E9828572</a:ObjectID>
<a:Name>SqlOptsQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, TABLE, Set, OPTIONS EX}

select 
   S.object_name,
   T.object_name,
   case (D.insert_mode) when &#39;DD&#39; then &#39;true&#39; else &#39;false&#39; end,
   case when (D.max_table_size = 0) then &#39;&#39; else &#39;max table size &#39; || cast(D.max_table_size as varchar(30)) end
   || case when (D.diskpool = 0) then &#39;&#39; else &#39; disk pool &#39; || cast(D.diskpool as varchar(30)) end
   || case when (V.is_insertlog = &#39;Y&#39;) then &#39; attribute insertlog&#39; else &#39; attribute no insertlog&#39; end
   || case (D.partitioning_scheme)
      when &#39;N&#39; then &#39; no partition&#39;
      else &#39; hash partition by (%SqlPartKeys.&#39; || cast(T.object_uid as varchar(30)) || &#39;%)&#39;
      end
from
   definition_schema_version_2400.objects T
   join definition_schema_version_2400.objects S on (S.schema_uid = T.schema_uid and S.object_type = &#39;SL&#39;)
   join definition_schema_version_2400.access_paths D on (D.table_uid = T.object_uid and D.access_path_type = T.object_type)
   join definition_schema_version_2400.mvs_table_info V on (V.base_table_uid = T.object_uid)
where  T.object_type = &#39;BT&#39; and T.object_name_space = &#39;TA&#39;
[  and S.object_name = %.q:OWNER%]
[  and T.object_name = %.q:TABLE%]
order by 1, 2
</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>1286521621</a:ModificationDate>
<a:Modifier>llane</a:Modifier>
<a:Comment>SQL query to reverse object physical options</a:Comment>
</o:TextTargetItem>
<o:TextTargetItem Id="o94">
