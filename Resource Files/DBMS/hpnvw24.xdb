<a:CreationDate>1278073636</a:CreationDate>
<a:Creator>mledier</a:Creator>
<a:ModificationDate>1285080554</a:ModificationDate>
<a:Modifier>mledier</a:Modifier>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:HandlerScript>Function %GetEstimatedSize%(obj, ByRef message)
   Dim S, C, N, V

   Dim ColSizes
   Set ColSizes = CreateObject(&quot;Scripting.Dictionary&quot;)
   
   S = 4
   N = 0
   V = 0
   for each C in obj.Columns
      if Not(C.Mandatory) then N = N + 1
      if (InStr(C.ConceptualDataType, &quot;V&quot;) &gt; 0) then V = V + 1
      ColSizes.Add C, C.GetEstimatedSize(message)
      S = S + ColSizes.Item(C)
   next
   S = S + (N / 32 + 1) * 4
   S = S + V * 4
   
   S = S
   Dim X, XMsg, XS
   XMsg = &quot;&quot;
   for each X in obj.Indexes
      XS = 0
      For each C in X.IndexColumns
         XS = XS + ColSizes.Item(C.Column)
      next
      XMsg = XMsg &amp; CStr(XS * obj.Number) &amp; &quot;|&quot; &amp; X.ObjectID &amp; vbCrLf
      S = S + XS
   next
   
   S = S * obj.Number
   
   message = CStr(S) &amp; &quot;||&quot; &amp; obj.ShortDescription &amp; vbCrLf &amp; XMsg
   
   %GetEstimatedSize% = S
   
End Function</a:HandlerScript>
</o:EventHandlerTargetItem>
