<a:Modifier>mledier</a:Modifier>
<a:Comment>Contains part of statement for identity definition.</a:Comment>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:ExtendedAttributeTargetItem.DataType>12</a:ExtendedAttributeTargetItem.DataType>
<a:Computed>1</a:Computed>
<a:GetMethodScript>Function %Get%(obj)
   dim def, att
   def = &quot;&quot;
   att = obj.GetExtendedAttribute(&quot;StartWith&quot;)
   if (att &lt;&gt; &quot;&quot;) then
      def = def &amp; &quot; start with &quot; &amp; att
   end if
   att = obj.GetExtendedAttribute(&quot;Increment&quot;)
   if (att &lt;&gt; &quot;&quot;) then
      def = def &amp; &quot; increment by &quot; &amp; att
   end if
   att = obj.GetExtendedAttribute(&quot;MaxValue&quot;)
   if (att &lt;&gt; &quot;&quot;) then
      def = def &amp; &quot; maxvalue &quot; &amp; att
   end if
   att = obj.GetExtendedAttribute(&quot;MinValue&quot;)
   if (att &lt;&gt; &quot;&quot;) then
      def = def &amp; &quot; minvalue &quot; &amp; att
   end if
   att = obj.GetExtendedAttributeText(&quot;Cycle&quot;)
   if (LCase(att) = &quot;true&quot;) then
      def = def &amp; &quot; cycle&quot;
   end if
   def = trim(def)
   if (def &lt;&gt; &quot;&quot;) then
      %Get% = &quot;( &quot; &amp; def &amp; &quot; )&quot;
   else
      %Get% = &quot;&quot;
   end if
End Function</a:GetMethodScript>
<a:SetMethodScript>Sub %Set%(obj, value)
   Dim rxp, itm, mch
   Set rxp = new RegExp
   rxp.IgnoreCase = True
   rxp.Global = True
   itm = Replace(Replace(value, vbCr, &quot; &quot;), vbLf, &quot; &quot;)

   rxp.Pattern = &quot;(start\s+with|increment\s+by|maxvalue|no\s+maxvalue|minvalue|no\s+minvalue|no\s+cycle|cycle)\s+(\w+)?&quot;
   Dim M, S
   For each mch in rxp.Execute(itm)
      S = Replace(LCase(rxp.Replace(mch.Value, &quot;$1&quot;)), &quot; &quot;, &quot;&quot;)
      M = rxp.Replace(mch.Value, &quot;$2&quot;)
      select case (S)
         case &quot;startwith&quot;:
            obj.SetExtendedAttribute &quot;StartWith&quot;, M
         case &quot;incrementby&quot;:
            obj.SetExtendedAttribute &quot;Increment&quot;, M
         case &quot;maxvalue&quot;
            obj.SetExtendedAttribute &quot;MaxValue&quot;, M
         case &quot;nomaxvalue&quot;
            obj.SetExtendedAttribute &quot;MaxValue&quot;, &quot;&quot;
         case &quot;minvalue&quot;
            obj.SetExtendedAttribute &quot;MinValue&quot;, M
         case &quot;nominvalue&quot;
            obj.SetExtendedAttribute &quot;MaxValue&quot;, &quot;&quot;
         case &quot;nocycle&quot;
            obj.SetExtendedAttribute &quot;Cycle&quot;, False
         case &quot;cycle&quot;
            obj.SetExtendedAttribute &quot;Cycle&quot;, True
      end select
      
   Next



End Sub</a:SetMethodScript>
