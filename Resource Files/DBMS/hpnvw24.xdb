<a:MethodScript>Sub %Method%(obj)
   if (obj.Columns.Count = 0) then
      MsgBox &quot;There is no column to select in &quot; &amp; obj
      Exit Sub
   end if
   Dim ColList, Selected
   Set Selected = obj.Model.CreateSelection()
   if (Selected is Nothing) then
      MsgBox &quot;Unable to create a selection&quot;
      exit sub
   end if
   ColList = obj.GetExtendedAttribute(&quot;PartitionColumnList&quot;)
   
   if (Trim(ColList) &lt;&gt; &quot;&quot;) then
      Dim pCol, sCol
      for each sCol in Split(ColList, &quot;,&quot;)
         for each pCol in obj.Columns
            if (lCase(pCol.Code) = Trim(lCase(sCol))) then Exit For
         next
         if (IsObject(pCol)) then
            Selected.Objects.Add(pCol)
         end if
      next
   end if
   ColList = &quot;&quot;
   
   Selected.ShowObjMultiSelection obj, &quot;Columns&quot;
   for each pCol in Selected.objects
      ColList = ColList &amp; &quot;, &quot; &amp; pCol.Code
   Next
   if (ColList &lt;&gt; &quot;&quot;) then
      ColList = Mid(ColList, 3)
   end if
   obj.SetExtendedAttributeText &quot;PartitionColumnList&quot;, ColList

End Sub</a:MethodScript>
