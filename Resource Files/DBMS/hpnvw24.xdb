<a:Name>SqlAttrQuery</a:Name>
<a:TextTargetItem.Value>{OWNER ID, TRIGGER ID, TABLOWNER ID, TABLE ID, Text ...}

select 
   S.object_name,
   R.object_name,
   W.object_name,
   T.object_name,
   C.text
from
   definition_schema_version_2400.trigger_used G
   join definition_schema_version_2400.objects R on (R.object_uid = G.trigger_uid and R.object_type = &#39;TR&#39;)
   join definition_schema_version_2400.objects T on (T.object_uid = G.used_object_uid and T.object_type = &#39;BT&#39; and T.object_name_space = &#39;TA&#39;)
   join definition_schema_version_2400.objects S on (S.schema_uid = R.schema_uid and S.object_type = &#39;SL&#39;)
   join definition_schema_version_2400.objects W on (W.schema_uid = T.schema_uid and W.object_type = &#39;SL&#39;)
   join definition_schema_version_2400.text C on (C.object_uid = G.trigger_uid)
where G.is_subject_table = &#39;Y&#39;
[  and S.object_name = %.q:SCHEMA%]
[  and T.object_name = %.q:TABLE%]
[  and W.object_name = %.q:TABLOWNER%]
order by 3, 4, 1, 2, C.sequence_num
</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>1286526132</a:ModificationDate>
<a:Modifier>llane</a:Modifier>
<a:Comment>SQL query to reverse object attributes</a:Comment>
</o:TextTargetItem>
<o:TextTargetItem Id="o145">
<a:ObjectID>D179C0EC-834F-4E6B-8FDA-55D3AB9AC40E</a:ObjectID>
<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, TRIGGER, TABLOWNER, TABLE, TRGTIME, TRGEVENT}

SELECT 
   S.object_name,
   R.object_name,
   W.object_name,
   T.object_name,
   case(H.activation_time) when &#39;B&#39; then &#39;before&#39; when &#39;A&#39; then &#39;after&#39; else &#39;&#39; end,
   case (H.operation) when &#39;I&#39; then &#39;insert&#39; when &#39;D&#39; then &#39;delete&#39; when &#39;U&#39; then &#39;update&#39; else &#39;&#39; end
FROM
   definition_schema_version_2400.trigger_used G
   join definition_schema_version_2400.objects R on (R.object_uid = G.trigger_uid and R.object_type = &#39;TR&#39;)
   join definition_schema_version_2400.objects T on (T.object_uid = G.used_object_uid and T.object_type = &#39;BT&#39; and T.object_name_space = &#39;TA&#39;)
   join definition_schema_version_2400.triggers H on (H.trigger_uid = G.trigger_uid)
   join definition_schema_version_2400.objects S on (S.schema_uid = R.schema_uid and S.object_type = &#39;SL&#39;)
   join definition_schema_version_2400.objects W on (W.schema_uid = T.schema_uid and W.object_type = &#39;SL&#39;)
where G.is_subject_table = &#39;Y&#39;
[  and T.object_name = %.q:TABLE%]
[  and W.object_name = %.q:SCHEMA%]
order by 3, 4, 1, 2
</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>1286526132</a:ModificationDate>
<a:Modifier>llane</a:Modifier>
<a:Comment>SQL query to list objects</a:Comment>
</o:TextTargetItem>
