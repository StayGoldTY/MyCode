   join definition_schema_version_2400.cols C on (C.object_uid = T.object_uid and C.column_number = U.column_number)
where K.constraint_type in (&#39;P&#39;, &#39;U&#39;)
[  and S.object_name = %.q:OWNER%]
[  and T.object_name = %.q:TABLE%]
