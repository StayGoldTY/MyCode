   i.rdb$unique_flag,
   i.rdb$index_type
from
   rdb$relations t,
   rdb$indices i
where
   t.rdb$relation_name = i.rdb$relation_name
[  and t.rdb$relation_name = %.q:TABLE%]
[  and t.rdb$owner_name = %.q:OWNER%]
